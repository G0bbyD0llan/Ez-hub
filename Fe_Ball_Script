G2L["35"]["Text"] = [[_]];
G2L["35"]["Position"] = UDim2.new(0.8285714387893677, 0, 0, 0);

-- StarterGui.feball.menu.hide.LocalScript
G2L["36"] = Instance.new("LocalScript", G2L["35"]);


-- StarterGui.feball.feball
G2L["37"] = Instance.new("LocalScript", G2L["1"]);
G2L["37"]["Name"] = [[feball]];

-- StarterGui.feball.menu.All.ScrollingFrame2.PP.LocalScript
local function C_8()
local script = G2L["8"];
	script.Parent.MouseButton1Click:Connect(function()
	script.Parent.Parent.Visible = false
		script.Parent.Parent.Parent.ScrollingFrame.Visible = true
		script.Parent.Parent.Parent.ScrollingFrame3.Visible = false
	end)
end;
task.spawn(C_8);
