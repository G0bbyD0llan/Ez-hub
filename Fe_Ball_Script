-- StarterGui.feball.menu.hide.LocalScript
local function C_36()
local script = G2L["36"];
	script.Parent.MouseButton1Click:Connect(function()
	if script.Parent.Parent.All.Visible == true then
	script.Parent.BackgroundColor3 = Color3.new(0, 1, 0)
	script.Parent.Text = "+"
	script.Parent.Parent.All.Visible = false
	elseif script.Parent.Parent.All.Visible == false then
	script.Parent.BackgroundColor3 = Color3.new(0, 0, 1)
	script.Parent.Text = "-"
	script.Parent.Parent.All.Visible = true
	end
	end)
end;
task.spawn(C_36);
-- StarterGui.feball.feball
local function C_37()
local script = G2L["37"];
	--waits for load if is in studio
	
	if game["Run Service"]:IsStudio() then 
		--DISABLES GUI
	script.Parent.Enabled = false
	wait(2)
		--ENABLES GUI
		script.Parent.Enabled = true
	end
	
	--DRAG SCRIPT
	local UserInputService = game:GetService("UserInputService")
	
	local gui = script.Parent.menu
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
	
	-- Servicios
	local UserInputService = game:GetService("UserInputService")
	local RunService = game:GetService("RunService")
	local Camera = workspace.CurrentCamera
	local Players = game:GetService("Players")
	local buttons = script.Parent.menu.All.ScrollingFrame
	
	-- Constantes
	local SPEED_MULTIPLIER = 30
	local JUMP_POWER = 60
	local JUMP_GAP = 0.3
	
	-- Inicialización del jugador y del personaje
	local player = Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	
	-- Desactivar la colisión de todas las partes del personaje
	for _, v in ipairs(character:GetDescendants()) do
		if v:IsA("BasePart") then
			v.CanCollide = false
		end
	end
	
	-- Configuración de la bola (HumanoidRootPart)
	local ball = character.HumanoidRootPart
	ball.Shape = Enum.PartType.Ball
	ball.Transparency = 0.5
	ball.Size = Vector3.new(5, 5, 5)
	local TWEEN_DURATION = 1
	local TweenService = game:GetService("TweenService")
	local function tweenColor(targetColor)
		local tweenInfo = TweenInfo.new(TWEEN_DURATION, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut)
		local colorTween = TweenService:Create(ball, tweenInfo, {Color = targetColor})
		colorTween:Play()
	end
	local humanoid = character:WaitForChild("Humanoid")
	local moveLeft = false
	local moveRight = false
	local moveUp = false
	local moveDown = false
	
	-- Parámetros del Raycast
	local params = RaycastParams.new()
	params.FilterType = Enum.RaycastFilterType.Blacklist
	params.FilterDescendantsInstances = {character}
	
	-- Función para manejar el movimiento de la bola
	
	local function onRenderStepped(delta)
		ball.CanCollide = true
		humanoid.PlatformStand = true
	
		-- Evitar movimientos si el jugador está escribiendo en una caja de texto
		if UserInputService:GetFocusedTextBox() then return end
	
		-- Movimiento de la bola basado en la entrada del teclado
		if UserInputService:IsKeyDown(Enum.KeyCode.W) or moveUp then
			ball.RotVelocity -= Camera.CFrame.RightVector * delta * SPEED_MULTIPLIER
		end
		if UserInputService:IsKeyDown(Enum.KeyCode.A) or moveLeft then
			ball.RotVelocity -= Camera.CFrame.LookVector * delta * SPEED_MULTIPLIER
		end
		if UserInputService:IsKeyDown(Enum.KeyCode.S) or moveDown then
			ball.RotVelocity += Camera.CFrame.RightVector * delta * SPEED_MULTIPLIER
		end
		if UserInputService:IsKeyDown(Enum.KeyCode.D) or moveRight then
			ball.RotVelocity += Camera.CFrame.LookVector * delta * SPEED_MULTIPLIER
		end
	end
	-- Conexión del evento RenderStepped
	local renderSteppedConnection = RunService.RenderStepped:Connect(onRenderStepped)
	
	-- Función para manejar los saltos
	local function onJumpRequest()
		local result = workspace:Raycast(
			ball.Position,
			Vector3.new(0, -((ball.Size.Y / 2) + JUMP_GAP), 0),
			params
		)
	
		if result then
			ball.Velocity += Vector3.new(0, JUMP_POWER, 0)
		end
	end
	
	-- Conexión del evento de salto
	xd = UserInputService.JumpRequest:Connect(onJumpRequest)
	
	-- Cambiar el sujeto de la cámara a la bola
	Camera.CameraSubject = ball
	
	-- Creación del ScreenGui y Frame para los controles
	
	local function onResetButtonClick()
	spawn(function()
			local trail = character.PrimaryPart:FindFirstChild("Trail")
			local ballLight = character.PrimaryPart:FindFirstChild("BALLLIGHT")
			local particleEmitter = character.PrimaryPart:FindFirstChild("ParticleEmitter")
			if trail then
				trail:Remove()
			end
			if ballLight then
				ballLight:Remove()
			end
			if particleEmitter then
				particleEmitter:Remove()
			end
	end)
		ball.Shape = Enum.PartType.Block
		ball.Size = Vector3.new(2, 2, 1)
		ball.Transparency = 1
		xd:Disconnect()
		Camera.CameraSubject = humanoid
		renderSteppedConnection:Disconnect()
		humanoid.PlatformStand = false
		for _, v in ipairs(character:GetDescendants()) do
			if v:IsA("BasePart") then
				v.CanCollide = true
			end
		end
		script.Parent:Destroy()
	end
	
	local CloseScriptButtonOne = script.Parent.menu.close
	CloseScriptButtonOne.MouseButton1Click:Connect(function()
	onResetButtonClick()
	end)
	
	-- Botones en el Frame
	local stopButton = buttons.EB
	local activeSB = false
	stopButton.MouseButton1Click:Connect(function()
		if activeSB then  return end
		activeSB = true
		local orgcol = ball.Color
		wait()
		stopButton.Text = "¡¡EMERGENCY BRAKE!!"
		stopButton.BackgroundColor3 = Color3.new(1, 0, 0)
		tweenColor(Color3.new(1, 0, 0))
		task.spawn(function()
		repeat
				ball.RotVelocity = Vector3.new(0, 0, 0)
				wait()
		until activeSB == false
		end)
		wait(2)
		tweenColor(orgcol)
		wait(1)
		stopButton.Text = "emergency brake"
		stopButton.BackgroundColor3 = Color3.new(0, 0.701961, 1)
		activeSB = false
	end)
	
	local resetButton = buttons.RS
	resetButton.MouseButton1Click:Connect(onResetButtonClick)
	
	local transparencyButton = buttons.ST
	transparencyButton.MouseButton1Click:Connect(function()
		if ball.Transparency == 0.5 then
			ball.Transparency = 1
			transparencyButton.Text = "Set Transparency"
		else
			ball.Transparency = 0.5
			transparencyButton.Text = "reset Transparency"
		end
	end)
	
	local SpeedButton = buttons.AS
	SpeedButton.MouseButton1Click:Connect(function()
		SPEED_MULTIPLIER = SPEED_MULTIPLIER + 10  -- Incrementa la velocidad en 10
	end)
	local UnSpeedButton = buttons.DS
	UnSpeedButton.MouseButton1Click:Connect(function()
		SPEED_MULTIPLIER = SPEED_MULTIPLIER - 10  -- Incrementa la velocidad en 10
	end)
	
	local CustomizableBox = buttons.Parent.ScrollingFrame2.CSP --customizable speed box
	
	-- Filtra letras y otros caracteres no numéricos mientras se escribe
	local ScrollingFrame3 = script.Parent.menu.All.ScrollingFrame3
	local left = ScrollingFrame3.MCl
	local right = ScrollingFrame3.MCr
	local up = ScrollingFrame3.MCu
	local down = ScrollingFrame3.MCd
	
	-- Conexión de eventos a los botones para simular la presión de teclas
	
	-- Variables para controlar el estado del botón
	local isLeftPressed = false
	local isRightPressed = false
	local isUpPressed = false
	local isDownPressed = false
	
	-- Función para actualizar el estado del movimiento
	local function updateMovementState(button, state)
		if button == left then
			moveLeft = state
		elseif button == right then
			moveRight = state
		elseif button == up then
			moveUp = state
		elseif button == down then
			moveDown = state
		end
	end
	
	-- Función para manejar eventos de botón
	local function setupButton(button, moveStateVar)
		button.MouseButton1Down:Connect(function()
			moveStateVar = true
			updateMovementState(button, true)
		end)
		button.MouseButton1Up:Connect(function()
			moveStateVar = false
			updateMovementState(button, false)
		end)
		button.MouseEnter:Connect(function()
			if moveStateVar then
				updateMovementState(button, true)
			end
		end)
		button.MouseLeave:Connect(function()
			updateMovementState(button, false)
		end)
	end
	
	-- Configuración de los botones
	setupButton(left, isLeftPressed)
	setupButton(right, isRightPressed)
	setupButton(up, isUpPressed)
	setupButton(down, isDownPressed)
	local allowbypasses = false -- permite velocidades mayores a 1000 o inferiores a -1000
	local showSpeedWarnings = true -- muestra advertencias de velocidad
	local originalPlaceholder = CustomizableBox.PlaceholderText
	
	CustomizableBox.FocusLost:Connect(function(enterPressed)
		if enterPressed then
			local cmdToPass = CustomizableBox.Text
			CustomizableBox.Text = ""
	
			-- Función para mostrar avisos temporales
			local function showTemporaryMessage(message, duration)
				CustomizableBox.PlaceholderText = message
				task.wait(duration)
				CustomizableBox.PlaceholderText = originalPlaceholder
			end
	
			local character = player.Character or player.CharacterAdded:Wait()
			local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
			if not humanoidRootPart then return end
	
			local trail = humanoidRootPart:FindFirstChild("Trail")
			local ballLight = humanoidRootPart:FindFirstChild("BALLLIGHT")
			local particleEmitter = humanoidRootPart:FindFirstChild("ParticleEmitter")
	
			-- Comando para ajustar la velocidad
			if cmdToPass:sub(1, 6) == "speed " then
				local speedToPass = tonumber(cmdToPass:sub(7))
				if speedToPass then
					if allowbypasses or (speedToPass >= -1000 and speedToPass <= 1000) then
						SPEED_MULTIPLIER = speedToPass
						showTemporaryMessage("Speed set to " .. speedToPass, 2)
					else
						showTemporaryMessage("Speed out of bounds", 2)
					end
				end
	
				-- Comando para ajustar la potencia del salto
			elseif cmdToPass:sub(1, 10) == "jumpspeed " then
				local jumpSpeedToPass = tonumber(cmdToPass:sub(11))
				if jumpSpeedToPass then
					JUMP_POWER = jumpSpeedToPass
					showTemporaryMessage("Jump speed set to " .. jumpSpeedToPass, 2)
				end
	
				-- Comando para permitir velocidades extremas
			elseif cmdToPass == "allowbypasses" then
				allowbypasses = not allowbypasses
				if allowbypasses then
					showTemporaryMessage("Bypasses allowed", 2)
				else
					showTemporaryMessage("Bypasses not allowed", 2)
				end
	
				-- Comando para alternar las advertencias de velocidad
			elseif cmdToPass == "togglespeedwarning" then
				showSpeedWarnings = not showSpeedWarnings
				if showSpeedWarnings then
					showTemporaryMessage("Speed warnings on", 2)
				else
					showTemporaryMessage("Speed warnings off", 2)
				end
	
				-- Comando para mostrar la lista de comandos
			elseif cmdToPass == "help" then
				showTemporaryMessage("Commands: speed <value>, jumpseed <value>, togglespeedwarning, allowbypasses, settraillifetime <value>, settrailwidth <value>, setlightbrightness <value>, setlightrange <value>, setparticlelifetime <value>, setparticlesize <value>", 10)
			elseif cmdToPass:sub(1, 17) == "settraillifetime " then
				local lifetime = tonumber(cmdToPass:sub(18))
				if trail and lifetime then
					trail.Lifetime = lifetime
					showTemporaryMessage("Trail lifetime set to " .. lifetime, 2)
				else
					showTemporaryMessage("Invalid lifetime value or Trail not found", 2)
				end
	
			elseif cmdToPass:sub(1, 14) == "settrailwidth " then
				local width = tonumber(cmdToPass:sub(15))
				if trail and width then
					trail.WidthScale = NumberSequence.new(width)
					showTemporaryMessage("Trail width set to " .. width, 2)
				else
					showTemporaryMessage("Invalid width value or Trail not found", 2)
				end
	
				-- Comandos para PointLight
			elseif cmdToPass:sub(1, 19) == "setlightbrightness " then
				local brightness = tonumber(cmdToPass:sub(20))
				if ballLight and brightness then
					ballLight.Brightness = brightness
					showTemporaryMessage("Light brightness set to " .. brightness, 2)
				else
					showTemporaryMessage("Invalid brightness value or Light not found", 2)
				end
	
			elseif cmdToPass:sub(1, 14) == "setlightrange " then
				local range = tonumber(cmdToPass:sub(15))
				if ballLight and range then
					ballLight.Range = range
					showTemporaryMessage("Light range set to " .. range, 2)
				else
					showTemporaryMessage("Invalid range value or Light not found", 2)
				end
	
				-- Comandos para ParticleEmitter
			elseif cmdToPass:sub(1, 20) == "setparticlelifetime " then
				local lifetime = tonumber(cmdToPass:sub(21))
				if particleEmitter and lifetime then
					particleEmitter.Lifetime = NumberRange.new(lifetime)
					showTemporaryMessage("Particle lifetime set to " .. lifetime, 2)
				else
					showTemporaryMessage("Invalid lifetime value or ParticleEmitter not found", 2)
				end
	
			elseif cmdToPass:sub(1, 16) == "setparticlesize " then
				local size = tonumber(cmdToPass:sub(17))
				if particleEmitter and size then
					particleEmitter.Size = NumberSequence.new(size)
					showTemporaryMessage("Particle size set to " .. size, 2)
				else
					showTemporaryMessage("Invalid size value or ParticleEmitter not found", 2)
				end
	
			else
				showTemporaryMessage("Unknown command, type 'help' for list of commands", 2)
			end
		end
	end)
	local originalballcoL = ball.Color
	-- Lista de colores (puedes agregar más colores si lo deseas)
	local colors = {
		Color3.fromRGB(255, 0, 0),    -- Rojo
		Color3.fromRGB(0, 255, 0),    -- Verde
		Color3.fromRGB(0, 0, 255),    -- Azul
		Color3.fromRGB(255, 255, 0),  -- Amarillo
		Color3.fromRGB(255, 165, 0),  -- Naranja
		Color3.fromRGB(0, 0, 0),      -- Negro
	    originalballcoL               -- Color original	
	}
	
	local currentIndex = 1  -- Índice inicial en la lista de colores
	
	local ChangeCButton = buttons.BCC
	ChangeCButton.MouseButton1Click:Connect(function()
		if ball.Transparency == 0.5 then
			-- Cambiar el color del ball
			tweenColor(colors[currentIndex])
			ChangeCButton.BackgroundColor3 = colors[currentIndex]
	
			-- Incrementar el índice o volver al inicio si llegamos al final de la lista
			currentIndex = currentIndex + 1
			if currentIndex > #colors then
				currentIndex = 1
			end
		end
	end)
	local ABJ = buttons.ABJ
	ABJ.MouseButton1Click:Connect(function()
	JUMP_POWER = JUMP_POWER + 10
	end)
	local DBJ = buttons.DBJ
	DBJ.MouseButton1Click:Connect(function()
		JUMP_POWER = JUMP_POWER - 10
	end)
	local debugbutton = buttons.DEBUGMODE
	local activeDBM = false
	debugbutton.MouseButton1Click:Connect(function()
	if buttons.debuginfo.Visible == true then
	buttons.debuginfo.Visible = false
	debugbutton.Text = "show debug"
	activeDBM = false
	elseif buttons.debuginfo.Visible == false then
	debugbutton.Text = "hide debug"
	buttons.debuginfo.Visible = true
	activeDBM = true
	end
	end)
	local AP = buttons.Parent.ScrollingFrame2.AP -- botón para agregar y eliminar partículas
	
	local particles = nil
	local particleEffectEnabled = false
	
	AP.MouseButton1Click:Connect(function()
		if not particleEffectEnabled then
			-- Crear y agregar el efecto de partículas a la bola
			particles = Instance.new("ParticleEmitter")
			particles.Size = NumberSequence.new(0.5)
			particles.Acceleration = Vector3.new(0, -10, 0)
			particles.Lifetime = NumberRange.new(1)
			particles.Rate = 100
			particles.RotSpeed = NumberRange.new(100)
			particles.Speed = NumberRange.new(5)
			particles.Parent = ball
	
			AP.Text = "Remove Particles"
			particleEffectEnabled = true
		else
			-- Eliminar el efecto de partículas de la bola
			if particles then
				particles:Destroy()
			end
	
			AP.Text = "Add Particles"
			particleEffectEnabled = false
		end
	end)
	task.spawn(function()
	while wait() do
	if particles then
				particles.Color = ColorSequence.new(ball.Color)
	end
	end
	end)
	local AddTrailButton = buttons.Parent.ScrollingFrame2.ATB -- Botón para agregar estela
	
	local trail = nil
	local trailEnabled = false
	
	AddTrailButton.MouseButton1Click:Connect(function()
		if not trailEnabled then
			-- Crear y configurar los attachments en la bola
			local attachment0 = Instance.new("Attachment", ball)
			attachment0.Position = Vector3.new(-0.5, 0, 0)
			local attachment1 = Instance.new("Attachment", ball)
			attachment1.Position = Vector3.new(0.5, 0, 0)
	
			-- Crear y configurar la estela
			trail = Instance.new("Trail")
			trail.Attachment0 = attachment0
			trail.Attachment1 = attachment1
			trail.Texture = "rbxassetid://14166981368" -- ID de la textura de huellas de patas
			trail.TextureMode = Enum.TextureMode.Static
			trail.TextureLength = 2
			trail.Lifetime = 1
			trail.Color = ColorSequence.new(Color3.fromRGB(0, 162, 255))
			trail.Transparency = NumberSequence.new(0, 1)
			trail.WidthScale = NumberSequence.new({
				NumberSequenceKeypoint.new(0, 5),  -- Incrementa el valor aquí para aumentar el ancho inicial de la estela
				NumberSequenceKeypoint.new(1, 5)   -- Incrementa el valor aquí para aumentar el ancho final de la estela
			})
			trail.Parent = ball
	
			AddTrailButton.Text = "Remove Trail"
			trailEnabled = true
		else
			-- Eliminar la estela
			if trail then
				trail:Destroy()
			end
	
			AddTrailButton.Text = "Add Trail"
			trailEnabled = false
		end
	end)
	task.spawn(function()
		while wait() do
			if trail then
				trail.Color = ColorSequence.new(ball.Color)
			end
		end
	end)
	
	local enlargeballsize = buttons.Parent.ScrollingFrame2.ELB
	local reduceballsize = buttons.Parent.ScrollingFrame2.RB
	
	local function enlargeBallSize()
		ball.Size = ball.Size * 1.2  -- Aumenta el tamaño de la bola en un 20%
	end
	
	local function reduceBallSize()
		ball.Size = ball.Size / 1.2  -- Reduce el tamaño de la bola en un 20%
	end
	
	enlargeballsize.MouseButton1Click:Connect(enlargeBallSize)
	reduceballsize.MouseButton1Click:Connect(reduceBallSize)
	
	local addRB = buttons.Parent.ScrollingFrame2.ARE
	
	local isReflectanceEnabled = false
	
	local function toggleReflectance()
		isReflectanceEnabled = not isReflectanceEnabled
		if isReflectanceEnabled then addRB.Text = "UnAdd refleteance effect" else addRB.Text = "Add refleteance effect" end
		ball.Reflectance = isReflectanceEnabled and 0.55 or 0  -- 1 para habilitar reflectancia, 0 para deshabilitarla
	end
	
	addRB.MouseButton1Click:Connect(toggleReflectance)
	local APB = buttons.Parent.ScrollingFrame2.APB
	
	local isPointLightEnabled = false
	local pointLight = Instance.new("PointLight")
	pointLight.Parent = ball
	pointLight.Color = Color3.fromRGB(255, 255, 255)
	pointLight.Range = 15
	pointLight.Brightness = 5
	pointLight.Name = "BALLLIGHT"
	pointLight.Enabled = false
	task.spawn(function()
	while wait(0.1) do
	pointLight.Color = ball.Color
	end
	end)
	local function togglePointLight()
		isPointLightEnabled = not isPointLightEnabled
		pointLight.Enabled = isPointLightEnabled
		APB.Text = isPointLightEnabled and "Remove PointLight" or "Add PointLight"
	end
	
	APB.MouseButton1Click:Connect(togglePointLight)
	
	local DisableControls = false
	local DC = buttons.Parent.ScrollingFrame2.DC -- Referencia al botón "disable button"
	
	DC.MouseButton1Click:Connect(function()
		DisableControls = not DisableControls  -- Alternar el estado de DisableControls
	
		if DisableControls then
			-- Desactivar controles
			renderSteppedConnection:Disconnect()  -- Desconectar la función onRenderStepped
			xd:Disconnect()  -- Desconectar la función de salto (onJumpRequest)
	
			-- Restaurar configuraciones de colisión de partes del personaje
			for _, v in ipairs(character:GetDescendants()) do
				if v:IsA("BasePart") then
					v.CanCollide = true
				end
			end
	
			-- Actualizar texto del botón
			DC.Text = "Enable Controls"
			DC.BackgroundColor3 = Color3.fromRGB(255, 0, 0)  -- Color rojo para indicar desactivado
		else
			-- Activar controles nuevamente
			renderSteppedConnection = RunService.RenderStepped:Connect(onRenderStepped)  -- Volver a conectar onRenderStepped
			xd = UserInputService.JumpRequest:Connect(onJumpRequest)  -- Volver a conectar onJumpRequest
	
			-- Desactivar colisión de partes del personaje
			for _, v in ipairs(character:GetDescendants()) do
				if v:IsA("BasePart") then
					v.CanCollide = false
				end
			end
	
			-- Actualizar texto del botón
			DC.Text = "Disable Controls"
			DC.BackgroundColor3 = Color3.fromRGB(0, 255, 0)  -- Color verde para indicar activado
		end
	end)
	
	-----
	local warningLabel = buttons.warning
	-- Task para detectar velocidad alta o negativa
	task.spawn(function()
		game:GetService("RunService").RenderStepped:Connect(function()
			if showSpeedWarnings then
				warningLabel.Visible=true
				if SPEED_MULTIPLIER > 150 then
					warningLabel.Text = "¡¡EXTREMELY SUPER-HIGH SPEED DETECTED!!, REDUCE YOUR SPEED RIGHT NOW. SPEED:" .. SPEED_MULTIPLIER
					warningLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
				elseif SPEED_MULTIPLIER > 100 then
					warningLabel.Text = "¡¡DANGEROUSLY HIGH SPEED DETECTED!!, Reduce your speed immediately. speed:" .. SPEED_MULTIPLIER
					warningLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
				elseif SPEED_MULTIPLIER > 70 then
					warningLabel.Text = "¡¡HIGH SPEED DETECTED!!, speed:" .. SPEED_MULTIPLIER
					warningLabel.TextColor3 = Color3.fromRGB(255, 69, 0)
				elseif SPEED_MULTIPLIER < -150 then
					warningLabel.Text = "¡¡DANGEROUSLY NEGATIVE SPEED DETECTED!!, REDUCE YOUR SPEED RIGHT NOW. SPEED:" .. SPEED_MULTIPLIER
					warningLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
				elseif SPEED_MULTIPLIER < -100 then
					warningLabel.Text = "¡¡EXTREMELY NEGATIVE SPEED DETECTED!!, Adjust your settings immediately. speed:" .. SPEED_MULTIPLIER
					warningLabel.TextColor3 = Color3.fromRGB(128, 0, 128)
				elseif SPEED_MULTIPLIER < -70 then
					warningLabel.Text = "¡¡VERY NEGATIVE SPEED DETECTED!!, speed:" .. SPEED_MULTIPLIER
					warningLabel.TextColor3 = Color3.fromRGB(75, 0, 130)
				elseif SPEED_MULTIPLIER < 0 then
					warningLabel.Text = "¡¡NEGATIVE SPEED DETECTED!!, speed:" .. SPEED_MULTIPLIER
					warningLabel.TextColor3 = Color3.fromRGB(0, 0, 255)
				else
					warningLabel.Text = "warning: Avoid setting negative speeds or excessively high speeds, speed:" .. SPEED_MULTIPLIER
					warningLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
				end
			else
				warningLabel.Text = ""
				warningLabel.Visible=false
			end
		end)
	end)
	local CJP = buttons.CJP
	task.spawn(function()
		game:GetService("RunService").RenderStepped:Connect(function()
			if JUMP_POWER < 0 then JUMP_POWER = 0 end
	CJP.Text = "Current JumpPower:"..JUMP_POWER
		end)
	end)
	local debuginfo = buttons.debuginfo
	task.spawn(function()
		game:GetService("RunService").RenderStepped:Connect(function()
			local ballPosition = string.format("%.2f, %.2f, %.2f", ball.Position.X, ball.Position.Y, ball.Position.Z)
			local ballVelocity = string.format("%.2f, %.2f, %.2f", ball.Velocity.X, ball.Velocity.Y, ball.Velocity.Z)
			debuginfo.Text = "Debug Information:\n"
				.. "Speed Multiplier: " .. SPEED_MULTIPLIER .. "\n"
				.. "Jump Power: " .. JUMP_POWER .. "\n"
				.. "Ball Position: " .. ballPosition .. "\n"
				.. "Ball Velocity: " .. ballVelocity
		end)
	end)
	
	
	-- Manejar la muerte del personaje
	humanoid.Died:Connect(function()
	
		local hint = Instance.new("Hint",workspace)
		hint.Text = "FE BALL SCRIPT DISABLED DUE TO DIE /// NEED TO RE-EXECUTE"
		game:GetService("Debris"):AddItem(hint, 3) 
		renderSteppedConnection:Disconnect()
		script.Parent:Destroy()
	end)
	
end;
task.spawn(C_37);

return G2L["1"], require;
