-- StarterGui.ScreenGui.WindowFrame.ContentArea.misc.MyButton.LocalScript
local function C_62()
	local script = G2L["62"];
	local button = script.Parent -- Botn que sirve para activar/desactivar el toggle
	local originalText = button.Text -- Guardamos el texto original del botn
	local isToggled = false -- Estado del toggle (desactivado por defecto)

	-- Funcin para alternar el estado del toggle
	local function toggleKickTeleport()
		isToggled = not isToggled
		if isToggled then
			button.Text = "Anti kick: ON" -- Cambiamos el texto para reflejar que est activado
		else
			button.Text = "Anti kick: OFF" -- Cambiamos el texto para reflejar que est desactivado
		end
		-- La funcin que verifica si el jugador es "kickeado"
		while isToggled do
			wait(1)
			if game.Players.LocalPlayer.PlayerGui.MainGui.Prompts.Ban.KickPlayer.Text == game.Players.LocalPlayer.Name then
				local player = game.Players.LocalPlayer
				local placeId = game.PlaceId
				local serverId = game.JobId
				game:GetService("TeleportService"):TeleportToPlaceInstance(placeId, serverId, player)
			end
		end

	end

	-- Conectamos la accin al hacer clic en el botn
	button.MouseButton1Click:Connect(toggleKickTeleport)
end;
task.spawn(C_62);
