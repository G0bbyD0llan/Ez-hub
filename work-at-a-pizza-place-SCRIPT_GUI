		end)

		wait(2)

		createTween(button, {TextTransparency = 1}, textChangeDuration).Completed:Connect(function()
			button.Text = OLDTEXT
			createTween(button, {TextTransparency = 0}, textChangeDuration)
		end)
		wait(1)
		activeTween = false

	end)

end;
task.spawn(C_52);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.misc.MyButton.LocalScript
local function C_54()
	local script = G2L["54"];
	local button = script.Parent
	local frame = script.Parent.Parent
	local OLDTEXT = "Infinite yield"
	local scriptname = OLDTEXT

	-- Aadir efectos de hover y clic
	local TweenService = game:GetService("TweenService")

	-- Definir colores y tiempos de transicin
	local hoverColor = Color3.fromRGB(70, 70, 70)
	local clickColor = Color3.fromRGB(30, 30, 30)
	local defaultColor = Color3.fromRGB(3, 0, 50)
	local textChangeDuration = 0.5

	-- Funcin para crear transiciones
	local function createTween(instance, properties, duration)
		local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
		local tween = TweenService:Create(instance, tweenInfo, properties)
		tween:Play()
		return tween
	end
	local activeTween = false
	-- Eventos de hover
	button.MouseEnter:Connect(function()
		createTween(button, {BackgroundColor3 = hoverColor}, 0.2)
	end)

	button.MouseLeave:Connect(function()
		createTween(button, {BackgroundColor3 = defaultColor}, 0.2)
	end)

	button.MouseButton1Down:Connect(function()
		createTween(button, {BackgroundColor3 = clickColor}, 0.1)
	end)

	button.MouseButton1Up:Connect(function()
		createTween(button, {BackgroundColor3 = hoverColor}, 0.1)
	end)

	-- Evento de clic para ejecutar el script
	button.MouseButton1Click:Connect(function()
		if activeTween then return end
		local runningText = "Running "..scriptname
		activeTween = true
		-- Cambiar texto con transicin
		createTween(button, {TextTransparency = 1}, textChangeDuration).Completed:Connect(function()
			button.Text = runningText
			createTween(button, {TextTransparency = 0}, textChangeDuration)
		end)

		task.spawn(function()
			loadstring(game:HttpGet("https://raw.githubusercontent.com/CMD-X/CMD-X/master/Source", true))()
		end)

		wait(2)

		createTween(button, {TextTransparency = 1}, textChangeDuration).Completed:Connect(function()
			button.Text = OLDTEXT
			createTween(button, {TextTransparency = 0}, textChangeDuration)
		end)
		wait(1)
		activeTween = false

	end)

end;
task.spawn(C_54);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.misc.MyButton.LocalScript
local function C_57()
	local script = G2L["57"];
	local button = script.Parent
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local player = game.Players.LocalPlayer

	local toggleJobChange = false -- Estado inicial del toggle

	local function changeJobs()
		while toggleJobChange do
			pcall(function()
				local jobList = {"Cashier", "Cook", "Delivery", "Supplier", "On Break"}

				for _, job in ipairs(jobList) do
					ReplicatedStorage.PlayerChannel:FireServer("ChangeJob", job)
					task.wait(0.02)
				end

				-- Eliminar ropa del personaje
				if player.Character then
					local pants = player.Character:FindFirstChild("Pants")
					local shirt = player.Character:FindFirstChild("Shirt")

					if pants then pants:Destroy() end
					if shirt then shirt:Destroy() end
				end
			end)
			task.wait()
		end
	end

	-- Funcin para alternar el estado del toggle
	button.MouseButton1Click:Connect(function()
		toggleJobChange = not toggleJobChange -- Alternar entre true y false
		button.Text = toggleJobChange and "STOP" or "Break Leaderboard"

		if toggleJobChange then
			changeJobs()
		end
	end)

end;
task.spawn(C_57);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.misc.MyButton.LocalScript
local function C_59()
	local script = G2L["59"];
	local teleportPlaces = {
		["Starting Zone"] = CFrame.new(49, 3, 198),
		["Cashier Area"] = CFrame.new(48, 4, 80),
		["Cook Area"] = CFrame.new(42, 4, 61),
		["Boxer Area"] = CFrame.new(55, 4, 31),
		["Supplier Area"] = CFrame.new(8, 13, -1020),
		["Manager Room"] = CFrame.new(23, 5, 6.5)
	}

	local scrollingFrame = script.Parent:FindFirstChild("ScrollingFrame") -- ScrollingFrame dentro del botn
	local templateButton = scrollingFrame:FindFirstChild("Template_Button"):Clone() -- El template del botn

	local function createButton(name, position)
		-- Clonamos el Template_Button
		local newButton = templateButton:Clone()
		newButton.Name = name
		newButton.Text = name
		newButton.Parent = scrollingFrame

		-- Ajustamos el tamao y la posicin de los botones
		newButton.Position = UDim2.new(0, 0, 0, (newButton.Size.Y.Offset + 10) * #scrollingFrame:GetChildren()) -- Espaciado entre botones

		-- Definimos la accin cuando el jugador hace clic en el botn
		newButton.MouseButton1Click:Connect(function()
			game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(position) -- Teletransportamos al jugador
		end)
	end


	-- Borramos los botones existentes antes de crear nuevos
	for _, child in pairs(scrollingFrame:GetChildren()) do
		if child:IsA("TextButton") and child.Name ~= "Template_Button" then
			child:Destroy()
		end
	end

	-- Crear los botones de teleportacin en el ScrollingFrame
	for name, position in pairs(teleportPlaces) do
		createButton(name, position)
	end

	script.Parent.MouseButton1Click:Connect(function()
		for _, v in pairs(scrollingFrame:GetDescendants()) do
			if v:IsA("TextButton") and v.Text:lower() == "button" then v:Remove() end
		end
		-- Si el ScrollingFrame no est visible, lo mostramos
		scrollingFrame.Visible = not scrollingFrame.Visible
	end)
end;
task.spawn(C_59);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.misc.MyButton.LocalScript
local function C_5e()
	local script = G2L["5e"];
	local button = script.Parent -- Botn que sirve para activar/desactivar el toggle
	local originalText = button.Text -- Guardamos el texto original del botn
	local isToggled = false -- Estado del toggle (desactivado por defecto)

	-- Funcin para alternar el estado del toggle
	local function toggleAntiOvenFire()
		isToggled = not isToggled

		if isToggled then
			button.Text = "Anti-Oven Fire: ON" -- Cambiamos el texto para reflejar que est activado
		else
			button.Text = "Anti-Oven Fire: OFF" -- Cambiamos el texto para reflejar que est desactivado
		end

		while isToggled do
			wait(0.40) -- Controlamos la frecuencia de ejecucin
			pcall(function()
				-- Iteramos sobre los jugadores
				for _, player in pairs(game.Players:GetPlayers()) do
					local character = player.Character
					if character and character:FindFirstChild("UpperTorso") then
						local g = {[1] = character.UpperTorso}
						workspace.GameService.ExtinguishFire:FireServer(unpack(g))
					end
				end

				-- Iteramos sobre los objetos Dough en Workspace
				for _, dough in pairs(game.Workspace.AllDough:GetChildren()) do
					local g = {[1] = dough}
					workspace.GameService.ExtinguishFire:FireServer(unpack(g))
				end
			end)
		end

	end

	-- Conectamos la accin al hacer clic en el botn
	button.MouseButton1Click:Connect(toggleAntiOvenFire)
end;
task.spawn(C_5e);
