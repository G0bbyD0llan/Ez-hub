-- StarterGui.ScreenGui.WindowFrame.ContentArea.configuration.confFrame.ConfigurationFRAME.AllowC.LocalScript
local function C_ab()
	local script = G2L["ab"];
	script.Parent.MouseButton1Click:Connect(function()
		everyClipboard = setclipboard or toclipboard or set_clipboard or (Clipboard and Clipboard.set)
		local function notify(arg1,arg2)
			--notify the user by using SetCore from StarterGUi
			game:GetService("StarterGui"):SetCore("SendNotification",{
				Title = arg1,
				Text = arg2,
				Duration = 5
			})
		end

		local function toClipboard(txt)
			if everyClipboard then
				everyClipboard(tostring(txt))
				notify("Clipboard", "Copied to clipboard")
			else
				notify("Clipboard", "Your exploit doesn't have the ability to use the clipboard")
			end
		end

		toClipboard("https://www.youtube.com/@G0bbyD0llan57-ee1vl/featured?sub_confirmation=1")
	end)
end;
task.spawn(C_ab);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.configuration.confFrame.ConfigurationFRAME.ReSiZeTool.LocalScript
local function C_ad()
	local script = G2L["ad"];
	local option_page = script.Parent.Parent
	local IsReSiZeToolActive = false
	option_page.ReSiZeTool.MouseButton1Click:Connect(function()
		IsReSiZeToolActive = not IsReSiZeToolActive
		if IsReSiZeToolActive then
			option_page.ReSiZeTool.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
			option_page.ReSiZeTool.Text = "yes"
			if script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.WindowFrame:FindFirstChild("ResizeGrip") then
				script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.WindowFrame.ResizeGrip.Visible = true
			else
				task.spawn(function()
					local UserInputService = game:GetService("UserInputService")

					local screenGui = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent
					local mainWindow = screenGui:WaitForChild("WindowFrame")

					-- Original MIN_WIDTH and MIN_HEIGHT in pixels
					local MIN_WIDTH_PIXELS = 150
					local MIN_HEIGHT_PIXELS = 100

					-- Initialize UIScale and determine designAbsoluteSize (size at Scale = 1)
					local windowScaler = mainWindow:FindFirstChild("MainWindowScaler")
					local designAbsoluteSize

					if windowScaler and windowScaler:IsA("UIScale") then
						if windowScaler.Scale ~= 0 then
							designAbsoluteSize = mainWindow.AbsoluteSize / windowScaler.Scale
						else
							warn("ResizeMainWindow: MainWindowScaler had a scale of 0. Resetting to 1.")
							windowScaler.Scale = 1
							designAbsoluteSize = mainWindow.AbsoluteSize -- mainWindow.AbsoluteSize is now size at scale 1
						end
					else
						windowScaler = Instance.new("UIScale")
						windowScaler.Name = "MainWindowScaler"
						windowScaler.Parent = mainWindow
						-- UIScale defaults to Scale = 1, so AbsoluteSize is already the size at scale 1
						designAbsoluteSize = mainWindow.AbsoluteSize
					end

					-- Calculate minimum allowed scale based on pixel minimums and design size
					local ACTUAL_MIN_SCALE
					if designAbsoluteSize.X > 0 and designAbsoluteSize.Y > 0 then
						local minScaleX = MIN_WIDTH_PIXELS / designAbsoluteSize.X
						local minScaleY = MIN_HEIGHT_PIXELS / designAbsoluteSize.Y
						ACTUAL_MIN_SCALE = math.max(minScaleX, minScaleY)
					else
						-- Fallback if designAbsoluteSize is zero (should not happen for a visible window)
						warn("ResizeMainWindow: designAbsoluteSize is zero. Using a default min scale.")
						ACTUAL_MIN_SCALE = 0.1 
					end
					-- Ensure ACTUAL_MIN_SCALE is at least a very small positive number
					ACTUAL_MIN_SCALE = math.max(ACTUAL_MIN_SCALE, 0.05) -- Absolute smallest scale

					-- Create Resize Grip (if it doesn't exist, or ensure it's configured)
					local resizeGrip = mainWindow:FindFirstChild("ResizeGrip")
					if not resizeGrip or not resizeGrip:IsA("TextButton") then
						if resizeGrip then resizeGrip:Destroy() end -- Remove if wrong type
						resizeGrip = Instance.new("TextButton")
						resizeGrip.Name = "ResizeGrip"
						resizeGrip.Text = "" 
						resizeGrip.Size = UDim2.new(0, 20, 0, 20) 
						resizeGrip.AnchorPoint = Vector2.new(1, 1) 
						resizeGrip.Position = UDim2.new(1, 0, 1, 0) 
						resizeGrip.BackgroundColor3 = Color3.fromRGB(120, 120, 120)
						resizeGrip.BorderColor3 = Color3.fromRGB(80, 80, 80)
						resizeGrip.BorderSizePixel = 1
						resizeGrip.AutoButtonColor = false
						resizeGrip.ZIndex = (mainWindow.ZIndex or 1) + 10 -- Ensure it's on top
						resizeGrip.Parent = mainWindow
					end


					local isResizing = false
					local initialMousePosition = nil
					local initialScalerValue = nil

					resizeGrip.InputBegan:Connect(function(input)
						if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
							if designAbsoluteSize.X == 0 or designAbsoluteSize.Y == 0 then
								warn("ResizeMainWindow: Cannot start resize, designAbsoluteSize is zero.")
								-- Re-evaluate designAbsoluteSize in case it became valid
								if windowScaler.Scale ~= 0 then
									designAbsoluteSize = mainWindow.AbsoluteSize / windowScaler.Scale
								else
									windowScaler.Scale = 1
									designAbsoluteSize = mainWindow.AbsoluteSize
								end
								if designAbsoluteSize.X == 0 or designAbsoluteSize.Y == 0 then
									return -- Still zero, cannot proceed
								end
							end

							isResizing = true
							initialMousePosition = UserInputService:GetMouseLocation()
							initialScalerValue = windowScaler.Scale
						end
					end)

					UserInputService.InputEnded:Connect(function(input)
						if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
							if isResizing then
								isResizing = false
								initialMousePosition = nil
								initialScalerValue = nil
							end
						end
					end)

					UserInputService.InputChanged:Connect(function(input)
						if not isResizing or not initialMousePosition or not initialScalerValue then
							return
						end

						if designAbsoluteSize.X == 0 or designAbsoluteSize.Y == 0 then
							-- This check is important if the window was somehow initialized to zero size
							-- and InputBegan couldn't get a valid designAbsoluteSize.
							warn("ResizeMainWindow: Cannot resize, designAbsoluteSize is zero during InputChanged.")
							isResizing = false -- Stop resizing attempt
							return
						end

						if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
							local currentMousePosition = UserInputService:GetMouseLocation()
							local delta = currentMousePosition - initialMousePosition

							-- Calculate the absolute size the window had at the start of the drag
							local effectiveWidthAtDragStart = designAbsoluteSize.X * initialScalerValue
							local effectiveHeightAtDragStart = designAbsoluteSize.Y * initialScalerValue

							-- Calculate the new target absolute size based on mouse delta
							local targetAbsoluteWidth = effectiveWidthAtDragStart + delta.X
							local targetAbsoluteHeight = effectiveHeightAtDragStart + delta.Y

							-- Calculate the new scale required to achieve these target absolute sizes,
							-- based on the designAbsoluteSize (size at scale = 1)
							local newScaleX = targetAbsoluteWidth / designAbsoluteSize.X
							-- local newScaleY = targetAbsoluteHeight / designAbsoluteSize.Y -- Kept for reference if aspect ratio logic is desired

							-- For UIScale, we use a single scale value. Let's use the X-axis driven scale.
							-- More complex logic could be used here to maintain aspect ratio or average scales.
							local finalNewScale = newScaleX

							-- Apply minimum scale
							finalNewScale = math.max(ACTUAL_MIN_SCALE, finalNewScale)

							windowScaler.Scale = finalNewScale
						end
					end)
				end)
			end
		else
			script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.WindowFrame:FindFirstChild("ResizeGrip").Visible = false
			option_page.ReSiZeTool.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
			option_page.ReSiZeTool.Text = "no"
		end
	end)

end;
task.spawn(C_ad);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.Frame.ShowCreditsButton.CreditsHandler
local function C_b4()
	local script = G2L["b4"];
	local Tween = {
		Transparency = function(Object, Target)
			if typeof(Object) == "Instance" and typeof(Target) == "number" then
				if Object:IsA("GuiObject") then
					local TweenService = game:GetService("TweenService")
					local time = 2
					local tween = TweenService:Create(Object, TweenInfo.new(time), {Transparency = Target})
					tween:Play()
				end
			end
		end,
		TextTransparency = function(Object, Target)
			if typeof(Object) == "Instance" and typeof(Target) == "number" then
				if Object:IsA("GuiObject") then
					local TweenService = game:GetService("TweenService")
					local time = 2
					local tween = TweenService:Create(Object, TweenInfo.new(time), {TextTransparency = Target})
					tween:Play()
				end
			end
		end,
		BackgroundTransparency = function(Object, Target)
			if typeof(Object) == "Instance" and typeof(Target) == "number" then
				if Object:IsA("GuiObject") then
					local TweenService = game:GetService("TweenService")
					local time = 2
					local tween = TweenService:Create(Object, TweenInfo.new(time), {BackgroundTransparency = Target})
					tween:Play()
				end
			end
		end,
	}

	local IsAnimating = false
	local CreditsFrame = script.Parent.Parent.CreditsFrame

	local function Animate(way)
		if typeof(way) == "string" then
			if way:lower() == "start" then
				IsAnimating = true
				CreditsFrame.Visible = true
				script.Parent.Parent.HideCreditsButton.Active = false
				script.Parent.Parent.ShowCreditsButton.Active = false
				script.Parent.Parent.HideCreditsButton.Visible = true
				Tween.BackgroundTransparency(CreditsFrame, 0.2)
				Tween.TextTransparency(CreditsFrame.CreditsText, 0)
				Tween.BackgroundTransparency(CreditsFrame.Title, 0.2)
				Tween.TextTransparency(CreditsFrame.Title, 0)
				Tween.Transparency(script.Parent.Parent.HideCreditsButton, 0)
				Tween.Transparency(script.Parent.Parent.ShowCreditsButton, 1)
				wait(2)
				script.Parent.Parent.HideCreditsButton.Active = true
				script.Parent.Parent.ShowCreditsButton.Visible = false
				IsAnimating = false
			elseif way:lower() == "end" then
				IsAnimating = true
				script.Parent.Parent.HideCreditsButton.Active = false
				script.Parent.Parent.ShowCreditsButton.Active = false
				script.Parent.Parent.ShowCreditsButton.Visible = true
				Tween.BackgroundTransparency(CreditsFrame, 1)
				Tween.TextTransparency(CreditsFrame.CreditsText, 1)
				Tween.BackgroundTransparency(CreditsFrame.Title, 1)
				Tween.TextTransparency(CreditsFrame.Title, 1)
				Tween.Transparency(script.Parent.Parent.HideCreditsButton, 1)
				Tween.Transparency(script.Parent.Parent.ShowCreditsButton, 0)
				wait(2)
				script.Parent.Parent.HideCreditsButton.Active = false
				script.Parent.Parent.HideCreditsButton.Visible = false
				script.Parent.Parent.ShowCreditsButton.Visible = true
				CreditsFrame.Visible = false
				IsAnimating = false
			end
		end
	end

	script.Parent.MouseButton1Click:Connect(function()
		if not IsAnimating then
			Animate("start")
		end
	end)

	script.Parent.Parent.HideCreditsButton.MouseButton1Click:Connect(function()
		if not IsAnimating then
			Animate("end")
		end
	end)

end;
task.spawn(C_b4);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.Frame.SettingsButton.LocalScript
local function C_bd()
	local script = G2L["bd"];
	local imageButton = script.Parent
	imageButton.MouseEnter:Connect(function()
		imageButton.ImageColor3 = Color3.fromRGB(200, 200, 200) -- Cambiar color al pasar el mouse
	end)

	imageButton.MouseLeave:Connect(function()
		imageButton.ImageColor3 = Color3.fromRGB(255, 255, 255) -- Restaurar color predeterminado al retirar el mouse
	end)
	imageButton.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Frame.Visible = false
		script.Parent.Parent.Parent.configuration.Visible = true
	end)
end;
task.spawn(C_bd);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.Frame.Message.LocalScript
local function C_c0()
	local script = G2L["c0"];
	repeat wait() until script.Parent.Parent.Parent.Parent.Transparency == 1

	local TextLabel = script.Parent -- Asegúrate de colocar este script dentro del TextLabel
	local pastebinUrl = "https://pastebin.com/raw/RtUmQWvg" -- Reemplaza con tu enlace raw de Pastebin

	local version, lastMessage = "Unknown", "No Message"

	local success, result = pcall(function()
		return game:HttpGet(pastebinUrl)
	end)

	if success then
		local lines = result:split("\n")
		version = lines[1] or "Unknown"
		lastMessage = lines[2] or "No Message"
	end

	local function updateText()
		while true do
			local currentTime = os.date("%X")
			TextLabel.Text = string.format("Time = %s\nVersion = %s\nCreator Last Message: %s", currentTime, version, lastMessage)
			wait(1) -- Espera 1 segundo antes de la siguiente actualización
		end
	end

	local function FadeIn()
		wait(3.3)
		script.Parent.TextTransparency = 0
		wait(0.3)
		script.Parent.TextTransparency = 1
		wait(0.3)
		script.Parent.TextTransparency = 0
		wait(0.3)
		script.Parent.TextTransparency = 1
		wait(0.3)
		script.Parent.TextTransparency = 0
	end

	spawn(updateText)
	spawn(FadeIn)
end;
task.spawn(C_c0);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.Car.ActualCarBox.LocalScript
local function C_c4()
	local script = G2L["c4"];
	-- 📦 Car/Truck Handler mejorado con soporte para respawn
	local textBox = script.Parent

	-- Función para actualizar el texto
	local function setLabel(name: string)
		if typeof(name) == "string" then
			textBox.Text = name
		end
	end

	-- ObjectValue para guardar el auto actual
	local CarObjectValue = Instance.new("ObjectValue")
	CarObjectValue.Name = "CarObjectValue"
	CarObjectValue.Parent = textBox

	-- Jugador local
	local LocalPlayer = game:GetService("Players").LocalPlayer

	-- 🔄 Función para actualizar el auto actual
	local function updateCurrentCar(humanoid: Humanoid?)
		if not humanoid or humanoid.Health <= 0 then
			CarObjectValue.Value = nil
			setLabel("")
			return
		end

		local seat = humanoid.SeatPart
		if seat and (seat:IsA("VehicleSeat") or seat:IsA("Seat")) then
			local car = seat.Parent
			CarObjectValue.Value = car
			setLabel("Actual Car - " .. car.Name)
		else
			CarObjectValue.Value = nil
			setLabel("")
		end
	end

	-- 🏎️ Función para encontrar y sentarse en vehículo
	local function trySitInVehicle(humanoid: Humanoid?, folder: Folder, character: Model)
		if not humanoid or humanoid.Health <= 0 then return end
		local root = character:FindFirstChild("HumanoidRootPart")
		local oldPos = root and root.CFrame

		-- Si ya está en un vehículo de esa carpeta, no hace nada
		if humanoid.SeatPart and folder:IsAncestorOf(humanoid.SeatPart) then
			return
		end

		humanoid.Sit = false
		task.wait(0.1)

		-- Buscar un vehículo libre
		for _, vehicle in ipairs(folder:GetChildren()) do
			if vehicle:FindFirstChild("Driver") and vehicle.Driver.Occupant == nil then
				if vehicle:FindFirstChild("Owner") and vehicle.Owner.Value ~= nil then
					continue
				end
				vehicle.Driver:Sit(humanoid)
				task.wait(0.3)
				if humanoid.SeatPart then
					break
				end
			end
		end

		-- Teleportar el vehículo a la posición original del player
		if humanoid.SeatPart and oldPos then
			local car = humanoid.SeatPart.Parent
			if car.PrimaryPart then
				car:SetPrimaryPartCFrame(oldPos)
			else
				-- fallback: usar Driver como PrimaryPart temporal
				local driver = car:FindFirstChild("Driver")
				if driver then
					car.PrimaryPart = driver
					car:SetPrimaryPartCFrame(oldPos)
				end
			end
		end
	end

	-- 📌 Conectar todo al humanoide válido
	local function connectHumanoid(humanoid: Humanoid, character: Model)
		-- actualizar al inicio
		updateCurrentCar(humanoid)

		-- escuchar cambios de SeatPart
		humanoid:GetPropertyChangedSignal("SeatPart"):Connect(function()
			updateCurrentCar(humanoid)
		end)

		-- manejar comandos en TextBox
		textBox.FocusLost:Connect(function()
			local command = textBox.Text:lower()

			if command == "car" or command == "vehicle" then
				trySitInVehicle(humanoid, workspace.Cars, character)
			elseif command == "truck" or command == "bigcar" then
				trySitInVehicle(humanoid, workspace.Trucks, character)
			end

			updateCurrentCar(humanoid)
		end)
	end

	-- 🔄 Reconectar en cada respawn
	local function onCharacterAdded(character: Model)
		local humanoid: Humanoid = character:WaitForChild("Humanoid")
		connectHumanoid(humanoid, character)
	end

	LocalPlayer.CharacterAdded:Connect(onCharacterAdded)
	if LocalPlayer.Character then
		onCharacterAdded(LocalPlayer.Character)
	end

end;
task.spawn(C_c4);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.Car.Frame.ActionButtonMANAGER.LocalScript
local function C_c7()
	local script = G2L["c7"];

	local player = game.Players.LocalPlayer

	local function GetCurrentCar()
		if not player.Character or not player.Character:FindFirstChild("Humanoid") then
			return nil
		end

		local ActualCarBox = script.Parent.Parent.Parent.ActualCarBox
		local CarObjectValue: ObjectValue = ActualCarBox:FindFirstChild("CarObjectValue")
		if not CarObjectValue or not CarObjectValue.Value then
			return nil
		end

		return CarObjectValue.Value
	end

	local FLYING = false
	local QEfly = true
	local iyflyspeed = 1
	local vehicleflyspeed = 1

	local flyKeyUp  = nil
	local flyKeyDown = nil
	local function getRoot(character: Model)
		return character:FindFirstChild("HumanoidRootPart") or character:FindFirstChild("Torso")
	end

	local Players = game:GetService("Players")
	local UserInputService = game:GetService("UserInputService")
	local RunService = game:GetService("RunService")
	local randomString = function()
		return math.random() * math.random()
	end
	local camera = game.Workspace.CurrentCamera
	function sFLY(vfly)
		local plr = Players.LocalPlayer
		local char = plr.Character or plr.CharacterAdded:Wait()
		local humanoid = char:FindFirstChildOfClass("Humanoid")
		if not humanoid then
			repeat task.wait() until char:FindFirstChildOfClass("Humanoid")
			humanoid = char:FindFirstChildOfClass("Humanoid")
		end

		if flyKeyDown or flyKeyUp then
			flyKeyDown:Disconnect()
			flyKeyUp:Disconnect()
		end

		local T = getRoot(char)
		local CONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
		local lCONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
		local SPEED = 0

		local function FLY()
			FLYING = true
			local BG = Instance.new('BodyGyro')
			local BV = Instance.new('BodyVelocity')
			BG.P = 9e4
			BG.Parent = T
			BV.Parent = T
			BG.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
			BG.CFrame = T.CFrame
			BV.Velocity = Vector3.new(0, 0, 0)
			BV.MaxForce = Vector3.new(9e9, 9e9, 9e9)
			task.spawn(function()
				repeat task.wait()
					local camera = workspace.CurrentCamera
					if not vfly and humanoid then
						humanoid.PlatformStand = true
					end

					if CONTROL.L + CONTROL.R ~= 0 or CONTROL.F + CONTROL.B ~= 0 or CONTROL.Q + CONTROL.E ~= 0 then
						SPEED = 50
					elseif not (CONTROL.L + CONTROL.R ~= 0 or CONTROL.F + CONTROL.B ~= 0 or CONTROL.Q + CONTROL.E ~= 0) and SPEED ~= 0 then
						SPEED = 0
					end
					if (CONTROL.L + CONTROL.R) ~= 0 or (CONTROL.F + CONTROL.B) ~= 0 or (CONTROL.Q + CONTROL.E) ~= 0 then
						BV.Velocity = ((camera.CFrame.LookVector * (CONTROL.F + CONTROL.B)) + ((camera.CFrame * CFrame.new(CONTROL.L + CONTROL.R, (CONTROL.F + CONTROL.B + CONTROL.Q + CONTROL.E) * 0.2, 0).p) - camera.CFrame.p)) * SPEED
						lCONTROL = {F = CONTROL.F, B = CONTROL.B, L = CONTROL.L, R = CONTROL.R}
					elseif (CONTROL.L + CONTROL.R) == 0 and (CONTROL.F + CONTROL.B) == 0 and (CONTROL.Q + CONTROL.E) == 0 and SPEED ~= 0 then
						BV.Velocity = ((camera.CFrame.LookVector * (lCONTROL.F + lCONTROL.B)) + ((camera.CFrame * CFrame.new(lCONTROL.L + lCONTROL.R, (lCONTROL.F + lCONTROL.B + CONTROL.Q + CONTROL.E) * 0.2, 0).p) - camera.CFrame.p)) * SPEED
					else
						BV.Velocity = Vector3.new(0, 0, 0)
					end
					BG.CFrame = camera.CFrame
				until not FLYING
				CONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
				lCONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
				SPEED = 0
				BG:Destroy()
				BV:Destroy()

				if humanoid then humanoid.PlatformStand = false end
			end)
		end

		flyKeyDown = UserInputService.InputBegan:Connect(function(input, processed)
			if input.KeyCode == Enum.KeyCode.W then
				CONTROL.F = (vfly and vehicleflyspeed or iyflyspeed)
			elseif input.KeyCode == Enum.KeyCode.S then
				CONTROL.B = - (vfly and vehicleflyspeed or iyflyspeed)
			elseif input.KeyCode == Enum.KeyCode.A then
				CONTROL.L = - (vfly and vehicleflyspeed or iyflyspeed)
			elseif input.KeyCode == Enum.KeyCode.D then
				CONTROL.R = (vfly and vehicleflyspeed or iyflyspeed)
			elseif input.KeyCode == Enum.KeyCode.E and QEfly then
				CONTROL.Q = (vfly and vehicleflyspeed or iyflyspeed)*2
			elseif input.KeyCode == Enum.KeyCode.Q and QEfly then
				CONTROL.E = -(vfly and vehicleflyspeed or iyflyspeed)*2
			end
			pcall(function() camera.CameraType = Enum.CameraType.Track end)
		end)

		flyKeyUp = UserInputService.InputEnded:Connect(function(input, processed)
			if input.KeyCode == Enum.KeyCode.W then
				CONTROL.F = 0
			elseif input.KeyCode == Enum.KeyCode.S then
				CONTROL.B = 0
			elseif input.KeyCode == Enum.KeyCode.A then
				CONTROL.L = 0
			elseif input.KeyCode == Enum.KeyCode.D then
				CONTROL.R = 0
			elseif input.KeyCode == Enum.KeyCode.E then
				CONTROL.Q = 0
			elseif input.KeyCode == Enum.KeyCode.Q then
				CONTROL.E = 0
			end
		end)
		FLY()
	end

	function NOFLY()
		FLYING = false
		if flyKeyDown or flyKeyUp then flyKeyDown:Disconnect() flyKeyUp:Disconnect() end
		if Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid') then
			Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid').PlatformStand = false
		end
		pcall(function() workspace.CurrentCamera.CameraType = Enum.CameraType.Custom end)
	end

	local velocityHandlerName = randomString()
	local gyroHandlerName = randomString()
	local mfly1
	local mfly2

	local unmobilefly = function(speaker)
		pcall(function()
			FLYING = false
			local root = getRoot(speaker.Character)
			root:FindFirstChild(velocityHandlerName):Destroy()
			root:FindFirstChild(gyroHandlerName):Destroy()
			speaker.Character:FindFirstChildWhichIsA("Humanoid").PlatformStand = false
			mfly1:Disconnect()
			mfly2:Disconnect()
		end)
	end

	local mobilefly = function(speaker, vfly)
		unmobilefly(speaker)
		FLYING = true

		local root = getRoot(speaker.Character)
		local camera = workspace.CurrentCamera
		local v3none = Vector3.new()
		local v3zero = Vector3.new(0, 0, 0)
		local v3inf = Vector3.new(9e9, 9e9, 9e9)

		local controlModule = require(speaker.PlayerScripts:WaitForChild("PlayerModule"):WaitForChild("ControlModule"))
		local bv = Instance.new("BodyVelocity")
		bv.Name = velocityHandlerName
		bv.Parent = root
		bv.MaxForce = v3zero
		bv.Velocity = v3zero

		local bg = Instance.new("BodyGyro")
		bg.Name = gyroHandlerName
		bg.Parent = root
		bg.MaxTorque = v3inf
		bg.P = 1000
		bg.D = 50

		mfly1 = speaker.CharacterAdded:Connect(function()
			local bv = Instance.new("BodyVelocity")
			bv.Name = velocityHandlerName
			bv.Parent = root
			bv.MaxForce = v3zero
			bv.Velocity = v3zero

			local bg = Instance.new("BodyGyro")
			bg.Name = gyroHandlerName
			bg.Parent = root
			bg.MaxTorque = v3inf
			bg.P = 1000
			bg.D = 50
		end)

		mfly2 = RunService.RenderStepped:Connect(function()
			root = getRoot(speaker.Character)
			camera = workspace.CurrentCamera
			if speaker.Character:FindFirstChildWhichIsA("Humanoid") and root and root:FindFirstChild(velocityHandlerName) and root:FindFirstChild(gyroHandlerName) then
				local humanoid = speaker.Character:FindFirstChildWhichIsA("Humanoid")
				local VelocityHandler = root:FindFirstChild(velocityHandlerName)
				local GyroHandler = root:FindFirstChild(gyroHandlerName)

				VelocityHandler.MaxForce = v3inf
				GyroHandler.MaxTorque = v3inf
				if not vfly then humanoid.PlatformStand = true end
				GyroHandler.CFrame = camera.CoordinateFrame
				VelocityHandler.Velocity = v3none

				local direction = controlModule:GetMoveVector()
				if direction.X > 0 then
					VelocityHandler.Velocity = VelocityHandler.Velocity + camera.CFrame.RightVector * (direction.X * ((vfly and vehicleflyspeed or iyflyspeed) * 50))
				end
				if direction.X < 0 then
					VelocityHandler.Velocity = VelocityHandler.Velocity + camera.CFrame.RightVector * (direction.X * ((vfly and vehicleflyspeed or iyflyspeed) * 50))
				end
				if direction.Z > 0 then
					VelocityHandler.Velocity = VelocityHandler.Velocity - camera.CFrame.LookVector * (direction.Z * ((vfly and vehicleflyspeed or iyflyspeed) * 50))
				end
				if direction.Z < 0 then
					VelocityHandler.Velocity = VelocityHandler.Velocity - camera.CFrame.LookVector * (direction.Z * ((vfly and vehicleflyspeed or iyflyspeed) * 50))
				end
			end
		end)
	end

	local VehicleFlyActive = false

	local IsOnMobile = table.find({Enum.Platform.Android, Enum.Platform.IOS}, UserInputService:GetPlatform())

	local function ToggleVehicleFly(ForcePCMode: boolean)
		VehicleFlyActive = not VehicleFlyActive
		if VehicleFlyActive then
			if not GetCurrentCar() then
				NOFLY()
				return
			end
			if not IsOnMobile then
				NOFLY()
				wait()
				sFLY(true)
			else
				if ForcePCMode then
					NOFLY()
					wait()
					sFLY(true)
				else
					mobilefly(player, true)
				end
			end
			local ActualCarBox = script.Parent.Parent.Parent.ActualCarBox
			local CarObjectValue: ObjectValue = ActualCarBox:FindFirstChild("CarObjectValue")

			CarObjectValue.Changed:Once(function()
				if CarObjectValue.Value == nil then
					if not IsOnMobile then NOFLY() else if ForcePCMode then NOFLY() else unmobilefly(player) end end
					script.Parent.Text = "Vehicle Fly: OFF"
				end
			end)
			script.Parent.Text = "Vehicle Fly: ON"
		else
			if not IsOnMobile then NOFLY() else if ForcePCMode then NOFLY() else unmobilefly(player) end end
			script.Parent.Text = "Vehicle Fly: OFF"
		end
	end


	script.Parent.MouseButton1Click:Connect(function()
		ToggleVehicleFly(false)
	end)

	script.Parent.MouseButton2Click:Connect(function()
		ToggleVehicleFly(true)
	end)
end;
task.spawn(C_c7);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.Car.Frame.ActionButtonBRING.LocalScript
local function C_c9()
	local script = G2L["c9"];
	local player = game.Players.LocalPlayer

	local function GetCurrentCar()
		if not player.Character or not player.Character:FindFirstChild("Humanoid") then
			return nil
		end

		local ActualCarBox = script.Parent.Parent.Parent.ActualCarBox
		local CarObjectValue: ObjectValue = ActualCarBox:FindFirstChild("CarObjectValue")
		if not CarObjectValue or not CarObjectValue.Value then
			return nil
		end

		return CarObjectValue.Value
	end
	local Noclipping = nil
	local Clip = true
	local vnoclipParts = {}
	local speaker = game:GetService("Players").LocalPlayer
	local RunService = game:GetService("RunService")

	local function EnableNoclip()
		Clip = false
		wait(0.1)
		local function NoclipLoop()
			if Clip == false and speaker.Character ~= nil then
				for _, child in pairs(speaker.Character:GetDescendants()) do
					if child:IsA("BasePart") and child.CanCollide == true then
						child.CanCollide = false
					end
				end
			end
		end
		Noclipping = RunService.Stepped:Connect(NoclipLoop)
	end

	local function DisableNoclip()
		if Noclipping then
			Noclipping:Disconnect()
		end
		Clip = true
	end
	script.Parent.MouseButton1Click:Connect(function()
		local car = GetCurrentCar()
		if not car then
			return
		end
		vnoclipParts = {}
		local seat = speaker.Character:FindFirstChildOfClass('Humanoid').SeatPart
		local vehicleModel = seat.Parent
		repeat
			if vehicleModel.ClassName ~= "Model" then
				vehicleModel = vehicleModel.Parent
			end
		until vehicleModel.ClassName == "Model"
		wait(0.1)
		EnableNoclip()
		for i,v in pairs(vehicleModel:GetDescendants()) do
			if v:IsA("BasePart") and v.CanCollide then
				table.insert(vnoclipParts,v)
				v.CanCollide = false
			end
		end

		local ActualCarBox = script.Parent.Parent.Parent.ActualCarBox
		local CarObjectValue: ObjectValue = ActualCarBox:FindFirstChild("CarObjectValue")
		CarObjectValue.Changed:Once(function()
			DisableNoclip()
			for i, v in pairs(vnoclipParts) do
				v.CanCollide = true
			end
			vnoclipParts = {}
		end)
	end)
end;
task.spawn(C_c9);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.Car.Frame.ActionButtonFLING.LocalScript
local function C_cb()
	local script = G2L["cb"];
	local teleportPlaces = {
		["Starting Zone"] = CFrame.new(49, 3, 198),
		["Cashier Area"] = CFrame.new(48, 4, 80),
		["Cook Area"] = CFrame.new(42, 4, 61),
		["Boxer Area"] = CFrame.new(55, 4, 31),
		["Supplier Area"] = CFrame.new(8, 13, -1020),
		["Manager Room"] = CFrame.new(23, 5, 6.5)
	}

	local isActive = false -- Toggle para encender/apagar el loop
	local player = game.Players.LocalPlayer
	local button = script.Parent 

	-- 📌 Función para obtener el coche actual
	local function GetCurrentCar()
		if not player.Character or not player.Character:FindFirstChild("Humanoid") then
			return nil
		end

		local ActualCarBox = button.Parent.Parent:FindFirstChild("ActualCarBox")
		if not ActualCarBox then return nil end

		local CarObjectValue: ObjectValue = ActualCarBox:FindFirstChild("CarObjectValue")
		if not CarObjectValue or not CarObjectValue.Value then
			return nil
		end

		return CarObjectValue.Value
	end

	-- 📌 Smooth Teleport con spin y disable collisions
	local function SmoothTPToCFrame(Car: Model, targetCFrame: CFrame, duration: number)
		duration = duration or 1
		local startTime = tick()

		local startCFrame = Car:GetPivot()
		local parts = Car:GetDescendants()
		local collides = {}

		for _, p in ipairs(parts) do
			if p:IsA("BasePart") then
				collides[p] = p.CanCollide
				p.CanCollide = false
			end
		end

		local connection
		connection = game:GetService("RunService").Heartbeat:Connect(function()
			local elapsed = tick() - startTime
			local alpha = math.clamp(elapsed / duration, 0, 1)

			local newCFrame = startCFrame:Lerp(targetCFrame, alpha)
			local spin = CFrame.Angles(0, math.rad(720 * alpha), 0)
			Car:PivotTo(newCFrame * spin)

			if alpha >= 1 then
				connection:Disconnect()
				for part, oldCollide in pairs(collides) do
					if part and part.Parent then
						part.CanCollide = oldCollide
					end
				end
			end
		end)
	end

	-- 📌 Loop de teleports
	local function StartCarLoop()
		spawn(function()
			while isActive do
				local car = GetCurrentCar()
				if car and car:IsA("Model") then
					for name, cf in pairs(teleportPlaces) do
						if not isActive then break end
						SmoothTPToCFrame(car, cf, 2) -- 2 segundos de transición
						task.wait(2.5) -- un poco más para que termine la animación
					end
				end
				task.wait(1) -- espera antes de repetir ciclo
			end
		end)
	end

	-- 📌 Toggle con botón
	button.MouseButton1Click:Connect(function()
		isActive = not isActive
		if isActive then
			button.Text = "Annoy In Pizzeria Using Vehicle: ON"
			StartCarLoop()
		else
			button.Text = "Annoy In Pizzeria Using Vehicle: OFF"
		end
	end)

end;
task.spawn(C_cb);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.Car.Frame.IncreaseSpeedValue.LocalScript
local function C_cd()
	local script = G2L["cd"];
	local velocityMult = 0
	local velocityEnabled = false
	local LocalPlayer = game.Players.LocalPlayer
	local IncreaseSpeedValue = script.Parent
	local DecreaseSpeedValue = script.Parent.Parent:WaitForChild("DecreaseSpeedValue")

	-- Función para correr el loop solo si no está corriendo ya
	local function StartVelocityLoop()
		if velocityEnabled then return end -- ya está corriendo
		velocityEnabled = true

		task.spawn(function()
			while velocityEnabled and velocityMult > 0 do
				task.wait(0.1) -- control de frecuencia

				local Character = LocalPlayer.Character
				if Character then
					local Humanoid = Character:FindFirstChildWhichIsA("Humanoid")
					if Humanoid then
						local SeatPart = Humanoid.SeatPart
						if SeatPart and SeatPart:IsA("VehicleSeat") then
							local vel = SeatPart.AssemblyLinearVelocity
							SeatPart.AssemblyLinearVelocity = Vector3.new(
								vel.X * (1 + velocityMult),
								vel.Y,
								vel.Z * (1 + velocityMult)
							)
						end
					end
				end
			end
			-- si sale del loop
			velocityEnabled = false
		end)
	end

	local function UpdateTextBoxCounter()
		script.Parent.Parent.CurrentAmmount.Text = string.format("%.2f", velocityMult).."x"
	end

	script.Parent.Parent.CurrentAmmount.FocusLost:Connect(function()
		local newAmmount = tonumber(script.Parent.Parent.CurrentAmmount.Text)
		if newAmmount then
			velocityMult = math.max(0, newAmmount)
			if velocityMult <= 0 then
				velocityEnabled = false
			else
				StartVelocityLoop()
			end
		end
	end)
	-- Botón para aumentar velocidad
	IncreaseSpeedValue.MouseButton1Click:Connect(function()
		velocityMult = velocityMult + 0.01
		StartVelocityLoop() -- solo arranca loop si hace falta
	end)

	-- Botón para disminuir velocidad
	DecreaseSpeedValue.MouseButton1Click:Connect(function()
		velocityMult = math.max(0, velocityMult - 0.01)

		-- Si llegamos a 0, apagamos el loop
		if velocityMult <= 0 then
			velocityEnabled = false
		end
	end)

end;
task.spawn(C_cd);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.Car.Frame.ActionButtonHOUSE.LocalScript
local function C_cf()
	local script = G2L["cf"];
	-- 🎨 Colores disponibles
	local colors = {
		BrickColor.Red(),
		BrickColor.Blue(),
		BrickColor.Green(),
		BrickColor.Yellow(),
		BrickColor.Gray(),
		BrickColor.White(),
		BrickColor.DarkGray()
	}

	local currentIndex = 1
	local isActive = false -- Toggle para encender/apagar el loop
	local player = game.Players.LocalPlayer
	local button = script.Parent -- el botón con texto "Mega-MultiColor [can lag]"

	-- 📌 Función para obtener el coche actual
	local function GetCurrentCar()
		if not player.Character or not player.Character:FindFirstChild("Humanoid") then
			return nil
		end

		local ActualCarBox = button.Parent.Parent.ActualCarBox
		local CarObjectValue: ObjectValue = ActualCarBox:FindFirstChild("CarObjectValue")
		if not CarObjectValue or not CarObjectValue.Value then
			return nil
		end

		return CarObjectValue.Value
	end

	-- 📌 Función para pintar partes del coche
	local function PaintCar(Car: Model, Color_Code: BrickColor)
		for _, Child in ipairs(Car:GetChildren()) do
			-- 🔹 Primer método
			game:GetService("ReplicatedStorage").VehicleChannel:FireServer(
				"PaintVehicle",
				Child,
				"None",
				Color_Code.Color,
				false
			)

			-- 🔹 Segundo método (por compatibilidad)
			task.spawn(function()
				game:GetService("ReplicatedStorage").Channels.VehicleChannel:FireServer(
					"Paint",
					Child,
					"None",
					Color_Code
				)
			end)
		end
	end

	-- 📌 Función para pintar el coche en modo arcoiris
	local function PaintACar(Car: Model)
		local selectedColor = colors[currentIndex]
		PaintCar(Car, selectedColor)

		-- Avanzar al siguiente color
		currentIndex += 1
		if currentIndex > #colors then
			currentIndex = 1
		end
	end

	-- 🔄 Loop controlado por toggle
	local function StartMultiColorLoop()
		while isActive do
			local car = GetCurrentCar()
			if car then
				PaintACar(car)
			end
			task.wait(1) -- ⏱️ cambia color cada segundo
		end
	end

	-- 🎛️ Evento del botón
	button.MouseButton1Click:Connect(function()
		if not GetCurrentCar() then
			return
		end
		isActive = not isActive -- Alterna ON/OFF
		if isActive then
			button.Text = "Stop MultiColor"
			task.spawn(StartMultiColorLoop)
		else
			button.Text = "Mega-MultiColor [can lag]"
		end
	end)

end;
task.spawn(C_cf);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.Car.Frame.ActionButtonHOUSE.LocalScript
local function C_d1()
	local script = G2L["d1"];
	-- 📌 Función para detectar el coche actual
	local player = game.Players.LocalPlayer
	local function GetCurrentCar()
		if not player.Character or not player.Character:FindFirstChild("Humanoid") then
			return nil
		end

		local ActualCarBox = script.Parent.Parent.Parent.ActualCarBox
		local CarObjectValue: ObjectValue = ActualCarBox:FindFirstChild("CarObjectValue")
		if not CarObjectValue or not CarObjectValue.Value then
			return nil
		end

		return CarObjectValue.Value
	end

	-- 📌 Función para simular la tecla
	local function pressKey(k)
		local vim = game:GetService("VirtualInputManager")
		vim:SendKeyEvent(true, k, false, nil)
		task.wait(0.8)
		vim:SendKeyEvent(false, k, false, nil)
	end

	-- 📌 Toggle del loop
	local IsRunning = false
	local button = script.Parent

	button.MouseButton1Click:Connect(function()
		IsRunning = not IsRunning
		if IsRunning then
			button.Text = "Stop loop-horn"
			task.spawn(function()
				while IsRunning do
					local car = GetCurrentCar()
					if car then
						pressKey("H")
					end
					task.wait(0.8) -- tiempo entre bocinazos
				end
			end)
		else
			button.Text = "loop-horn"
		end
	end)

end;
task.spawn(C_d1);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.Car.Frame.DecreaseSpeedValue.LocalScript
local function C_d3()
	local script = G2L["d3"];
	local function getplayer()
		local textbox = script.Parent.Parent.Parent.PlayerNameTextBox
		local player = game.Players:FindFirstChild(textbox.Text)
		if player then
			return player
		end
	end
	local remote
	do
		local reg = (getreg or debug.getregistry)()
		for i=1,#reg do
			local f = reg[i]
			if type(f)=="table" and rawget(f,"FireServer") and rawget(f,"BindEvents") then
				remote = f
			end
		end
	end
	function moveThing(bmd, location)
		remote:FireServer("UpdateProperty", bmd, "CFrame", location)
		wait()
		remote:FireServer("SquishDough", bmd)
	end
	local ruinherlife = false
	local playerToF0ckIt
	script.Parent.MouseButton1Click:Connect(function()
		local PlayerToManager = getplayer()
		if PlayerToManager then
			playerToF0ckIt = PlayerToManager
			ruinherlife = not ruinherlife
			if ruinherlife then
				script.Parent.Text ="un fill it with garbage"
				script.Parent.BackgroundColor3 = Color3.new(1, 0, 0)
			else
				script.Parent.Text = "fill it with garbage"
				script.Parent.BackgroundColor3 = Color3.new(0, 0.470588, 0.843137)
			end
			while ruinherlife do
				wait()
				local posicionActual = playerToF0ckIt.Character.HumanoidRootPart.Position
				if not posicionActual then return end
				spawn(function()
					for i,v in pairs(workspace.AllDough:GetChildren()) do
						local NEWPLRPOS = CFrame.new(
							posicionActual.X + math.random(-5, 5), -- Variación en el eje X (-10 a 10)
							posicionActual.Y, -- Mantenemos la misma altura
							posicionActual.Z + math.random(-5, 5) -- Variación en el eje Z (-10 a 10)
						)
						moveThing(v, NEWPLRPOS)
					end
				end)
				spawn(function()
					for i,v in pairs(workspace.AllMountainDew:GetChildren()) do
						local NEWPLRPOS = CFrame.new(
							posicionActual.X + math.random(-5, 5), -- Variación en el eje X (-10 a 10)
							posicionActual.Y, -- Mantenemos la misma altura
							posicionActual.Z + math.random(-5, 5) -- Variación en el eje Z (-10 a 10)
						)
						moveThing(v, NEWPLRPOS)
					end
				end)
				spawn(function()
					for i,v in pairs(workspace.BoxingRoom:GetChildren()) do
						local NEWPLRPOS = CFrame.new(
							posicionActual.X + math.random(-5, 5), -- Variación en el eje X (-10 a 10)
							posicionActual.Y, -- Mantenemos la misma altura
							posicionActual.Z + math.random(-5, 5) -- Variación en el eje Z (-10 a 10)
						)
						moveThing(v, NEWPLRPOS)
					end
				end)
				spawn(function()
					for i,v in pairs(workspace.AllBox:GetChildren()) do
						local NEWPLRPOS = CFrame.new(
							posicionActual.X + math.random(-5, 5), -- Variación en el eje X (-10 a 10)
							posicionActual.Y, -- Mantenemos la misma altura
							posicionActual.Z + math.random(-5, 5) -- Variación en el eje Z (-10 a 10)
						)
						moveThing(v, NEWPLRPOS)
					end
				end)
				spawn(function()
					for i,v in pairs(workspace.AllSupplyBoxes:GetChildren()) do
						local NEWPLRPOS = CFrame.new(
							posicionActual.X + math.random(-5, 5), -- Variación en el eje X (-10 a 10)
							posicionActual.Y, -- Mantenemos la misma altura
							posicionActual.Z + math.random(-5, 5) -- Variación en el eje Z (-10 a 10)
						)
						moveThing(v, NEWPLRPOS)
					end
				end)
			end
		end
	end)
end;
task.spawn(C_d3);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.Car.Frame.TextBox.LocalScript
local function C_d5()
	local script = G2L["d5"];
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer


	-- Función para hacer spin al auto
	local function TeleportCarTo(carModel: Model, Position: Vector3)
		if not carModel or not carModel.PrimaryPart then
			warn("❌ El auto no tiene PrimaryPart asignado")
			return
		end

		local seat = carModel:FindFirstChildWhichIsA("VehicleSeat", true)
		if not seat then
			warn("❌ El auto no tiene VehicleSeat")
			return
		end

		-- Verificar que el jugador esté sentado en el auto
		if seat.Occupant ~= player.Character:FindFirstChild("Humanoid") then
			warn("⚠️ No estás sentado en este auto")
			return
		end

		-- teleport car
		carModel:SetPrimaryPartCFrame(CFrame.new(Position))
		print("✅ Auto teletransportado a la posición:", Position)
	end

	local function GetCurrentCar()
		if not player.Character or not player.Character:FindFirstChild("Humanoid") then
			return nil
		end

		local ActualCarBox = script.Parent.Parent.Parent.ActualCarBox
		local CarObjectValue: ObjectValue = ActualCarBox:FindFirstChild("CarObjectValue")
		if not CarObjectValue or not CarObjectValue.Value then
			return nil
		end

		return CarObjectValue.Value
	end

	-- Función para buscar al jugador
	local function buscarJugador(nombre)
		-- Crear una expresión regular para buscar coincidencias parciales
		local pattern = "^" .. nombre:lower() .. ".*$"

		-- Buscar coincidencias en los nombres de los jugadores
		for _, player in ipairs(game.Players:GetPlayers()) do
			if player.Name:lower():match(pattern) or player.DisplayName:lower():match(pattern) then
				print("player Found: " .. player.Name)
				return player
			end
		end
	end

	local textBox = script.Parent
	-- Conectar la función al evento FocusLost del TextBox
	textBox.FocusLost:Connect(function(enterPressed)
		-- Verificar si se presionó la tecla Enter al perder el foco
		if enterPressed then
			-- Obtener el texto del TextBox
			local PlayerName = textBox.Text

			-- Verificar si el texto no está vacío
			if PlayerName ~= "" then
				-- Buscar al jugador usando el texto del TextBox
				local Result = buscarJugador(PlayerName:lower()) -- Convertir a minúsculas para hacer coincidencia de caso-insensitivo
				if Result then
					local Car = GetCurrentCar()
					if Car then
						TeleportCarTo(Car, Result.Character.HumanoidRootPart.Position)
					end
				else
					warn("Player not found: " .. PlayerName)
				end
			else
				warn("operation canceled because the Textbox has no text.")
			end
		end
	end)


end;
task.spawn(C_d5);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.Car.Frame.ActionButtonHOUSE.LocalScript
local function C_d7()
	local script = G2L["d7"];

	local player = game.Players.LocalPlayer

	local function GetCurrentCar()
		if not player.Character or not player.Character:FindFirstChild("Humanoid") then
			return nil
		end

		local ActualCarBox = script.Parent.Parent.Parent.ActualCarBox
		local CarObjectValue: ObjectValue = ActualCarBox:FindFirstChild("CarObjectValue")
		if not CarObjectValue or not CarObjectValue.Value then
			return nil
		end

		return CarObjectValue.Value
	end

	local FLYING = false
	local QEfly = true
	local iyflyspeed = 1
	local vehicleflyspeed = 1

	local flyKeyUp  = nil
	local flyKeyDown = nil

	local function getRoot(character: Model)
		return character:FindFirstChild("HumanoidRootPart") or character:FindFirstChild("Torso")
	end

	local Players = game:GetService("Players")
	local UserInputService = game:GetService("UserInputService")
	local RunService = game:GetService("RunService")
	local randomString = function()
		return math.random() * math.random()
	end
	local camera = game.Workspace.CurrentCamera
	function sFLY(vfly)
		local plr = Players.LocalPlayer
		local char = plr.Character or plr.CharacterAdded:Wait()
		local humanoid = char:FindFirstChildOfClass("Humanoid")
		if not humanoid then
			repeat task.wait() until char:FindFirstChildOfClass("Humanoid")
			humanoid = char:FindFirstChildOfClass("Humanoid")
		end

		if flyKeyDown or flyKeyUp then
			flyKeyDown:Disconnect()
			flyKeyUp:Disconnect()
		end

		local T = getRoot(char)
		local CONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
		local lCONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
		local SPEED = 0

		local function FLY()
			FLYING = true
			local BG = Instance.new('BodyGyro')
			local BV = Instance.new('BodyVelocity')
			BG.P = 9e4
			BG.Parent = T
			BV.Parent = T
			BG.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
			BG.CFrame = T.CFrame
			BV.Velocity = Vector3.new(0, 0, 0)
			BV.MaxForce = Vector3.new(9e9, 9e9, 9e9)
			task.spawn(function()
				repeat task.wait()
					local camera = workspace.CurrentCamera
					if not vfly and humanoid then
						humanoid.PlatformStand = true
					end

					if CONTROL.L + CONTROL.R ~= 0 or CONTROL.F + CONTROL.B ~= 0 or CONTROL.Q + CONTROL.E ~= 0 then
						SPEED = 50
					elseif not (CONTROL.L + CONTROL.R ~= 0 or CONTROL.F + CONTROL.B ~= 0 or CONTROL.Q + CONTROL.E ~= 0) and SPEED ~= 0 then
						SPEED = 0
					end
					if (CONTROL.L + CONTROL.R) ~= 0 or (CONTROL.F + CONTROL.B) ~= 0 or (CONTROL.Q + CONTROL.E) ~= 0 then
						BV.Velocity = ((camera.CFrame.LookVector * (CONTROL.F + CONTROL.B)) + ((camera.CFrame * CFrame.new(CONTROL.L + CONTROL.R, (CONTROL.F + CONTROL.B + CONTROL.Q + CONTROL.E) * 0.2, 0).p) - camera.CFrame.p)) * SPEED
						lCONTROL = {F = CONTROL.F, B = CONTROL.B, L = CONTROL.L, R = CONTROL.R}
					elseif (CONTROL.L + CONTROL.R) == 0 and (CONTROL.F + CONTROL.B) == 0 and (CONTROL.Q + CONTROL.E) == 0 and SPEED ~= 0 then
						BV.Velocity = ((camera.CFrame.LookVector * (lCONTROL.F + lCONTROL.B)) + ((camera.CFrame * CFrame.new(lCONTROL.L + lCONTROL.R, (lCONTROL.F + lCONTROL.B + CONTROL.Q + CONTROL.E) * 0.2, 0).p) - camera.CFrame.p)) * SPEED
					else
						BV.Velocity = Vector3.new(0, 0, 0)
					end
					BG.CFrame = camera.CFrame
				until not FLYING
				CONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
				lCONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
				SPEED = 0
				BG:Destroy()
				BV:Destroy()

				if humanoid then humanoid.PlatformStand = false end
			end)
		end

		flyKeyDown = UserInputService.InputBegan:Connect(function(input, processed)
			if input.KeyCode == Enum.KeyCode.W then
				CONTROL.F = (vfly and vehicleflyspeed or iyflyspeed)
			elseif input.KeyCode == Enum.KeyCode.S then
				CONTROL.B = - (vfly and vehicleflyspeed or iyflyspeed)
			elseif input.KeyCode == Enum.KeyCode.A then
				CONTROL.L = - (vfly and vehicleflyspeed or iyflyspeed)
			elseif input.KeyCode == Enum.KeyCode.D then
				CONTROL.R = (vfly and vehicleflyspeed or iyflyspeed)
			elseif input.KeyCode == Enum.KeyCode.E and QEfly then
				CONTROL.Q = (vfly and vehicleflyspeed or iyflyspeed)*2
			elseif input.KeyCode == Enum.KeyCode.Q and QEfly then
				CONTROL.E = -(vfly and vehicleflyspeed or iyflyspeed)*2
			end
			pcall(function() camera.CameraType = Enum.CameraType.Track end)
		end)

		flyKeyUp = UserInputService.InputEnded:Connect(function(input, processed)
			if input.KeyCode == Enum.KeyCode.W then
				CONTROL.F = 0
			elseif input.KeyCode == Enum.KeyCode.S then
				CONTROL.B = 0
			elseif input.KeyCode == Enum.KeyCode.A then
				CONTROL.L = 0
			elseif input.KeyCode == Enum.KeyCode.D then
				CONTROL.R = 0
			elseif input.KeyCode == Enum.KeyCode.E then
				CONTROL.Q = 0
			elseif input.KeyCode == Enum.KeyCode.Q then
				CONTROL.E = 0
			end
		end)
		FLY()
	end

	function NOFLY()
		FLYING = false
		if flyKeyDown or flyKeyUp then flyKeyDown:Disconnect() flyKeyUp:Disconnect() end
		if Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid') then
			Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid').PlatformStand = false
		end
		pcall(function() workspace.CurrentCamera.CameraType = Enum.CameraType.Custom end)
	end

	local velocityHandlerName = randomString()
	local gyroHandlerName = randomString()
	local mfly1
	local mfly2

	local unmobilefly = function(speaker)
		pcall(function()
			FLYING = false
			local root = getRoot(speaker.Character)
			root:FindFirstChild(velocityHandlerName):Destroy()
			root:FindFirstChild(gyroHandlerName):Destroy()
			speaker.Character:FindFirstChildWhichIsA("Humanoid").PlatformStand = false
			mfly1:Disconnect()
			mfly2:Disconnect()
		end)
	end

	local mobilefly = function(speaker, vfly)
		unmobilefly(speaker)
		FLYING = true

		local root = getRoot(speaker.Character)
		local camera = workspace.CurrentCamera
		local v3none = Vector3.new()
		local v3zero = Vector3.new(0, 0, 0)
		local v3inf = Vector3.new(9e9, 9e9, 9e9)

		local controlModule = require(speaker.PlayerScripts:WaitForChild("PlayerModule"):WaitForChild("ControlModule"))
		local bv = Instance.new("BodyVelocity")
		bv.Name = velocityHandlerName
		bv.Parent = root
		bv.MaxForce = v3zero
		bv.Velocity = v3zero

		local bg = Instance.new("BodyGyro")
		bg.Name = gyroHandlerName
		bg.Parent = root
		bg.MaxTorque = v3inf
		bg.P = 1000
		bg.D = 50

		mfly1 = speaker.CharacterAdded:Connect(function()
			local bv = Instance.new("BodyVelocity")
			bv.Name = velocityHandlerName
			bv.Parent = root
			bv.MaxForce = v3zero
			bv.Velocity = v3zero

			local bg = Instance.new("BodyGyro")
			bg.Name = gyroHandlerName
			bg.Parent = root
			bg.MaxTorque = v3inf
			bg.P = 1000
			bg.D = 50
		end)

		mfly2 = RunService.RenderStepped:Connect(function()
			root = getRoot(speaker.Character)
			camera = workspace.CurrentCamera
			if speaker.Character:FindFirstChildWhichIsA("Humanoid") and root and root:FindFirstChild(velocityHandlerName) and root:FindFirstChild(gyroHandlerName) then
				local humanoid = speaker.Character:FindFirstChildWhichIsA("Humanoid")
				local VelocityHandler = root:FindFirstChild(velocityHandlerName)
				local GyroHandler = root:FindFirstChild(gyroHandlerName)

				VelocityHandler.MaxForce = v3inf
				GyroHandler.MaxTorque = v3inf
				if not vfly then humanoid.PlatformStand = true end
				GyroHandler.CFrame = camera.CoordinateFrame
				VelocityHandler.Velocity = v3none

				local direction = controlModule:GetMoveVector()
				if direction.X > 0 then
					VelocityHandler.Velocity = VelocityHandler.Velocity + camera.CFrame.RightVector * (direction.X * ((vfly and vehicleflyspeed or iyflyspeed) * 50))
				end
				if direction.X < 0 then
					VelocityHandler.Velocity = VelocityHandler.Velocity + camera.CFrame.RightVector * (direction.X * ((vfly and vehicleflyspeed or iyflyspeed) * 50))
				end
				if direction.Z > 0 then
					VelocityHandler.Velocity = VelocityHandler.Velocity - camera.CFrame.LookVector * (direction.Z * ((vfly and vehicleflyspeed or iyflyspeed) * 50))
				end
				if direction.Z < 0 then
					VelocityHandler.Velocity = VelocityHandler.Velocity - camera.CFrame.LookVector * (direction.Z * ((vfly and vehicleflyspeed or iyflyspeed) * 50))
				end
			end
		end)
	end

	local VehicleFlyActive = false

	local IsOnMobile = table.find({Enum.Platform.Android, Enum.Platform.IOS}, UserInputService:GetPlatform())

	local Noclipping = nil
	local Clip = true
	local vnoclipParts = {}
	local speaker = game:GetService("Players").LocalPlayer
	local RunService = game:GetService("RunService")

	local flinging = false 

	local function EnableNoclip()
		Clip = false
		wait(0.1)
		local function NoclipLoop()
			if Clip == false and speaker.Character ~= nil then
				for _, child in pairs(speaker.Character:GetDescendants()) do
					if child:IsA("BasePart") and child.CanCollide == true then
						child.CanCollide = false
					end
				end
			end
		end
		Noclipping = RunService.Stepped:Connect(NoclipLoop)
	end

	local function DisableNoclip()
		if Noclipping then
			Noclipping:Disconnect()
		end
		Clip = true
	end
	local flingDied 
	local function DisableFling()
		DisableNoclip()
		if flingDied then
			flingDied:Disconnect()
		end
		flinging = false
		wait(.1)
		local speakerChar = speaker.Character
		if not speakerChar or not getRoot(speakerChar) then return end
		for i,v in pairs(getRoot(speakerChar):GetChildren()) do
			if v.ClassName == 'BodyAngularVelocity' then
				v:Destroy()
			end
		end
		for _, child in pairs(speakerChar:GetDescendants()) do
			if child.ClassName == "Part" or child.ClassName == "MeshPart" then
				child.CustomPhysicalProperties = PhysicalProperties.new(0.7, 0.3, 0.5)
			end
		end
	end
	local function EnableFlingForCar(Model: Model)
		flinging = true
		flinging = false
		for _, child in pairs(Model:GetDescendants()) do
			if child:IsA("BasePart") then
				child.CustomPhysicalProperties = PhysicalProperties.new(100, 0.3, 0.5)
			end
		end
		EnableNoclip()
		wait(.1)
		local bambam = Instance.new("BodyAngularVelocity")
		bambam.Name = randomString()
		bambam.Parent = getRoot(Model)
		bambam.AngularVelocity = Vector3.new(0,99999,0)
		bambam.MaxTorque = Vector3.new(0,math.huge,0)
		bambam.P = math.huge
		local Char = Model:GetChildren()
		for i, v in next, Char do
			if v:IsA("BasePart") then
				v.CanCollide = false
				v.Massless = true
				v.Velocity = Vector3.new(0, 0, 0)
			end
		end
		flinging = true
		local function flingDiedF()
			DisableFling()
		end
		flingDied = speaker.Character:FindFirstChildOfClass('Humanoid').Died:Connect(flingDiedF)
		repeat
			bambam.AngularVelocity = Vector3.new(0,99999,0)
			wait(.2)
			bambam.AngularVelocity = Vector3.new(0,0,0)
			wait(.1)
		until flinging == false
	end

	script.Parent.MouseButton1Click:Connect(function()
		if not VehicleFlyActive then
			local car = GetCurrentCar()
			if not car then return end

			-- Activar Fly
			if not IsOnMobile then
				NOFLY()
				task.wait()
				sFLY(true)
			else
				mobilefly(player, true)
			end

			-- Activar Noclip para auto y jugador
			EnableNoclip()
			for _, v in pairs(car:GetDescendants()) do
				if v:IsA("BasePart") then
					v.CanCollide = false
				end
			end

			-- Activar fling para el auto
			EnableFlingForCar(car)

			-- Detectar cuando el jugador se baje del auto
			local ActualCarBox = script.Parent.Parent.Parent.ActualCarBox
			local CarObjectValue: ObjectValue = ActualCarBox:FindFirstChild("CarObjectValue")

			CarObjectValue.Changed:Once(function()
				if CarObjectValue.Value == nil then
					-- Apagar todo
					if not IsOnMobile then
						NOFLY()
					else
						unmobilefly(player)
					end
					DisableNoclip()
					DisableFling()
					script.Parent.Text = "Vehicle FlyFling: OFF"
				end
			end)

			VehicleFlyActive = true
			script.Parent.Text = "Vehicle Fly: ON"
		else
			-- Si está activo, desactivamos manualmente
			if not IsOnMobile then
				NOFLY()
			else
				unmobilefly(player)
			end
			DisableNoclip()
			DisableFling()
			VehicleFlyActive = false
			script.Parent.Text = "Vehicle FlyFling: OFF"
		end
	end)

end;
task.spawn(C_d7);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.Car.Frame.ActionButtonHOUSE.LocalScript
local function C_d9()
	local script = G2L["d9"];
	local function getplayer()
		local textbox = script.Parent.Parent.Parent.PlayerNameTextBox
		local player = game.Players:FindFirstChild(textbox.Text)
		if player then
			return player
		end
	end

	script.Parent.MouseButton1Click:Connect(function()
		if getplayer() then
			local playerToHouse = getplayer()
			for _, house in pairs(game:GetService("Workspace").Houses:GetChildren()) do
				if house:FindFirstChild(getplayer().DisplayName) then
					game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(house:FindFirstChild(getplayer().DisplayName).Head.CFrame)
				end
			end
		end
	end)
end;
task.spawn(C_d9);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.Car.Frame.TextBox.LocalScript
local function C_db()
	local script = G2L["db"];
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer

	-- Variable global para no spawnear mil veces el BodyAngularVelocity
	local currentSpinForce = nil  

	-- Función para hacer spin al auto
	local function SpinCar(carModel: Model, spinSpeed: number)
		if not carModel or not carModel.PrimaryPart then
			warn("❌ El auto no tiene PrimaryPart asignado")
			return
		end

		local seat = carModel:FindFirstChildWhichIsA("VehicleSeat", true)
		if not seat then
			warn("❌ El auto no tiene VehicleSeat")
			return
		end

		-- Verificar que el jugador esté sentado en el auto
		if seat.Occupant ~= player.Character:FindFirstChild("Humanoid") then
			warn("⚠️ No estás sentado en este auto")
			return
		end

		-- Si ya existe un spin activo, lo quitamos primero
		if currentSpinForce then
			currentSpinForce:Destroy()
			currentSpinForce = nil
		end

		-- Crear BodyAngularVelocity para girar el auto
		local bav = Instance.new("BodyAngularVelocity")
		bav.AngularVelocity = Vector3.new(0, spinSpeed, 0) -- spin en Y
		bav.MaxTorque = Vector3.new(0, math.huge, 0) -- torque infinito en Y
		bav.P = 10000
		bav.Parent = carModel.PrimaryPart

		currentSpinForce = bav
	end

	local function GetCurrentCar()
		if not player.Character or not player.Character:FindFirstChild("Humanoid") then
			return nil
		end

		local ActualCarBox = script.Parent.Parent.Parent.ActualCarBox
		local CarObjectValue: ObjectValue = ActualCarBox:FindFirstChild("CarObjectValue")
		if not CarObjectValue or not CarObjectValue.Value then
			return nil
		end

		return CarObjectValue.Value
	end

	script.Parent.FocusLost:Connect(function()
		local car = GetCurrentCar()
		if currentSpinForce then
			currentSpinForce:Destroy()
			currentSpinForce = nil
		end
		if not car then
			return
		end

		local speed = tonumber(script.Parent.Text)
		if not speed then
			return
		end

		SpinCar(car, speed)
	end)
end;
task.spawn(C_db);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.Car.Frame.ActionButtonHOUSE.LocalScript
local function C_dd()
	local script = G2L["dd"];
	local player = game.Players.LocalPlayer
	local RunService = game:GetService("RunService")

	-- Variable global para no spawnear mil veces el BodyAngularVelocity
	local currentSpinForce = nil  
	local loopConnection = nil -- conexión al Heartbeat

	-- Función para hacer spin al auto
	local function SpinCar(carModel: Model, spinSpeed: number)
		if not carModel or not carModel.PrimaryPart then
			warn("❌ El auto no tiene PrimaryPart asignado")
			return
		end

		local seat = carModel:FindFirstChildWhichIsA("VehicleSeat", true)
		if not seat then
			warn("❌ El auto no tiene VehicleSeat")
			return
		end

		-- Verificar que el jugador esté sentado en el auto
		if seat.Occupant ~= player.Character:FindFirstChild("Humanoid") then
			warn("⚠️ No estás sentado en este auto")
			return
		end

		-- Si ya existe un spin activo, lo quitamos primero
		if currentSpinForce then
			currentSpinForce:Destroy()
			currentSpinForce = nil
		end

		-- Crear BodyAngularVelocity para girar el auto
		local bav = Instance.new("BodyAngularVelocity")
		bav.AngularVelocity = Vector3.new(0, spinSpeed, 0) -- spin en Y
		bav.MaxTorque = Vector3.new(0, math.huge, 0) -- torque infinito en Y
		bav.P = 10000
		bav.Parent = carModel.PrimaryPart

		currentSpinForce = bav
	end

	local function GetCurrentCar()
		if not player.Character or not player.Character:FindFirstChild("Humanoid") then
			return nil
		end

		local ActualCarBox = script.Parent.Parent.Parent.ActualCarBox
		local CarObjectValue: ObjectValue = ActualCarBox:FindFirstChild("CarObjectValue")
		if not CarObjectValue or not CarObjectValue.Value then
			return nil
		end

		return CarObjectValue.Value
	end

	local IsActive = false
	local oldtext = script.Parent.Text
	local SavedCFrame = nil

	script.Parent.MouseButton1Click:Connect(function()
		IsActive = not IsActive

		if IsActive then
			local car = GetCurrentCar()
			if not car then
				return
			end

			-- Activar spin
			SpinCar(car, 100)
			SavedCFrame = car:GetPivot()

			-- Loop fuerte con Heartbeat
			loopConnection = RunService.Heartbeat:Connect(function()
				if not IsActive or not car or not car.Parent then return end

				local currentCFrame = car:GetPivot()
				-- Revisa que el carro no se aleje más de 15 studs
				if (currentCFrame.Position - SavedCFrame.Position).Magnitude > 15 then
					car:PivotTo(SavedCFrame)
				end
			end)

			script.Parent.Text = "Stop Making Engine Sound"
		else
			-- Apagar spin
			if currentSpinForce then
				currentSpinForce:Destroy()
				currentSpinForce = nil
			end

			-- Desconectar loop fuerte
			if loopConnection then
				loopConnection:Disconnect()
				loopConnection = nil
			end

			-- Resetear el auto a su posición original
			if SavedCFrame then
				local car = GetCurrentCar()
				if car then
					car:PivotTo(SavedCFrame)
				end
			end

			script.Parent.Text = oldtext
			SavedCFrame = nil
		end
	end)

end;
task.spawn(C_dd);
-- StarterGui.ScreenGui.WindowFrame.CloseWarning.Frame.no.LocalScript
local function C_e4()
	local script = G2L["e4"];
	local NoButton = script.Parent
	local CloseWarning = NoButton.Parent.Parent
	local function closewarning()
		CloseWarning.Visible = false
	end
	NoButton.MouseButton1Click:Connect(closewarning)
end;
task.spawn(C_e4);
-- StarterGui.ScreenGui.WindowFrame.CloseWarning.Frame.yes.LocalScript
local function C_e6()
	local script = G2L["e6"];
	local TweenService = game:GetService("TweenService")
	local yesButton = script.Parent
	local screenGui = yesButton.Parent.Parent.Parent.Parent
	local windowFrame = screenGui:FindFirstChild("WindowFrame")

	-- Función para crear un Tween
	local function createTween(object, properties, duration)
		local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
		local tween = TweenService:Create(object, tweenInfo, properties)
		tween:Play()
		return tween
	end

	-- Función para ocultar con Tween
	local function fadeOut(object, duration)
		if object:IsA("GuiObject") then
			createTween(object, {Transparency = 1}, duration)
		end

		-- Si es un ImageLabel o ImageButton, también ajustamos la transparencia de la imagen
		if object:IsA("ImageLabel") or object:IsA("ImageButton") then
			createTween(object, {ImageTransparency = 1}, duration)
		end

		-- Aplicamos a los hijos
		for _, child in ipairs(object:GetChildren()) do
			fadeOut(child, duration)
		end
	end

	-- Evento de clic en el botón "Yes"
	yesButton.MouseButton1Click:Connect(function()
		-- Oculta los botones de confirmación
		yesButton.Visible = false
		if yesButton.Parent:FindFirstChild("no") then
			yesButton.Parent.no.Visible = false
		end

		-- Cambiar el título de la ventana
		yesButton.Parent.Parent.title.Text = "Ok, Closing..."
		-- Mensaje de despedida
		yesButton.Parent.Parent.warning.Text = "Thanks for using my script"

		wait(1)

		-- Desvanecer toda la ventana
		if windowFrame then
			fadeOut(windowFrame, 2)
		end
		-- Esperar el tiempo del tween antes de destruir
		task.wait(2)

		-- Eliminar la GUI completamente
		screenGui:Destroy()

	end)

end;
task.spawn(C_e6);

return G2L["1"], require;
