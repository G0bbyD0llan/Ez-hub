-- StarterGui.ScreenGui.Script_starter
local function C_2()
local script = G2L["2"];
	script.Parent.WindowFrame.Visible = false
	
	local gbdl = {
	version = 1.0,
	lastmsg = "hello"
	}
	
	local IsTesting = false
	
		print(string.format([[
		
		
		
		
		
		
		
		
		
		
		
		
		
	- Work at a Pizza Place HUB -
	
	     -   -   -   -   -   
	     
	 Made By G0bbyD0llan --
	  
	 Version: %s --
	 Creator Last Message: %s --
	   
	%s
	           
	           
	           
	           
	           
	           
	           
	           
	           
	           
	           
	           
	           
	           
	]], tostring(gbdl.version or "..."), tostring(gbdl.lastmsg or "..."), IsTesting and "TEST VERSION --" or "NORMAL VERSION --"))
	
	---------------------------------
	task.spawn(function()
	--if Is testing the make a Hint Instance that will make a 4 second countdown
	if not IsTesting then return end
	--create the hint
	local hint = Instance.new("Hint")
	hint.Parent = game.Workspace
	hint.Text = "2"
	--start countdown
	for i = 1, 2 do
	task.wait(1)
	hint.Text = (2-i)
	end
	hint:Destroy()
	end)
	
	wait(IsTesting and 2 or 0)
	
	local ScreenGui = script.Parent -- Asegúrate de que el script esté dentro del ScreenGui
	local TweenService = game:GetService("TweenService")
	
	function PlayAudio(Id, TimePosition, Volume)
		-- Verifica si SoundService existe
		local SoundService = game:GetService("StarterGui")
	
		-- Crea un nuevo objeto Sound
		local sound = Instance.new("Sound")
		sound.SoundId = "rbxassetid://" .. tostring(Id)
		sound.Volume = Volume
		sound.TimePosition = TimePosition
		sound.Parent = SoundService  -- Se coloca en SoundService para que se reproduzca globalmente
	
		-- Reproducir el sonido
		sound:Play()
	
		-- Conectar evento para limpiar el sonido cuando termine
		sound.Ended:Connect(function()
			sound:Destroy()
		end)
	
		return sound  -- Retorna el sonido por si se necesita manipular
	end
	
	
	-- Tabla para almacenar los valores originales
	local originalValues = {}
	
	-- Función para recorrer todos los objetos GUI
	local function storeAndModify(guiObject)
		if guiObject:IsA("GuiObject") then
			-- Guardar valores originales
			originalValues[guiObject] = {
				Transparency = guiObject.Transparency,
				BorderSize = guiObject.BorderSizePixel
			}
	if guiObject:IsA("ImageButton") then
	originalValues[guiObject]["ImageTransparency"] = guiObject.ImageTransparency
	end
			if guiObject:IsA("TextLabel") or guiObject:IsA("TextButton") or guiObject:IsA("TextBox") then
				originalValues[guiObject]["TextTransparenct"] = guiObject.TextTransparency
			end
			-- Modificar los valores temporalmente
			guiObject.Transparency = 1
			guiObject.BorderSizePixel = 0
			if guiObject:IsA("ImageButton") then
				guiObject.ImageTransparency = 1
			end
			if guiObject:IsA("TextLabel") or guiObject:IsA("TextButton") or guiObject:IsA("TextBox") then
				guiObject.TextTransparency = 1
			end
		end
	
		-- Aplicar a los descendientes
		for _, child in ipairs(guiObject:GetChildren()) do
			storeAndModify(child)
		end
	end
	
	-- Función para restaurar valores con tween
	local function restoreWithTween(guiObject)
		if originalValues[guiObject] then
			local goal = {
				Transparency = originalValues[guiObject].Transparency,
				BorderSizePixel = originalValues[guiObject].BorderSize
			}
			if guiObject:IsA("ImageButton") then
				goal.ImageTransparency = originalValues[guiObject].ImageTransparency
			end
			if guiObject:IsA("TextLabel") or guiObject:IsA("TextButton") or guiObject:IsA("TextBox") then
				goal.TextTransparency = originalValues[guiObject].TextTransparenct
			end
			local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
			local tween = TweenService:Create(guiObject, tweenInfo, goal)
			tween:Play()
		end
	
		-- Aplicar a los descendientes
		for _, child in ipairs(guiObject:GetChildren()) do
			restoreWithTween(child)
		end
	end
	
	-- Ejecutar el almacenamiento y modificación inicial
	storeAndModify(ScreenGui)
	task.spawn(function()
	wait(IsTesting and 0 or 0.4)
		PlayAudio(IsTesting and "14646205673" or "6366788549", IsTesting and 4.2 or 0, 2)
		ScreenGui.WindowFrame.Visible = true
	end)
	
	-- Esperar un poco y restaurar con tween
	wait(1)
	restoreWithTween(ScreenGui)
	
end;
task.spawn(C_2);
-- StarterGui.ScreenGui.WindowFrame.DraggableScript
local function C_4()
local script = G2L["4"];
	--Variables
	local TitleBar = script.Parent["TitleBar"]
	local IsCursonOnTitleBar = false
	--MouseEnter and MouseLeave Connection
	TitleBar.MouseEnter:Connect(function()
		if IsCursonOnTitleBar == false then
			IsCursonOnTitleBar = true
		end
	end)
	TitleBar.MouseLeave:Connect(function()
		if IsCursonOnTitleBar == true then
			IsCursonOnTitleBar = false
		end
	end)
	--Making the draggable gui
	local UserInputService = game:GetService("UserInputService")
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		if IsCursonOnTitleBar then
			local delta = input.Position - dragStart
			gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end;
task.spawn(C_4);
-- StarterGui.ScreenGui.WindowFrame.HowToUse
local function C_5()
local script = G2L["5"];
	local ArrowImage = script.Parent.Arrow
	local TweenService = game:GetService("TweenService")
	
	-- 🔹 Función para crear un Tween y reproducirlo
	local function createTween(object, properties, duration)
		local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
		local tween = TweenService:Create(object, tweenInfo, properties)
		tween:Play()
		return tween
	end
	
	-- 🔹 Mover la flecha a una posición y rotación específicas
	local function moveArrow(arrow, targetPosition, targetRotation, duration)
		if not arrow or not arrow:IsA("ImageLabel") then return end
	
		-- Aplicar Tween para la posición y rotación
		createTween(arrow, {
			Position = UDim2.new(targetPosition.X, 0, targetPosition.Y, 0),
			Rotation = targetRotation
		}, duration)
	end
	
	-- 🔹 Hacer que la flecha aparezca o desaparezca suavemente con Tween
	local function toggleArrowVisibility(arrow, isVisible, duration)
		if not arrow or not arrow:IsA("ImageLabel") then return end
		print("SHOWING OR HIDING ARROW")
	
		-- Si es visible, ponemos transparencia en 0, si no, en 1
		local transparencyGoal = isVisible and 0 or 1
		createTween(arrow, { ImageTransparency = transparencyGoal }, duration)
		print("TWEEN CREATED!")
	
	end
	
	-- 🔹 Crear y mostrar texto debajo de la flecha
	local function showTextBelowArrow(arrow, text, duration, textsize, OffSet, OffSet2)
		if not arrow or not arrow:IsA("ImageLabel") then return end
	
		-- Crear un nuevo TextLabel debajo del arrow
		local textLabel = Instance.new("TextLabel")
		textLabel.Text = text
		textLabel.BackgroundTransparency = 1
		textLabel.TextSize = textsize
		textLabel.TextTransparency = 1  -- Comienza invisible
		textLabel.Size = UDim2.new(0, 100,0, 20)  -- Ajusta el tamaño (se puede modificar)
		textLabel.ZIndex = 7
		textLabel.Name = "CT"
		textLabel.TextColor3 = Color3.new(1, 1, 1)
		local xOffset = (arrow.Position.X.Offset or 0) + (tonumber(OffSet2) or 0)
		local yOffset = (arrow.Position.Y.Offset or 0) + (tonumber(OffSet) or 0)
	
		textLabel.Position = UDim2.new(0, xOffset, 0, yOffset)
		textLabel.Parent = arrow  -- Se agrega al mismo parent que el arrow
	    print("TextLabel CREATED")
		-- Hacer aparecer el texto con un Tween
		createTween(textLabel, { TextTransparency = 0 }, duration)
	
		return textLabel  -- Retorna el TextLabel en caso de que necesites manipularlo después
	end
	
	
	-- 🔹 Ocultar el texto con animación
	local function hideTextBelowArrow(textLabel, duration)
		if not textLabel or not textLabel:IsA("TextLabel") then return end
	
		-- Hacer desaparecer el texto con un Tween
		createTween(textLabel, { TextTransparency = 1 }, duration)
		print("CREATING TWEEN TO HIDE TEXT")
	
		task.spawn(function()
		wait(1)
			print("DESTROYING...")
	
		textLabel:Destroy()
		end)
	end
	
	
	local function pointArrowToButton(arrow, button, duration)
		if not arrow or not button or not arrow:IsA("ImageLabel") or not button:IsA("TextButton") then
			return
		end
	
		-- Obtener la posición del botón
		local buttonPos = button.Position
		print("GOING TO TARGETPOS")
	
		-- Mover la flecha a la misma posición del botón, pero un poco más abajo
		local targetPosition = UDim2.new(buttonPos.X.Scale, buttonPos.X.Offset +10, buttonPos.Y.Scale, buttonPos.Y.Offset +55)  -- Ajusta 10 según sea necesario
	    print("TARGETPOS CREATED")
		-- Aplicar el Tween para mover la flecha hacia el botón
		createTween(arrow, { Position = targetPosition }, duration)
		print("TWEEN CREATED!!")
	end
	
	local function pointArrowToButton2(arrow, button, duration)
		if not arrow or not button or not arrow:IsA("ImageLabel") or not button:IsA("TextButton") then
			return
		end
	
		-- Obtener la posición del botón
		local buttonPos = button.Position
		print("GOING TO TARGETPOS")
	
		-- Mover la flecha a la misma posición del botón, pero un poco más abajo
		local targetPosition = UDim2.new(buttonPos.X.Scale, buttonPos.X.Offset +10, buttonPos.Y.Scale, buttonPos.Y.Offset +155)  -- Ajusta 10 según sea necesario
		print("TARGETPOS CREATED")
		-- Aplicar el Tween para mover la flecha hacia el botón
		createTween(arrow, { Position = targetPosition, Rotation = 180 }, duration)
		print("TWEEN CREATED!!")
	end
	
	local function ToggleArrrowMode()
		print("TOGGLING HOWTOUSE MODE...")
		script.Parent.HowToUseFRM.Visible = true
		script.Parent.HowToUseFRM.Transparency = 1
		script.Parent.Arrow.ImageTransparency = 1
		createTween(script.Parent.HowToUseFRM, { Transparency = 0.3 }, 2)
		
		script.Parent.Arrow.Visible = true
		toggleArrowVisibility(script.Parent.Arrow, true, 2)
	end
	
	local function DisableArrowMode()
		print("TOGGLING HOWTOUSE MODE...")
	
		createTween(script.Parent.HowToUseFRM, { Transparency = 1 }, 2)
		toggleArrowVisibility(script.Parent.Arrow, false, 2)
		wait(2)
		script.Parent.Arrow.Visible = false
		script.Parent.HowToUseFRM.Visible = false
		script.Parent.Arrow.ImageTransparency = 1
		script.Parent.HowToUseFRM.Transparency = 1
	end
	
	
	local function ShowZButton(Button, Duration)
		print("showing z button")
		task.spawn(function()
			if typeof(Button) == "Instance" and Button:IsA("TextButton") then
				print("Creating button")
				local CloneButton = Instance.new("TextButton")
				CloneButton.Parent = script.Parent
				CloneButton.ZIndex = 20
				CloneButton.Visible = true
				CloneButton.Text = Button.Text
				CloneButton.Position = Button.Position + UDim2.new(0,0,0.07,0)
				CloneButton.Size = UDim2.new(0, 100,0.05, 0)
				CloneButton.BackgroundColor3 = Button.BackgroundColor3
				CloneButton.TextScaled = Button.TextScaled
				CloneButton.TextSize = Button.TextSize
				CloneButton.TextColor3 = Button.TextColor3
				CloneButton.Transparency = 1
				print("button created")
	
				task.spawn(function()
					createTween(CloneButton, { Transparency = 0 }, Duration)
					print("showing button")
					wait(Duration)
					createTween(CloneButton, { Transparency = 1 }, Duration)
					print("hiding button")
					wait(Duration)
					CloneButton:Destroy()
				end)
			end
		end)
	end

	local function OnClick()
		local OldPosItIon = script.Parent.Arrow.Position
		ToggleArrrowMode()

		wait(2)

		showTextBelowArrow(script.Parent.Arrow, "Hello, Im your Helper. I Will show you how to use this script.", 2, 15, 60)

		wait(3)

		hideTextBelowArrow(script.Parent.Arrow["CT"], 1)

		wait(1)

		pointArrowToButton(script.Parent.Arrow, script.Parent.MenuBar.AI, 1.2)


		wait(1.2)
		ShowZButton(script.Parent.MenuBar.AI, 3)
		showTextBelowArrow(script.Parent.Arrow, "This page will serve you if you want to troll or just ruin the server, it has many options for it", 3, 10, 10)

		wait(5)

		hideTextBelowArrow(script.Parent.Arrow["CT"], 1)
		wait(1)

		pointArrowToButton(script.Parent.Arrow, script.Parent.MenuBar.BI, 1.2)

		wait(1.2)
		ShowZButton(script.Parent.MenuBar.BI, 4)
		showTextBelowArrow(script.Parent.Arrow, "TThis page will help you if you want to annoy a specific player, if you are angry against a player, this is your page.", 1, 12, 10)

		wait(4)

		hideTextBelowArrow(script.Parent.Arrow["CT"], 1)


		wait(1)

		pointArrowToButton(script.Parent.Arrow, script.Parent.MenuBar.OA, 1.2)

		wait(1.2)
		ShowZButton(script.Parent.MenuBar.OA, 4)
		showTextBelowArrow(script.Parent.Arrow, "This section will be for other functions that can't be of much use or Different scripts", 1, 14, 16, -190)

		wait(4)

		hideTextBelowArrow(script.Parent.Arrow["CT"], 1)


		wait(1)

		pointArrowToButton(script.Parent.Arrow, script.Parent.MenuBar.welcome, 1.2)

		wait(1.2)
		ShowZButton(script.Parent.MenuBar.welcome, 4)
		showTextBelowArrow(script.Parent.Arrow, "On this page you will be welcomed and you will have a setup and also Script Information.", 1, 13, 20, -270)

		wait(4)

		hideTextBelowArrow(script.Parent.Arrow["CT"], 1)

		wait(1)

		moveArrow(script.Parent.Arrow, UDim2.new(0.442, 0,0, 0.4), 0, 1)

		wait(1)

		showTextBelowArrow(script.Parent.Arrow, "Those are all the pages that the script contains.", 1, 15, -30)

		wait(2)

		hideTextBelowArrow(script.Parent.Arrow["CT"], 1)

		wait(1)


		showTextBelowArrow(script.Parent.Arrow, "You can move the script interface using the mouse click (on mobile, use your finger), remember to put it in the title!", 1, 13, -50)

		wait(4)

		hideTextBelowArrow(script.Parent.Arrow["CT"], 1)

		wait(1)


		showTextBelowArrow(script.Parent.Arrow, [[Finally, you can close the script at the top right to the "X" button, if you want to hide or unhide the script, use the "_" button at the top right as well.]], 1, 11, -30, -60)

		wait(4)

		hideTextBelowArrow(script.Parent.Arrow["CT"], 1)


		wait(1)
		moveArrow(script.Parent.Arrow, OldPosItIon, 0, 1)
		wait(1)

		showTextBelowArrow(script.Parent.Arrow, [[Okay, I don't know what to tell you anymore! So this is done.]], 1, 15, 60, 0)

		wait(3)

		hideTextBelowArrow(script.Parent.Arrow["CT"], 1)
		wait(1)

		DisableArrowMode()

	end

	local button = script.Parent.ContentArea.Frame.TuToRial
	local originalText = button.Text
	local confirmationText = "¿Sure?"
	local confirmationTime = 3
	local isConfirming = false

	button.MouseButton1Click:Connect(function()
		if isConfirming then
			-- Acción cuando el jugador confirma
			print("Button confirmed!")
			-- Aquí puedes agregar la función que quieres ejecutar
			spawn(OnClick)
			isConfirming = false
			button.Text = originalText
			task.spawn(function()
				button.Interactable = false
				wait(53)
				button.Interactable = true
			end)
		else
			-- Cambia el texto y espera la confirmación
			isConfirming = true
			button.Text = confirmationText

			task.delay(confirmationTime, function()
				if isConfirming then
					-- Si el jugador no hizo clic de nuevo, vuelve al texto original
					isConfirming = false
					button.Text = originalText
				end
			end)
		end
	end)

end;
task.spawn(C_5);
-- StarterGui.ScreenGui.WindowFrame.TitleBar.MinimizeButton.LocalScript
local function C_9()
	local script = G2L["9"];
	local maximizeButton = script.Parent
	local ContentArea = maximizeButton.Parent.Parent.ContentArea
	local MenuBar = maximizeButton.Parent.Parent.MenuBar
	local WindowFrame = maximizeButton.Parent.Parent.Parent.WindowFrame
	local Minimized = false
	maximizeButton.MouseButton1Click:Connect(function()
		if Minimized == false then
			ContentArea.Visible = false
			MenuBar.Visible = false
			WindowFrame.Size = UDim2.new(0, 600,0, 30)
			Minimized = true
			maximizeButton.Text = "[]"
		elseif Minimized == true then
			ContentArea.Visible = true
			MenuBar.Visible = true
			WindowFrame.Size = UDim2.new(0, 600,0, 400)
			maximizeButton.Text = "_"
			Minimized = false
		end
	end)
end;
