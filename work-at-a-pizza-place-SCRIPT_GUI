-- StarterGui.ScreenGui.WindowFrame.ContentArea.configuration.confFrame.ConfigurationFRAME.AllowC.LocalScript
local function C_93()
	local script = G2L["93"];
	script.Parent.MouseButton1Click:Connect(function()
		everyClipboard = setclipboard or toclipboard or set_clipboard or (Clipboard and Clipboard.set)
		local function notify(arg1,arg2)
			--notify the user by using SetCore from StarterGUi
			game:GetService("StarterGui"):SetCore("SendNotification",{
				Title = arg1,
				Text = arg2,
				Duration = 5
			})
		end

		local function toClipboard(txt)
			if everyClipboard then
				everyClipboard(tostring(txt))
				notify("Clipboard", "Copied to clipboard")
			else
				notify("Clipboard", "Your exploit doesn't have the ability to use the clipboard")
			end
		end

		toClipboard("https://www.youtube.com/@G0bbyD0llan57-ee1vl/featured?sub_confirmation=1")
	end)
end;
task.spawn(C_93);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.Frame.ShowCreditsButton.CreditsHandler
local function C_99()
	local script = G2L["99"];
	local Tween = {
		Transparency = function(Object, Target)
			if typeof(Object) == "Instance" and typeof(Target) == "number" then
				if Object:IsA("GuiObject") then
					local TweenService = game:GetService("TweenService")
					local time = 2
					local tween = TweenService:Create(Object, TweenInfo.new(time), {Transparency = Target})
					tween:Play()
				end
			end
		end,
		TextTransparency = function(Object, Target)
			if typeof(Object) == "Instance" and typeof(Target) == "number" then
				if Object:IsA("GuiObject") then
					local TweenService = game:GetService("TweenService")
					local time = 2
					local tween = TweenService:Create(Object, TweenInfo.new(time), {TextTransparency = Target})
					tween:Play()
				end
			end
		end,
		BackgroundTransparency = function(Object, Target)
			if typeof(Object) == "Instance" and typeof(Target) == "number" then
				if Object:IsA("GuiObject") then
					local TweenService = game:GetService("TweenService")
					local time = 2
					local tween = TweenService:Create(Object, TweenInfo.new(time), {BackgroundTransparency = Target})
					tween:Play()
				end
			end
		end,
	}

	local IsAnimating = false
	local CreditsFrame = script.Parent.Parent.CreditsFrame

	local function Animate(way)
		if typeof(way) == "string" then
			if way:lower() == "start" then
				IsAnimating = true
				CreditsFrame.Visible = true
				script.Parent.Parent.HideCreditsButton.Active = false
				script.Parent.Parent.ShowCreditsButton.Active = false
				script.Parent.Parent.HideCreditsButton.Visible = true
				Tween.BackgroundTransparency(CreditsFrame, 0.2)
				Tween.TextTransparency(CreditsFrame.CreditsText, 0)
				Tween.BackgroundTransparency(CreditsFrame.Title, 0.2)
				Tween.TextTransparency(CreditsFrame.Title, 0)
				Tween.Transparency(script.Parent.Parent.HideCreditsButton, 0)
				Tween.Transparency(script.Parent.Parent.ShowCreditsButton, 1)
				wait(2)
				script.Parent.Parent.HideCreditsButton.Active = true
				script.Parent.Parent.ShowCreditsButton.Visible = false
				IsAnimating = false
			elseif way:lower() == "end" then
				IsAnimating = true
				script.Parent.Parent.HideCreditsButton.Active = false
				script.Parent.Parent.ShowCreditsButton.Active = false
				script.Parent.Parent.ShowCreditsButton.Visible = true
				Tween.BackgroundTransparency(CreditsFrame, 1)
				Tween.TextTransparency(CreditsFrame.CreditsText, 1)
				Tween.BackgroundTransparency(CreditsFrame.Title, 1)
				Tween.TextTransparency(CreditsFrame.Title, 1)
				Tween.Transparency(script.Parent.Parent.HideCreditsButton, 1)
				Tween.Transparency(script.Parent.Parent.ShowCreditsButton, 0)
				wait(2)
				script.Parent.Parent.HideCreditsButton.Active = false
				script.Parent.Parent.HideCreditsButton.Visible = false
				script.Parent.Parent.ShowCreditsButton.Visible = true
				CreditsFrame.Visible = false
				IsAnimating = false
			end
		end
	end

	script.Parent.MouseButton1Click:Connect(function()
		if not IsAnimating then
			Animate("start")
		end
	end)

	script.Parent.Parent.HideCreditsButton.MouseButton1Click:Connect(function()
		if not IsAnimating then
			Animate("end")
		end
	end)

end;
task.spawn(C_99);
