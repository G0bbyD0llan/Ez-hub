-- StarterGui.ScreenGui.WindowFrame.ContentArea.target.Frame.ActionButtonHOUSE.LocalScript
local function C_2b()
	local script = G2L["2b"];
	local button = script.Parent
	local originalText = button.Text
	local toggle_active = false

	local function getPlayer()
		local textbox = button.Parent.Parent:FindFirstChild("PlayerNameTextBox")
		if textbox then
			return game.Players:FindFirstChild(textbox.Text)
		end
		return nil
	end

	button.MouseButton1Click:Connect(function()
		toggle_active = not toggle_active

		if toggle_active then
			button.Text = "Throwing Snowballs..."
		else
			button.Text = originalText
		end

		while toggle_active do
			local player = getPlayer()
			if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
				local targetPosition = player.Character.HumanoidRootPart.Position
				local snowballTool = game:GetService("Players").LocalPlayer.Character:FindFirstChild("SnowBall")

				if snowballTool then
					snowballTool.Clicked:FireServer(targetPosition)
				end
			end
			task.wait(0.5) -- Espera para evitar colapsar el juego
		end
	end)

end;
task.spawn(C_2b);
