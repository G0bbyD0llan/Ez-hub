-- StarterGui.ScreenGui.WindowFrame.ContentArea.misc.MyButton.LocalScript
local function C_58()
	local script = G2L["58"];
	local button = script.Parent -- Botón que sirve para activar/desactivar el toggle
	local originalText = button.Text -- Guardamos el texto original del botón
	local isToggled = false -- Estado del toggle (desactivado por defecto)

	-- Función para alternar el estado del toggle
	local function toggleKickTeleport()
		isToggled = not isToggled
		if isToggled then
			button.Text = "Anti kick: ON" -- Cambiamos el texto para reflejar que está activado
		else
			button.Text = "Anti kick: OFF" -- Cambiamos el texto para reflejar que está desactivado
		end
		-- La función que verifica si el jugador es "kickeado"
		while isToggled do
			wait(1)
			if game.Players.LocalPlayer.PlayerGui.MainGui.Prompts.Ban.KickPlayer.Text == game.Players.LocalPlayer.Name then
				local player = game.Players.LocalPlayer
				local placeId = game.PlaceId
				local serverId = game.JobId
				game:GetService("TeleportService"):TeleportToPlaceInstance(placeId, serverId, player)
			end
		end

	end

	-- Conectamos la acción al hacer clic en el botón
	button.MouseButton1Click:Connect(toggleKickTeleport)
end;
task.spawn(C_58);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.misc.MyButton.LocalScript
local function C_5a()
	local script = G2L["5a"];
	script.Parent.MouseButton1Click:Connect(function()
		local oldpos = game.Players.LocalPlayer.Character.PrimaryPart.Position
		wait(0.2)
		game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(43, 5, 7)
		wait(0.2)
		game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(42, 5, 7)
		wait(0.2)
		game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(43, 5, 7)
		wait(0.2)
		game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(51, 5, 7)
		wait(0.2)
		game:GetService("Players").LocalPlayer.Character:MoveTo(oldpos)
	end)
end;
task.spawn(C_5a);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.misc.MyButton.LocalScript
local function C_5c()
	local script = G2L["5c"];
	local button = script.Parent
	local colors = {} -- Lista de colores
	local currentIndex = 1 -- Índice para cambiar el color en orden

	-- Llenamos la lista con todos los colores de BrickColor
	for _, color in ipairs(BrickColor.palette()) do
		table.insert(colors, BrickColor.new(color))
	end

	local function PaintCar(Car, Color_Code)
		-- Pinta cada parte del coche enviando el evento remoto
		for _, Child in ipairs(Car:GetChildren()) do
			game:GetService("ReplicatedStorage").Channels.VehicleChannel:FireServer('Paint', Child, 'None', Color_Code)
		end
	end

	local function PaintAllCars()
		-- Obtiene el color actual por índice
		local selectedColor = colors[currentIndex]

		-- Pinta todos los autos en el juego
		for _, Car in ipairs(game:GetService("Workspace").Cars:GetChildren()) do
			PaintCar(Car, selectedColor.Number)
		end

		-- Avanza el índice, y si llega al final, vuelve al inicio
		currentIndex = currentIndex + 1
		if currentIndex > #colors then
			currentIndex = 1
		end
	end

	-- Conectar el botón para ejecutar la pintura en orden
	button.MouseButton1Click:Connect(PaintAllCars)

end;
task.spawn(C_5c);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.misc.MyButton.LocalScript
local function C_5e()
	local script = G2L["5e"];
	script.Parent.MouseButton1Click:Connect(function()
		for i, v in pairs(game:GetService("Workspace").AllDough:GetChildren()) do
			game:GetService("Workspace").GameService.AddIngredientToPizza:FireServer(v, "Sausage")
			game:GetService("Workspace").GameService.AddIngredientToPizza:FireServer(v, "Cheese")
			game:GetService("Workspace").GameService.AddIngredientToPizza:FireServer(v, "Pepperoni")
			game:GetService("Workspace").GameService.AddIngredientToPizza:FireServer(v, "TomatoSauce")
		end
	end)
end;
task.spawn(C_5e);
-- StarterGui.ScreenGui.WindowFrame.DraggableScript
local function C_67()
	local script = G2L["67"];
	--Variables
	local TitleBar = script.Parent["TitleBar"]
	local IsCursonOnTitleBar = false
	--MouseEnter and MouseLeave Connection
	TitleBar.MouseEnter:Connect(function()
		if IsCursonOnTitleBar == false then
			IsCursonOnTitleBar = true
		end
	end)
	TitleBar.MouseLeave:Connect(function()
		if IsCursonOnTitleBar == true then
			IsCursonOnTitleBar = false
		end
	end)
	--Making the draggable gui
	local UserInputService = game:GetService("UserInputService")

	local gui = script.Parent

	local dragging
	local dragInput
	local dragStart
	local startPos

	local function update(input)
		if IsCursonOnTitleBar then
			local delta = input.Position - dragStart
			gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end
	end

	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position

			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)

	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)

	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end;
task.spawn(C_67);
