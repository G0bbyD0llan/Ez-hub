-- StarterGui.ScreenGui.WindowFrame.ContentArea.misc.MyButton.LocalScript
local function C_5e()
	local script = G2L["5e"];
	local button = script.Parent
	local frame = script.Parent.Parent
	local OLDTEXT = "Infinite yield"
	local scriptname = OLDTEXT

	-- Añadir efectos de hover y clic
	local TweenService = game:GetService("TweenService")

	-- Definir colores y tiempos de transición
	local hoverColor = Color3.fromRGB(70, 70, 70)
	local clickColor = Color3.fromRGB(30, 30, 30)
	local defaultColor = Color3.fromRGB(3, 0, 50)
	local textChangeDuration = 0.5

	-- Función para crear transiciones
	local function createTween(instance, properties, duration)
		local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
		local tween = TweenService:Create(instance, tweenInfo, properties)
		tween:Play()
		return tween
	end
	local activeTween = false
	-- Eventos de hover
	button.MouseEnter:Connect(function()
		createTween(button, {BackgroundColor3 = hoverColor}, 0.2)
	end)

	button.MouseLeave:Connect(function()
		createTween(button, {BackgroundColor3 = defaultColor}, 0.2)
	end)

	button.MouseButton1Down:Connect(function()
		createTween(button, {BackgroundColor3 = clickColor}, 0.1)
	end)

	button.MouseButton1Up:Connect(function()
		createTween(button, {BackgroundColor3 = hoverColor}, 0.1)
	end)

	-- Evento de clic para ejecutar el script
	button.MouseButton1Click:Connect(function()
		if activeTween then return end
		local runningText = "Running "..scriptname
		activeTween = true
		-- Cambiar texto con transición
		createTween(button, {TextTransparency = 1}, textChangeDuration).Completed:Connect(function()
			button.Text = runningText
			createTween(button, {TextTransparency = 0}, textChangeDuration)
		end)

		task.spawn(function()
			loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
		end)

		wait(2)

		createTween(button, {TextTransparency = 1}, textChangeDuration).Completed:Connect(function()
			button.Text = OLDTEXT
			createTween(button, {TextTransparency = 0}, textChangeDuration)
		end)
		wait(1)
		activeTween = false

	end)

end;
task.spawn(C_5e);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.misc.MyButton.LocalScript
local function C_60()
	local script = G2L["60"];
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	local root = player.Character:WaitForChild("HumanoidRootPart")

	local orbiting = false
	local orbitThread

	local remote
	do
		local reg = (getreg or debug.getregistry)()
		for i=1,#reg do
			local f = reg[i]
			if type(f)=="table" and rawget(f,"FireServer") and rawget(f,"BindEvents") then
				remote = f
			end
		end
	end
	function moveThing(bmd, location)
		remote:FireServer("UpdateProperty", bmd, "CFrame", location)
		wait()
		remote:FireServer("SquishDough", bmd)
	end
	-- Función que corre la órbita
	local function orbitDews(radius, speed)
		orbitThread = task.spawn(function()
			while orbiting do
				task.wait()
				local dews = workspace.AllMountainDew:GetChildren()
				local n = #dews
				if n == 0 then continue end

				local t = tick() * speed

				for i, v in ipairs(dews) do
					if v:IsA("BasePart") then
						-- Ángulo de este Dew
						local angle = (i / n) * math.pi * 2 + t

						-- Nueva posición
						local x = root.Position.X + math.cos(angle) * radius
						local z = root.Position.Z + math.sin(angle) * radius
						local y = root.Position.Y + 2 -- un poco arriba del jugador

						moveThing(v, CFrame.new(x, y, z))
					end
				end
			end
		end)
	end

	-- Función toggle
	local function toggleOrbit(radius, speed)
		orbiting = not orbiting
		if orbiting then
			print("✅ Órbita activada")
			orbitDews(radius or 10, speed or 1)
		else
			print("❌ Órbita desactivada")
		end
	end

	local oldtext = script.Parent.Text
	script.Parent.MouseButton1Click:Connect(function()
		toggleOrbit(10, 1)
		if orbiting then
			script.Parent.Text = "STOP"
		else
			script.Parent.Text = oldtext
		end
	end)
end;
task.spawn(C_60);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.misc.MyButton.LocalScript
local function C_62()
	local script = G2L["62"];
	local button = script.Parent
	local frame = script.Parent.Parent
	local OLDTEXT = "Infinite yield"
	local scriptname = OLDTEXT

	-- Añadir efectos de hover y clic
	local TweenService = game:GetService("TweenService")

	-- Definir colores y tiempos de transición
	local hoverColor = Color3.fromRGB(70, 70, 70)
	local clickColor = Color3.fromRGB(30, 30, 30)
	local defaultColor = Color3.fromRGB(3, 0, 50)
	local textChangeDuration = 0.5

	-- Función para crear transiciones
	local function createTween(instance, properties, duration)
		local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
		local tween = TweenService:Create(instance, tweenInfo, properties)
		tween:Play()
		return tween
	end
	local activeTween = false
	-- Eventos de hover
	button.MouseEnter:Connect(function()
		createTween(button, {BackgroundColor3 = hoverColor}, 0.2)
	end)

	button.MouseLeave:Connect(function()
		createTween(button, {BackgroundColor3 = defaultColor}, 0.2)
	end)

	button.MouseButton1Down:Connect(function()
		createTween(button, {BackgroundColor3 = clickColor}, 0.1)
	end)

	button.MouseButton1Up:Connect(function()
		createTween(button, {BackgroundColor3 = hoverColor}, 0.1)
	end)

	-- Evento de clic para ejecutar el script
	button.MouseButton1Click:Connect(function()
		if activeTween then return end
		local runningText = "Running "..scriptname
		activeTween = true
		-- Cambiar texto con transición
		createTween(button, {TextTransparency = 1}, textChangeDuration).Completed:Connect(function()
			button.Text = runningText
			createTween(button, {TextTransparency = 0}, textChangeDuration)
		end)

		task.spawn(function()
			loadstring(game:HttpGet("https://raw.githubusercontent.com/FilteringEnabled/NamelessAdmin/main/Source"))();
		end)

		wait(2)

		createTween(button, {TextTransparency = 1}, textChangeDuration).Completed:Connect(function()
			button.Text = OLDTEXT
			createTween(button, {TextTransparency = 0}, textChangeDuration)
		end)
		wait(1)
		activeTween = false

	end)

end;
task.spawn(C_62);
