			loadstring(game:HttpGet("https://raw.githubusercontent.com/G0bbyD0llan/Ez-hub/refs/heads/main/work-at-a-pizza-place-SCRIPT_GUI"))()
		end)
		script.Parent.Parent.Parent.Parent.Parent.Parent.Parent:Destroy()
	end)
end;
task.spawn(C_93);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.configuration.confFrame.ConfigurationFRAME.AllowC.LocalScript
local function C_96()
	local script = G2L["96"];
	script.Parent.MouseButton1Click:Connect(function()
		everyClipboard = setclipboard or toclipboard or set_clipboard or (Clipboard and Clipboard.set)
		local function notify(arg1,arg2)
			--notify the user by using SetCore from StarterGUi
			game:GetService("StarterGui"):SetCore("SendNotification",{
				Title = arg1,
				Text = arg2,
				Duration = 5
			})
		end

		local function toClipboard(txt)
			if everyClipboard then
				everyClipboard(tostring(txt))
				notify("Clipboard", "Copied to clipboard")
			else
				notify("Clipboard", "Your exploit doesn't have the ability to use the clipboard")
			end
		end

		toClipboard("https://www.youtube.com/@G0bbyD0llan57-ee1vl/featured?sub_confirmation=1")
	end)
end;
task.spawn(C_96);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.configuration.confFrame.ConfigurationFRAME.ReSiZeTool.LocalScript
local function C_98()
	local script = G2L["98"];
	local option_page = script.Parent.Parent
	local IsReSiZeToolActive = false
	option_page.ReSiZeTool.MouseButton1Click:Connect(function()
		IsReSiZeToolActive = not IsReSiZeToolActive
		if IsReSiZeToolActive then
			option_page.ReSiZeTool.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
			option_page.ReSiZeTool.Text = "yes"
			if script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.WindowFrame:FindFirstChild("ResizeGrip") then
				script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.WindowFrame.ResizeGrip.Visible = true
			else
				task.spawn(function()
					local UserInputService = game:GetService("UserInputService")

					local screenGui = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent
					local mainWindow = screenGui:WaitForChild("WindowFrame")

					-- Original MIN_WIDTH and MIN_HEIGHT in pixels
					local MIN_WIDTH_PIXELS = 150
					local MIN_HEIGHT_PIXELS = 100

					-- Initialize UIScale and determine designAbsoluteSize (size at Scale = 1)
					local windowScaler = mainWindow:FindFirstChild("MainWindowScaler")
					local designAbsoluteSize

					if windowScaler and windowScaler:IsA("UIScale") then
						if windowScaler.Scale ~= 0 then
							designAbsoluteSize = mainWindow.AbsoluteSize / windowScaler.Scale
						else
							warn("ResizeMainWindow: MainWindowScaler had a scale of 0. Resetting to 1.")
							windowScaler.Scale = 1
							designAbsoluteSize = mainWindow.AbsoluteSize -- mainWindow.AbsoluteSize is now size at scale 1
						end
					else
						windowScaler = Instance.new("UIScale")
						windowScaler.Name = "MainWindowScaler"
						windowScaler.Parent = mainWindow
						-- UIScale defaults to Scale = 1, so AbsoluteSize is already the size at scale 1
						designAbsoluteSize = mainWindow.AbsoluteSize
					end

					-- Calculate minimum allowed scale based on pixel minimums and design size
					local ACTUAL_MIN_SCALE
					if designAbsoluteSize.X > 0 and designAbsoluteSize.Y > 0 then
						local minScaleX = MIN_WIDTH_PIXELS / designAbsoluteSize.X
						local minScaleY = MIN_HEIGHT_PIXELS / designAbsoluteSize.Y
						ACTUAL_MIN_SCALE = math.max(minScaleX, minScaleY)
					else
						-- Fallback if designAbsoluteSize is zero (should not happen for a visible window)
						warn("ResizeMainWindow: designAbsoluteSize is zero. Using a default min scale.")
						ACTUAL_MIN_SCALE = 0.1 
					end
					-- Ensure ACTUAL_MIN_SCALE is at least a very small positive number
					ACTUAL_MIN_SCALE = math.max(ACTUAL_MIN_SCALE, 0.05) -- Absolute smallest scale

					-- Create Resize Grip (if it doesn't exist, or ensure it's configured)
					local resizeGrip = mainWindow:FindFirstChild("ResizeGrip")
					if not resizeGrip or not resizeGrip:IsA("TextButton") then
						if resizeGrip then resizeGrip:Destroy() end -- Remove if wrong type
						resizeGrip = Instance.new("TextButton")
						resizeGrip.Name = "ResizeGrip"
						resizeGrip.Text = "" 
						resizeGrip.Size = UDim2.new(0, 20, 0, 20) 
						resizeGrip.AnchorPoint = Vector2.new(1, 1) 
						resizeGrip.Position = UDim2.new(1, 0, 1, 0) 
						resizeGrip.BackgroundColor3 = Color3.fromRGB(120, 120, 120)
						resizeGrip.BorderColor3 = Color3.fromRGB(80, 80, 80)
						resizeGrip.BorderSizePixel = 1
						resizeGrip.AutoButtonColor = false
						resizeGrip.ZIndex = (mainWindow.ZIndex or 1) + 10 -- Ensure it's on top
						resizeGrip.Parent = mainWindow
					end


					local isResizing = false
					local initialMousePosition = nil
					local initialScalerValue = nil

					resizeGrip.InputBegan:Connect(function(input)
						if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
							if designAbsoluteSize.X == 0 or designAbsoluteSize.Y == 0 then
								warn("ResizeMainWindow: Cannot start resize, designAbsoluteSize is zero.")
								-- Re-evaluate designAbsoluteSize in case it became valid
								if windowScaler.Scale ~= 0 then
									designAbsoluteSize = mainWindow.AbsoluteSize / windowScaler.Scale
								else
									windowScaler.Scale = 1
									designAbsoluteSize = mainWindow.AbsoluteSize
								end
								if designAbsoluteSize.X == 0 or designAbsoluteSize.Y == 0 then
									return -- Still zero, cannot proceed
								end
							end

							isResizing = true
							initialMousePosition = UserInputService:GetMouseLocation()
							initialScalerValue = windowScaler.Scale
						end
					end)

					UserInputService.InputEnded:Connect(function(input)
						if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
							if isResizing then
								isResizing = false
								initialMousePosition = nil
								initialScalerValue = nil
							end
						end
					end)

					UserInputService.InputChanged:Connect(function(input)
						if not isResizing or not initialMousePosition or not initialScalerValue then
							return
						end

						if designAbsoluteSize.X == 0 or designAbsoluteSize.Y == 0 then
							-- This check is important if the window was somehow initialized to zero size
							-- and InputBegan couldn't get a valid designAbsoluteSize.
							warn("ResizeMainWindow: Cannot resize, designAbsoluteSize is zero during InputChanged.")
							isResizing = false -- Stop resizing attempt
							return
						end

						if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
							local currentMousePosition = UserInputService:GetMouseLocation()
							local delta = currentMousePosition - initialMousePosition

							-- Calculate the absolute size the window had at the start of the drag
							local effectiveWidthAtDragStart = designAbsoluteSize.X * initialScalerValue
							local effectiveHeightAtDragStart = designAbsoluteSize.Y * initialScalerValue

							-- Calculate the new target absolute size based on mouse delta
							local targetAbsoluteWidth = effectiveWidthAtDragStart + delta.X
							local targetAbsoluteHeight = effectiveHeightAtDragStart + delta.Y

							-- Calculate the new scale required to achieve these target absolute sizes,
							-- based on the designAbsoluteSize (size at scale = 1)
							local newScaleX = targetAbsoluteWidth / designAbsoluteSize.X
							-- local newScaleY = targetAbsoluteHeight / designAbsoluteSize.Y -- Kept for reference if aspect ratio logic is desired

							-- For UIScale, we use a single scale value. Let's use the X-axis driven scale.
							-- More complex logic could be used here to maintain aspect ratio or average scales.
							local finalNewScale = newScaleX

							-- Apply minimum scale
							finalNewScale = math.max(ACTUAL_MIN_SCALE, finalNewScale)

							windowScaler.Scale = finalNewScale
						end
					end)
				end)
			end
		else
			script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.WindowFrame:FindFirstChild("ResizeGrip").Visible = false
			option_page.ReSiZeTool.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
			option_page.ReSiZeTool.Text = "no"
		end
	end)

end;
task.spawn(C_98);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.Frame.ShowCreditsButton.CreditsHandler
local function C_9f()
	local script = G2L["9f"];
	local Tween = {
		Transparency = function(Object, Target)
			if typeof(Object) == "Instance" and typeof(Target) == "number" then
				if Object:IsA("GuiObject") then
					local TweenService = game:GetService("TweenService")
					local time = 2
					local tween = TweenService:Create(Object, TweenInfo.new(time), {Transparency = Target})
					tween:Play()
				end
			end
		end,
		TextTransparency = function(Object, Target)
			if typeof(Object) == "Instance" and typeof(Target) == "number" then
				if Object:IsA("GuiObject") then
					local TweenService = game:GetService("TweenService")
					local time = 2
					local tween = TweenService:Create(Object, TweenInfo.new(time), {TextTransparency = Target})
					tween:Play()
				end
			end
		end,
		BackgroundTransparency = function(Object, Target)
			if typeof(Object) == "Instance" and typeof(Target) == "number" then
				if Object:IsA("GuiObject") then
					local TweenService = game:GetService("TweenService")
					local time = 2
					local tween = TweenService:Create(Object, TweenInfo.new(time), {BackgroundTransparency = Target})
					tween:Play()
				end
			end
		end,
	}

	local IsAnimating = false
	local CreditsFrame = script.Parent.Parent.CreditsFrame

	local function Animate(way)
		if typeof(way) == "string" then
			if way:lower() == "start" then
				IsAnimating = true
				CreditsFrame.Visible = true
				script.Parent.Parent.HideCreditsButton.Active = false
				script.Parent.Parent.ShowCreditsButton.Active = false
				script.Parent.Parent.HideCreditsButton.Visible = true
				Tween.BackgroundTransparency(CreditsFrame, 0.2)
				Tween.TextTransparency(CreditsFrame.CreditsText, 0)
				Tween.BackgroundTransparency(CreditsFrame.Title, 0.2)
				Tween.TextTransparency(CreditsFrame.Title, 0)
				Tween.Transparency(script.Parent.Parent.HideCreditsButton, 0)
				Tween.Transparency(script.Parent.Parent.ShowCreditsButton, 1)
				wait(2)
				script.Parent.Parent.HideCreditsButton.Active = true
				script.Parent.Parent.ShowCreditsButton.Visible = false
				IsAnimating = false
			elseif way:lower() == "end" then
				IsAnimating = true
				script.Parent.Parent.HideCreditsButton.Active = false
				script.Parent.Parent.ShowCreditsButton.Active = false
				script.Parent.Parent.ShowCreditsButton.Visible = true
				Tween.BackgroundTransparency(CreditsFrame, 1)
				Tween.TextTransparency(CreditsFrame.CreditsText, 1)
				Tween.BackgroundTransparency(CreditsFrame.Title, 1)
				Tween.TextTransparency(CreditsFrame.Title, 1)
				Tween.Transparency(script.Parent.Parent.HideCreditsButton, 1)
				Tween.Transparency(script.Parent.Parent.ShowCreditsButton, 0)
				wait(2)
				script.Parent.Parent.HideCreditsButton.Active = false
				script.Parent.Parent.HideCreditsButton.Visible = false
				script.Parent.Parent.ShowCreditsButton.Visible = true
				CreditsFrame.Visible = false
				IsAnimating = false
			end
		end
	end

	script.Parent.MouseButton1Click:Connect(function()
		if not IsAnimating then
			Animate("start")
		end
	end)

	script.Parent.Parent.HideCreditsButton.MouseButton1Click:Connect(function()
		if not IsAnimating then
			Animate("end")
		end
	end)

end;
task.spawn(C_9f);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.Frame.SettingsButton.LocalScript
local function C_a8()
	local script = G2L["a8"];
	local imageButton = script.Parent
	imageButton.MouseEnter:Connect(function()
		imageButton.ImageColor3 = Color3.fromRGB(200, 200, 200) -- Cambiar color al pasar el mouse
	end)

	imageButton.MouseLeave:Connect(function()
		imageButton.ImageColor3 = Color3.fromRGB(255, 255, 255) -- Restaurar color predeterminado al retirar el mouse
	end)
	imageButton.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Frame.Visible = false
		script.Parent.Parent.Parent.configuration.Visible = true
	end)
end;
task.spawn(C_a8);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.Frame.Message.LocalScript
local function C_ab()
	local script = G2L["ab"];
	repeat wait() until script.Parent.Parent.Parent.Parent.Transparency == 1

	local TextLabel = script.Parent -- Asegúrate de colocar este script dentro del TextLabel
	local pastebinUrl = "PASTEBIN_RAW_URL" -- Reemplaza con tu enlace raw de Pastebin

	local version, lastMessage = "Unknown", "No Message"

	local success, result = pcall(function()
		return game:HttpGet(pastebinUrl)
	end)

	if success then
		local lines = result:split("\n")
		version = lines[1] or "Unknown"
		lastMessage = lines[2] or "No Message"
	end

	local function updateText()
		while true do
			local currentTime = os.date("%X")
			TextLabel.Text = string.format("Time = %s\nVersion = %s\nCreator Last Message: %s", currentTime, version, lastMessage)
			wait(1) -- Espera 1 segundo antes de la siguiente actualización
		end
	end

	local function FadeIn()
		wait(3.3)
		script.Parent.TextTransparency = 0
		wait(0.3)
		script.Parent.TextTransparency = 1
		wait(0.3)
		script.Parent.TextTransparency = 0
		wait(0.3)
		script.Parent.TextTransparency = 1
		wait(0.3)
		script.Parent.TextTransparency = 0
	end

	spawn(updateText)
	spawn(FadeIn)
end;
task.spawn(C_ab);
-- StarterGui.ScreenGui.WindowFrame.CloseWarning.Frame.no.LocalScript
