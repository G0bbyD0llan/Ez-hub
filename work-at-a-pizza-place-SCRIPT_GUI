	end)

	button.MouseLeave:Connect(function()
		createTween(button, {BackgroundColor3 = defaultColor}, 0.2)
	end)

	button.MouseButton1Down:Connect(function()
		createTween(button, {BackgroundColor3 = clickColor}, 0.1)
	end)

	button.MouseButton1Up:Connect(function()
		createTween(button, {BackgroundColor3 = hoverColor}, 0.1)
	end)

	-- Evento de clic para ejecutar el script
	button.MouseButton1Click:Connect(function()
		if activeTween then return end
		local runningText = "Running "..scriptname
		activeTween = true
		-- Cambiar texto con transición
		createTween(button, {TextTransparency = 1}, textChangeDuration).Completed:Connect(function()
			button.Text = runningText
			createTween(button, {TextTransparency = 0}, textChangeDuration)
		end)

		task.spawn(function()
			loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
		end)

		wait(2)

		createTween(button, {TextTransparency = 1}, textChangeDuration).Completed:Connect(function()
			button.Text = OLDTEXT
			createTween(button, {TextTransparency = 0}, textChangeDuration)
		end)
		wait(1)
		activeTween = false

	end)

end;
