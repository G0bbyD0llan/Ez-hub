-- StarterGui.ScreenGui.WindowFrame.ContentArea.trolling.Frame.ActionButtonMCH.LocalScript
local function C_4a()
	local script = G2L["4a"];
	local player = game.Players.LocalPlayer
	local house
	local x = 0
	local houseSize
	-- Encuentra la casa del jugador local
	for _, h in pairs(game:GetService("Workspace").Houses:GetChildren()) do
		if h:FindFirstChild(player.DisplayName) then
			house = h
			local houseChild = h:FindFirstChild("Large") or h:FindFirstChild("TwoStory") or h:FindFirstChild("Medium") or h:FindFirstChild("Tiny") or h:FindFirstChild("Shack")
			if houseChild then
				print("size: "..houseChild.Name)
				houseSize = houseChild.Name
			else
				print("size: Default (Tiny)")
				houseSize = "Tiny"
			end
			break
		end
	end
	-- Función para cambiar el color de la casa
	local function ChangeHouseColor(color)
		for _, wall in ipairs(house[houseSize]:GetChildren()) do
			if wall.Name:lower() == "wall" then
				local args = {
					[1] = "PaintHouse",
					[2] = wall, -- Asegúrate de que "Tiny" y "Wall" sean los nombres correctos de las partes de la casa
					[3] = "None", -- No cambies esto si no estás seguro de qué hace
					[4] = color -- El nuevo color en Color3
				}
				game:GetService("ReplicatedStorage").HouseChannel:FireServer(unpack(args))
			end

		end
	end
	local loopcolor = false
	script.Parent.MouseButton1Click:Connect(function()
		loopcolor = not loopcolor
		if loopcolor then
			script.Parent.Text = "un MULTICOLOR HOUSE"
		else
			script.Parent.Text = "MULTICOLOR HOUSE"
		end
		while loopcolor do
			ChangeHouseColor(Color3.fromHSV(x, 1, 1)) -- Cambia el color de la casa usando la función
			x = x + 2/255 -- Incremento del color
			if x >= 1 then
				x = 0
			end
			wait() -- Espera para mantener el bucle suave
		end
	end)


	while wait() do
		script.Parent.BackgroundColor3 = Color3.fromHSV(tick()%5/5,1,1)
	end
end;
task.spawn(C_4a);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.trolling.Frame.ActionButtonLOV.LocalScript
local function C_4c()
	local script = G2L["4c"];
	script.Parent.MouseButton1Click:Connect(function()
		local SkidFling = function(TargetPlayer)
			local Character = game.Players.LocalPlayer.Character
			local Humanoid = Character and Character:FindFirstChildOfClass("Humanoid")
			local RootPart = Humanoid and Humanoid.RootPart

			local TCharacter = TargetPlayer.Character
			local THumanoid
			local TRootPart
			local THead
			local Accessory
			local Handle

			if TCharacter:FindFirstChildOfClass("Humanoid") then
				THumanoid = TCharacter:FindFirstChildOfClass("Humanoid")
			end
			if THumanoid and THumanoid.RootPart then
				TRootPart = THumanoid.RootPart
			end
			if TCharacter:FindFirstChild("Head") then
				THead = TCharacter.Head
			end
			if TCharacter:FindFirstChildOfClass("Accessory") then
				Accessory = TCharacter:FindFirstChildOfClass("Accessory")
			end
			if Accessoy and Accessory:FindFirstChild("Handle") then
				Handle = Accessory.Handle
			end

			if Character and Humanoid and RootPart then
				if RootPart.Velocity.Magnitude < 50 then
					getgenv().OldPos = RootPart.CFrame
				end
				if THumanoid and THumanoid.Sit and not AllBool then
				end
				if THead then
					workspace.CurrentCamera.CameraSubject = THead
				elseif not THead and Handle then
					workspace.CurrentCamera.CameraSubject = Handle
				elseif THumanoid and TRootPart then
					workspace.CurrentCamera.CameraSubject = THumanoid
				end
				if not TCharacter:FindFirstChildWhichIsA("BasePart") then
					return
				end

				local FPos = function(BasePart, Pos, Ang)
					RootPart.CFrame = CFrame.new(BasePart.Position) * Pos * Ang
					Character:SetPrimaryPartCFrame(CFrame.new(BasePart.Position) * Pos * Ang)
					RootPart.Velocity = Vector3.new(9e7, 9e7 * 10, 9e7)
					RootPart.RotVelocity = Vector3.new(9e8, 9e8, 9e8)
				end

				local SFBasePart = function(BasePart)
					local TimeToWait = 2
					local Time = tick()
					local Angle = 0

					repeat
						if RootPart and THumanoid then
							if BasePart.Velocity.Magnitude < 50 then
								Angle = Angle + 100

								FPos(BasePart, CFrame.new(0, 1.5, 0) + THumanoid.MoveDirection * BasePart.Velocity.Magnitude / 1.25, CFrame.Angles(math.rad(Angle),0 ,0))
								task.wait()

								FPos(BasePart, CFrame.new(0, -1.5, 0) + THumanoid.MoveDirection * BasePart.Velocity.Magnitude / 1.25, CFrame.Angles(math.rad(Angle), 0, 0))
								task.wait()

								FPos(BasePart, CFrame.new(2.25, 1.5, -2.25) + THumanoid.MoveDirection * BasePart.Velocity.Magnitude / 1.25, CFrame.Angles(math.rad(Angle), 0, 0))
								task.wait()

								FPos(BasePart, CFrame.new(-2.25, -1.5, 2.25) + THumanoid.MoveDirection * BasePart.Velocity.Magnitude / 1.25, CFrame.Angles(math.rad(Angle), 0, 0))
								task.wait()

								FPos(BasePart, CFrame.new(0, 1.5, 0) + THumanoid.MoveDirection,CFrame.Angles(math.rad(Angle), 0, 0))
								task.wait()

								FPos(BasePart, CFrame.new(0, -1.5, 0) + THumanoid.MoveDirection,CFrame.Angles(math.rad(Angle), 0, 0))
								task.wait()
							else
								FPos(BasePart, CFrame.new(0, 1.5, THumanoid.WalkSpeed), CFrame.Angles(math.rad(90), 0, 0))
								task.wait()

								FPos(BasePart, CFrame.new(0, -1.5, -THumanoid.WalkSpeed), CFrame.Angles(0, 0, 0))
								task.wait()

								FPos(BasePart, CFrame.new(0, 1.5, THumanoid.WalkSpeed), CFrame.Angles(math.rad(90), 0, 0))
								task.wait()

								FPos(BasePart, CFrame.new(0, 1.5, TRootPart.Velocity.Magnitude / 1.25), CFrame.Angles(math.rad(90), 0, 0))
								task.wait()

								FPos(BasePart, CFrame.new(0, -1.5, -TRootPart.Velocity.Magnitude / 1.25), CFrame.Angles(0, 0, 0))
								task.wait()

								FPos(BasePart, CFrame.new(0, 1.5, TRootPart.Velocity.Magnitude / 1.25), CFrame.Angles(math.rad(90), 0, 0))
								task.wait()

								FPos(BasePart, CFrame.new(0, -1.5, 0), CFrame.Angles(math.rad(90), 0, 0))
								task.wait()

								FPos(BasePart, CFrame.new(0, -1.5, 0), CFrame.Angles(0, 0, 0))
								task.wait()

								FPos(BasePart, CFrame.new(0, -1.5 ,0), CFrame.Angles(math.rad(-90), 0, 0))
								task.wait()

								FPos(BasePart, CFrame.new(0, -1.5, 0), CFrame.Angles(0, 0, 0))
								task.wait()
							end
						else
							break
						end
					until BasePart.Velocity.Magnitude > 500 or BasePart.Parent ~= TargetPlayer.Character or TargetPlayer.Parent ~= Players or not TargetPlayer.Character == TCharacter or THumanoid.Sit or Humanoid.Health <= 0 or tick() > Time + TimeToWait
				end

				workspace.FallenPartsDestroyHeight = 0/0

				local BV = Instance.new("BodyVelocity")
				BV.Name = "EpixVel"
				BV.Parent = RootPart
				BV.Velocity = Vector3.new(9e8, 9e8, 9e8)
				BV.MaxForce = Vector3.new(1/0, 1/0, 1/0)

				Humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, false)

				if TRootPart and THead then
					if (TRootPart.CFrame.p - THead.CFrame.p).Magnitude > 5 then
						SFBasePart(THead)
					else
						SFBasePart(TRootPart)
					end
				elseif TRootPart and not THead then
					SFBasePart(TRootPart)
				elseif not TRootPart and THead then
					SFBasePart(THead)
				elseif not TRootPart and not THead and Accessory and Handle then
					SFBasePart(Handle)
				else
				end

				BV:Destroy()
				Humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, true)
				workspace.CurrentCamera.CameraSubject = Humanoid

				repeat
					RootPart.CFrame = getgenv().OldPos * CFrame.new(0, .5, 0)
					Character:SetPrimaryPartCFrame(getgenv().OldPos * CFrame.new(0, .5, 0))
					Humanoid:ChangeState("GettingUp")
					table.foreach(Character:GetChildren(), function(_, x)
						if x:IsA("BasePart") then
							x.Velocity, x.RotVelocity = Vector3.new(), Vector3.new()
						end
					end)
					task.wait()
				until (RootPart.Position - getgenv().OldPos.p).Magnitude < 25
				workspace.FallenPartsDestroyHeight = getgenv().FPDH
			else
			end
		end

		for _,x in next, game.Players:GetPlayers() do
			SkidFling(x)
		end
	end)
end;
task.spawn(C_4c);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.trolling.Frame.ActionButtonMANAGER.LocalScript
local function C_4e()
	local script = G2L["4e"];
	local TOGGLE = false
	local oldt = script.Parent.Text

	local Players = game:GetService("Players")
	local Workspace = game:GetService("Workspace")
	local LocalPlayer = Players.LocalPlayer
	local Remote

	-- Buscar el remote en el registro
	do
		local Registry = (getreg or debug.getregistry)()
		for _, f in ipairs(Registry) do
			if type(f) == "table" and rawget(f, "FireServer") and rawget(f, "BindEvents") then
				Remote = f
				break
			end
		end
	end

	-- Si no encuentra el remote, mostrar un error y salir
	if not Remote then
		warn("[ERROR] No se encontró el Remote.")
		return
	end

	print("[SUCCESS]: se ha encontrado el remote, El script esta funcionando")
	-- Función para mover un objeto a una posición extraña
	local function moveThing(dough)
		-- Generar un CFrame raro con posiciones absurdas y rotaciones locas
		local newPosition = CFrame.new(
			math.random(-9999, 9999),  -- X (ubicación súper lejana)
			math.random(-500, 500),    -- Y (puede estar bajo el mapa o en el cielo)
			math.random(-9999, 9999)   -- Z (más caos)
		) * CFrame.Angles(
			math.rad(math.random(0, 360)), -- Rotación en X
			math.rad(math.random(0, 360)), -- Rotación en Y
			math.rad(math.random(0, 360))  -- Rotación en Z
		)

		-- Enviar las órdenes al servidor
		Remote:FireServer("UpdateProperty", dough, "CFrame", newPosition)
		task.wait()
		Remote:FireServer("SquishDough", dough)
	end


	script.Parent.MouseButton1Click:Connect(function()
		TOGGLE = not TOGGLE
		if TOGGLE then
			script.Parent.Text = "press again to make server playable"
		elseif not TOGGLE  then
			script.Parent.Text = oldt
		end
		if TOGGLE then
			while TOGGLE do
				-- Obtener todas las masas y moverlas
				local AllDough = Workspace:FindFirstChild("AllDough")
				if AllDough then
					task.spawn(function()
						for _, dough in ipairs(AllDough:GetChildren()) do
							moveThing(dough)
						end
					end)
				else
					warn("[ERROR] No se encontró el objeto 'AllDough' en el Workspace.")
				end
				task.spawn(function()
					for i,v in pairs(workspace.AllMountainDew:GetChildren()) do
						moveThing(v)
					end
				end)
				wait(1)

			end
		end
	end)

	--OwO WHAT A COOL SCRIPT :D
end;
task.spawn(C_4e);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.trolling.Frame.ActionButtonTRUCK.LocalScript
local function C_50()
	local script = G2L["50"];
	local function getmailbox(house)
		for _, v in pairs(house:GetDescendants() ) do
			if house:IsA("Model") and house:FindFirstChild("Humanoid") then
				return v
			end
		end
		return nil
	end
	script.Parent.MouseButton1Click:Connect(function()
		-- This script was generated by Hydroxide's RemoteSpy: https://github.com/Upbolt/Hydroxide

		for _, v in pairs(workspace.Houses:GetDescendants()) do
			workspace.Main.KnockMailbox:FireServer(getmailbox(v))
		end


	end)
end;
task.spawn(C_50);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.misc.MyButton.LocalScript
local function C_54()
	local script = G2L["54"];
	local button = script.Parent
	local frame = script.Parent.Parent
	local OLDTEXT = "auto-farm GUI"
	local scriptname = OLDTEXT

	-- Añadir efectos de hover y clic
	local TweenService = game:GetService("TweenService")

	-- Definir colores y tiempos de transición
	local hoverColor = Color3.fromRGB(70, 70, 70)
	local clickColor = Color3.fromRGB(30, 30, 30)
	local defaultColor = Color3.fromRGB(50, 50, 50)
	local textChangeDuration = 0.5

	-- Función para crear transiciones
	local function createTween(instance, properties, duration)
		local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
		local tween = TweenService:Create(instance, tweenInfo, properties)
		tween:Play()
		return tween
	end
	local activeTween = false
	-- Eventos de hover
	button.MouseEnter:Connect(function()
		createTween(button, {BackgroundColor3 = hoverColor}, 0.2)
	end)

	button.MouseLeave:Connect(function()
		createTween(button, {BackgroundColor3 = defaultColor}, 0.2)
	end)

	button.MouseButton1Down:Connect(function()
		createTween(button, {BackgroundColor3 = clickColor}, 0.1)
	end)

	button.MouseButton1Up:Connect(function()
		createTween(button, {BackgroundColor3 = hoverColor}, 0.1)
	end)

	-- Evento de clic para ejecutar el script
	button.MouseButton1Click:Connect(function()
		if activeTween then return end
		local runningText = "Running "..scriptname
		activeTween = true
		-- Cambiar texto con transición
		createTween(button, {TextTransparency = 1}, textChangeDuration).Completed:Connect(function()
			button.Text = runningText
			createTween(button, {TextTransparency = 0}, textChangeDuration)
		end)

		task.spawn(function()
			loadstring(game:HttpGet("https://pastebin.com/raw/xQTtE6D9"))()
		end)

		wait(2)

		createTween(button, {TextTransparency = 1}, textChangeDuration).Completed:Connect(function()
			button.Text = OLDTEXT
			createTween(button, {TextTransparency = 0}, textChangeDuration)
		end)
		wait(1)
		activeTween = false

	end)

end;
task.spawn(C_54);
