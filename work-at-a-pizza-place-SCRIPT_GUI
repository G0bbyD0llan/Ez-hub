-- StarterGui.ScreenGui.WindowFrame.ContentArea.configuration.confFrame.ConfigurationFRAME.AllowDragging.LocalScript
local function C_8b()
	local script = G2L["8b"];
	local function change(boolean)
		if boolean then
			script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.options.AllowDragging.Text = "yes"
			script.Parent.Text = "yes"
			script.Parent.BackgroundColor3 = Color3.new(0, 1, 0)
		elseif not boolean  then
			script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.options.AllowDragging.Text = "no"
			script.Parent.BackgroundColor3 = Color3.new(1, 0, 0)
			script.Parent.Text = "no"
		end
	end
	local Boolean = true
	script.Parent.MouseButton1Click:Connect(function()
		Boolean = not Boolean
		change(Boolean)
	end)
end;
task.spawn(C_8b);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.configuration.confFrame.ConfigurationFRAME.AllowC.LocalScript
local function C_8e()
	local script = G2L["8e"];
	local function change(boolean)
		if boolean then
			script.Parent.Parent.Parent.Parent.Parent.credits.Visible = true
			script.Parent.Text = "yes"
			script.Parent.BackgroundColor3 = Color3.new(0, 1, 0)
		elseif not boolean  then
			script.Parent.BackgroundColor3 = Color3.new(1, 0, 0)
			script.Parent.Text = "no"
			script.Parent.Parent.Parent.Parent.Parent.credits.Visible = false
		end
	end
	local Boolean = true
	script.Parent.MouseButton1Click:Connect(function()
		Boolean = not Boolean
		change(Boolean)
	end)
end;
task.spawn(C_8e);
