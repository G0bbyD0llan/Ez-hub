-- StarterGui.ScreenGui.WindowFrame.ContentArea.Frame.Message.LocalScript
local function C_a5()
	local script = G2L["a5"];
	repeat wait() until script.Parent.Parent.Parent.Parent.Transparency == 1

	local TextLabel = script.Parent -- Asegrate de colocar este script dentro del TextLabel
	local pastebinUrl = "PASTEBIN_RAW_URL" -- Reemplaza con tu enlace raw de Pastebin

	local version, lastMessage = "Unknown", "No Message"

	local success, result = pcall(function()
		return game:HttpGet(pastebinUrl)
	end)

	if success then
		local lines = result:split("\n")
		version = lines[1] or "Unknown"
		lastMessage = lines[2] or "No Message"
	end

	local function updateText()
		while true do
			local currentTime = os.date("%X")
			TextLabel.Text = string.format("Time = %s\nVersion = %s\nCreator Last Message: %s", currentTime, version, lastMessage)
			wait(1) -- Espera 1 segundo antes de la siguiente actualizacin
		end
	end

	local function FadeIn()
		wait(3.3)
		script.Parent.TextTransparency = 0
		wait(0.3)
		script.Parent.TextTransparency = 1
		wait(0.3)
		script.Parent.TextTransparency = 0
		wait(0.3)
		script.Parent.TextTransparency = 1
		wait(0.3)
		script.Parent.TextTransparency = 0
	end

	spawn(updateText)
	spawn(FadeIn)
end;
task.spawn(C_a5);
-- StarterGui.ScreenGui.WindowFrame.DraggableScript
local function C_a7()
	local script = G2L["a7"];
	--Variables
	local TitleBar = script.Parent["TitleBar"]
	local IsCursonOnTitleBar = false
	--MouseEnter and MouseLeave Connection
	TitleBar.MouseEnter:Connect(function()
		if IsCursonOnTitleBar == false then
			IsCursonOnTitleBar = true
		end
	end)
	TitleBar.MouseLeave:Connect(function()
		if IsCursonOnTitleBar == true then
			IsCursonOnTitleBar = false
		end
	end)
	--Making the draggable gui
	local UserInputService = game:GetService("UserInputService")

	local gui = script.Parent

	local dragging
	local dragInput
	local dragStart
	local startPos

	local function update(input)
		if IsCursonOnTitleBar then
			local delta = input.Position - dragStart
			gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end
	end

	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position

			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)

	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)

	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end;
task.spawn(C_a7);
