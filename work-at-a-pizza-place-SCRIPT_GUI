-- StarterGui.ScreenGui.WindowFrame.ContentArea.target.PlayerNameTextBox.LocalScript
local function C_19()
	local script = G2L["19"];
	-- Obtén el TextBox
	local textBox = script.Parent
	local set = function(name)
		if typeof(name) == "string" then
			textBox.Text = name
		end
	end
	-- Función para buscar al jugador
	local function buscarJugador(nombre)
		-- Crear una expresión regular para buscar coincidencias parciales
		local pattern = "^" .. nombre:lower() .. ".*$"

		-- Buscar coincidencias en los nombres de los jugadores
		for _, player in ipairs(game.Players:GetPlayers()) do
			if player.Name:lower():match(pattern) or player.DisplayName:lower():match(pattern) then
				print("player Found: " .. player.Name)
				set(player.Name)
			end
		end
	end

	-- Conectar la función al evento FocusLost del TextBox
	textBox.FocusLost:Connect(function(enterPressed)
		-- Verificar si se presionó la tecla Enter al perder el foco
		if enterPressed then
			-- Obtener el texto del TextBox
			local PlayerName = textBox.Text

			-- Verificar si el texto no está vacío
			if PlayerName ~= "" then
				-- Buscar al jugador usando el texto del TextBox
				buscarJugador(PlayerName:lower()) -- Convertir a minúsculas para hacer coincidencia de caso-insensitivo
			else
				warn("operation canceled because the Textbox has no text.")
			end
		end
	end)

end;
task.spawn(C_19);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.target.Frame.ActionButtonMANAGER.LocalScript
local function C_1c()
	local script = G2L["1c"];
	local function bring(PlayerToBring, position)
		local Players = game:GetService("Players")
		local player = Players.LocalPlayer
		local humanoid = player.Character.Humanoid
		local man = PlayerToBring

		if man then
			--check if he's respawning or sitting
			local htxt
			if man.Character==nil or man.Character:FindFirstChild("HumanoidRootPart")==nil or man.Character:FindFirstChild("Humanoid")==nil then
				htxt = "Failed because player is respawning"
			end
			local target = man.Character.HumanoidRootPart
			if man.Character.Humanoid.Sit then
				htxt = "Failed because player is sitting"
			end
			if htxt then
				local h = Instance.new("Message",workspace)
				h.Text = htxt
				wait(3)
				h:Destroy()
				return
			end
			--get in a car
			if not workspace.Cars:IsAncestorOf(humanoid.SeatPart) then
				humanoid.Sit=false
				wait(0.1)
				for _,car in ipairs(workspace.Cars:GetChildren()) do
					if car:FindFirstChild("Driver") and car.Driver.Occupant==nil and car:FindFirstChild("Owner") and car.Owner.Value==nil then
						car.Driver:Sit(humanoid)
						wait(0.3)
						if humanoid.SeatPart then
							break
						end
					end
				end
			end
			local seat = humanoid.SeatPart
			local car = seat.Parent
			local returncf = position
			for _,v in ipairs(car:GetChildren()) do if v.Name=="Wheel" then v.CanCollide=false end end
			seat.CanCollide = false
			for j=1,4 do
				--attempt to sit manager
				seat.Anchored=false
				local e = 0
				while car.HoodSeat.Occupant==nil and target.Parent and e<5 do
					local newpos = target.Position+Vector3.new(0,-3,0)+target.CFrame.lookVector*4.5+target.Velocity*Vector3.new(.5,0,.5)
					local flatdir = (target.CFrame.lookVector*Vector3.new(1,0,1)).Unit --target's looking direction, flattened
					if not (flatdir.x < 2) then --inf
						flatdir = Vector3.new(1,0,0)
					end
					car:SetPrimaryPartCFrame(CFrame.new(newpos,newpos-flatdir))
					seat.Velocity=Vector3.new()
					local e2=0
					while car.HoodSeat.Occupant==nil and target.Parent and e2<0.7 do
						e2=e2+wait()
					end
					e=e+e2
				end
				--attempt to move manager
				car:SetPrimaryPartCFrame(returncf)
				wait(.1)
				car:SetPrimaryPartCFrame(returncf)
				wait(.1)
				car:SetPrimaryPartCFrame(returncf)
				seat.Anchored=true
				wait(2)
				repeat
					wait()
				until PlayerToBring.Character.Humanoid.Sit == false
				wait(1)
				break
			end
			--reset car
			seat.Anchored=false
			wait()
			car:SetPrimaryPartCFrame(CFrame.new(120,10,-75))
			wait()
		end

		--become manager
		humanoid.Sit=false
		wait(0.1)
		player.Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame+Vector3.new(5,5,6)
	end
	local function manager(playerToManager)
		--by sirelKilla (v3rm)

		--vars
		local Players = game:GetService("Players")
		local player = Players.LocalPlayer
		local humanoid = player.Character.Humanoid
		local mteam = game:GetService("Teams").Manager
		local man = mteam:GetPlayers()[1]
		--deletes a glitchy chair in the office (optional)
		if workspace:FindFirstChild("ExtraChair") and workspace.ExtraChair:FindFirstChild("Seat") then
			workspace.ExtraChair.Seat:Destroy()
		end

		if man then
			--check if he's respawning or sitting
			local htxt
			if man.Character==nil or man.Character:FindFirstChild("HumanoidRootPart")==nil or man.Character:FindFirstChild("Humanoid")==nil then
				htxt = "Failed because manager is respawning"
			end
			local target = man.Character.HumanoidRootPart
			if man.Character.Humanoid.Sit then
				htxt = "Failed because manager is sitting"
			end
			if htxt then
				local h = Instance.new("Message",workspace)
				h.Text = htxt
				wait(3)
				h:Destroy()
				return
			end
			--get in a car
			if not workspace.Cars:IsAncestorOf(humanoid.SeatPart) then
				humanoid.Sit=false
				wait(0.1)
				for _,car in ipairs(workspace.Cars:GetChildren()) do
					if car:FindFirstChild("Driver") and car.Driver.Occupant==nil and car:FindFirstChild("Owner") and car.Owner.Value==nil then
						car.Driver:Sit(humanoid)
						wait(0.3)
						if humanoid.SeatPart then
							break
						end
					end
				end
			end
			local seat = humanoid.SeatPart
			local car = seat.Parent
			local returncf = CFrame.new(17,-3.5,21)*CFrame.Angles(0,math.pi/2,0)
			for _,v in ipairs(car:GetChildren()) do if v.Name=="Wheel" then v.CanCollide=false end end
			seat.CanCollide = false
			for j=1,4 do
				--attempt to sit manager
				seat.Anchored=false
				local e = 0
				while car.HoodSeat.Occupant==nil and mteam:GetPlayers()[1] and target.Parent and e<5 do
					local newpos = target.Position+Vector3.new(0,-3,0)+target.CFrame.lookVector*4.5+target.Velocity*Vector3.new(.5,0,.5)
					local flatdir = (target.CFrame.lookVector*Vector3.new(1,0,1)).Unit --target's looking direction, flattened
					if not (flatdir.x < 2) then --inf
						flatdir = Vector3.new(1,0,0)
					end
					car:SetPrimaryPartCFrame(CFrame.new(newpos,newpos-flatdir))
					seat.Velocity=Vector3.new()
					local e2=0
					while car.HoodSeat.Occupant==nil and mteam:GetPlayers()[1] and target.Parent and e2<0.7 do
						e2=e2+wait()
					end
					e=e+e2
				end
				--attempt to move manager
				car:SetPrimaryPartCFrame(returncf)
				wait(.1)
				car:SetPrimaryPartCFrame(returncf)
				seat.Anchored=true
				e = 0
				while mteam:GetPlayers()[1] and target.Parent and e<1 do
					e=e+wait()
				end
				car.HoodSeat:ClearAllChildren() --unsits anyone
				e = 0
				while mteam:GetPlayers()[1] and target.Parent and e<0.5 do
					e=e+wait()
				end
				if mteam:GetPlayers()[1]==nil or target.Parent==nil then
					break
				end
			end
			--reset car
			seat.Anchored=false
			wait()
			car:SetPrimaryPartCFrame(CFrame.new(120,10,-75))
			wait()
		end

		--become manager
		humanoid.Sit=false
		wait(1)
		bring(playerToManager, workspace.ManagerChair.Seat.CFrame)
	end
	local function getplayer()
		local textbox = script.Parent.Parent.Parent.PlayerNameTextBox
		local player = game.Players:FindFirstChild(textbox.Text)
		if player then
			return player
		end
	end

	script.Parent.MouseButton1Click:Connect(function()
		local PlayerToManager = getplayer()
		if PlayerToManager then
			local mteam = game:GetService("Teams").Manager
			local man = mteam:GetPlayers()[1]
			if typeof(man) == "Instance" and man:IsA("Player") then
				if man.Name ~= PlayerToManager.Name and man.Name ~= game.Players.LocalPlayer.Name then
					manager(PlayerToManager)
				end
			end
		end
	end)

	--local Players = game:GetService("Players")
	--local player = Players.LocalPlayer
	--local humanoid = player.Character.Humanoid	local seat = humanoid.SeatPart
	--local car = seat.Parent; 
	--car:SetPrimaryPartCFrame(game:GetService("Workspace")["Teleport to Party Island"].Part.CFrame+CFrame.new(0,-3.5,0)*CFrame.Angles(0,math.pi/2,0))
end;
task.spawn(C_1c);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.target.Frame.ActionButtonBRING.LocalScript
local function C_1e()
	local script = G2L["1e"];
	local function bring(PlayerToBring, position)
		local Players = game:GetService("Players")
		local player = Players.LocalPlayer
		local humanoid = player.Character.Humanoid
		local oldPos = player.Character.PrimaryPart.CFrame
		local man = PlayerToBring

		if man then
			--check if he's respawning or sitting
			local htxt
			if man.Character==nil or man.Character:FindFirstChild("HumanoidRootPart")==nil or man.Character:FindFirstChild("Humanoid")==nil then
				htxt = "Failed because player is respawning"
			end
			local target = man.Character.HumanoidRootPart
			if man.Character.Humanoid.Sit then
				htxt = "Failed because player is sitting"
			end
			if htxt then
				local h = Instance.new("Message",workspace)
				h.Text = htxt
				wait(3)
				h:Destroy()
				return
			end
			--get in a car
			if not workspace.Cars:IsAncestorOf(humanoid.SeatPart) then
				humanoid.Sit=false
				wait(0.1)
				for _,car in ipairs(workspace.Cars:GetChildren()) do
					if car:FindFirstChild("Driver") and car.Driver.Occupant==nil and car:FindFirstChild("Owner") and car.Owner.Value==nil then
						car.Driver:Sit(humanoid)
						wait(0.3)
						if humanoid.SeatPart then
							break
						end
					end
				end
			end
			local seat = humanoid.SeatPart
			local car = seat.Parent
			local returncf = position
			for _,v in ipairs(car:GetChildren()) do if v.Name=="Wheel" then v.CanCollide=false end end
			seat.CanCollide = false
			for j=1,4 do
				--attempt to sit manager
				seat.Anchored=false
				local e = 0
				while car.HoodSeat.Occupant==nil and target.Parent and e<5 do
					local newpos = target.Position+Vector3.new(0,-3,0)+target.CFrame.lookVector*4.5+target.Velocity*Vector3.new(.5,0,.5)
					local flatdir = (target.CFrame.lookVector*Vector3.new(1,0,1)).Unit --target's looking direction, flattened
					if not (flatdir.x < 2) then --inf
						flatdir = Vector3.new(1,0,0)
					end
					car:SetPrimaryPartCFrame(CFrame.new(newpos,newpos-flatdir))
					seat.Velocity=Vector3.new()
					local e2=0
					while car.HoodSeat.Occupant==nil and target.Parent and e2<0.7 do
						e2=e2+wait()
					end
					e=e+e2
				end
				--attempt to move manager
				car:SetPrimaryPartCFrame(returncf)
				wait(.1)
				seat.Anchored=true
				wait(0.5)
				repeat 
					wait()
				until PlayerToBring.Character.Humanoid.Sit == false
				break
			end
			wait(0.5)
			--reset car
			seat.Anchored=false
			wait()
			car:SetPrimaryPartCFrame(CFrame.new(120,10,-75))
			wait()
		end

		--become manager
		humanoid.Sit=false
		wait(0.1)
		player.Character.HumanoidRootPart.CFrame = oldPos
	end

	local function getplayer()
		local textbox = script.Parent.Parent.Parent.PlayerNameTextBox
		local player = game.Players:FindFirstChild(textbox.Text)
		if player then
			return player
		end
	end
	script.Parent.MouseButton1Click:Connect(function()
		local PLRTOBRING = getplayer()
		if PLRTOBRING then
			bring(PLRTOBRING, game.Players.LocalPlayer.Character.PrimaryPart.CFrame)
		end
	end)
end;
task.spawn(C_1e);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.target.Frame.ActionButtonFLING.LocalScript
local function C_20()
	local script = G2L["20"];
	local Players = game:GetService("Players")
	local Player = Players.LocalPlayer
	local SkidFling = function(TargetPlayer)
		local Character = Player.Character
		local Humanoid = Character and Character:FindFirstChildOfClass("Humanoid")
		local RootPart = Humanoid and Humanoid.RootPart

		local TCharacter = TargetPlayer.Character
		local THumanoid
		local TRootPart
		local THead
		local Accessory
		local Handle

		if TCharacter:FindFirstChildOfClass("Humanoid") then
			THumanoid = TCharacter:FindFirstChildOfClass("Humanoid")
		end
		if THumanoid and THumanoid.RootPart then
			TRootPart = THumanoid.RootPart
		end
		if TCharacter:FindFirstChild("Head") then
			THead = TCharacter.Head
		end
		if TCharacter:FindFirstChildOfClass("Accessory") then
			Accessory = TCharacter:FindFirstChildOfClass("Accessory")
		end
		if Accessoy and Accessory:FindFirstChild("Handle") then
			Handle = Accessory.Handle
		end

		if Character and Humanoid and RootPart then
			if RootPart.Velocity.Magnitude < 50 then
				getgenv().OldPos = RootPart.CFrame
			end
			if THumanoid and THumanoid.Sit and not AllBool then
			end
			if THead then
				workspace.CurrentCamera.CameraSubject = THead
			elseif not THead and Handle then
				workspace.CurrentCamera.CameraSubject = Handle
			elseif THumanoid and TRootPart then
				workspace.CurrentCamera.CameraSubject = THumanoid
			end
			if not TCharacter:FindFirstChildWhichIsA("BasePart") then
				return
			end

			local FPos = function(BasePart, Pos, Ang)
				RootPart.CFrame = CFrame.new(BasePart.Position) * Pos * Ang
				Character:SetPrimaryPartCFrame(CFrame.new(BasePart.Position) * Pos * Ang)
				RootPart.Velocity = Vector3.new(9e7, 9e7 * 10, 9e7)
				RootPart.RotVelocity = Vector3.new(9e8, 9e8, 9e8)
			end

			local SFBasePart = function(BasePart)
				local TimeToWait = 2
				local Time = tick()
				local Angle = 0

				repeat
					if RootPart and THumanoid then
						if BasePart.Velocity.Magnitude < 50 then
							Angle = Angle + 100

							FPos(BasePart, CFrame.new(0, 1.5, 0) + THumanoid.MoveDirection * BasePart.Velocity.Magnitude / 1.25, CFrame.Angles(math.rad(Angle),0 ,0))
							task.wait()

							FPos(BasePart, CFrame.new(0, -1.5, 0) + THumanoid.MoveDirection * BasePart.Velocity.Magnitude / 1.25, CFrame.Angles(math.rad(Angle), 0, 0))
							task.wait()

							FPos(BasePart, CFrame.new(2.25, 1.5, -2.25) + THumanoid.MoveDirection * BasePart.Velocity.Magnitude / 1.25, CFrame.Angles(math.rad(Angle), 0, 0))
							task.wait()

							FPos(BasePart, CFrame.new(-2.25, -1.5, 2.25) + THumanoid.MoveDirection * BasePart.Velocity.Magnitude / 1.25, CFrame.Angles(math.rad(Angle), 0, 0))
							task.wait()

							FPos(BasePart, CFrame.new(0, 1.5, 0) + THumanoid.MoveDirection,CFrame.Angles(math.rad(Angle), 0, 0))
							task.wait()

							FPos(BasePart, CFrame.new(0, -1.5, 0) + THumanoid.MoveDirection,CFrame.Angles(math.rad(Angle), 0, 0))
							task.wait()
						else
							FPos(BasePart, CFrame.new(0, 1.5, THumanoid.WalkSpeed), CFrame.Angles(math.rad(90), 0, 0))
							task.wait()

							FPos(BasePart, CFrame.new(0, -1.5, -THumanoid.WalkSpeed), CFrame.Angles(0, 0, 0))
							task.wait()

							FPos(BasePart, CFrame.new(0, 1.5, THumanoid.WalkSpeed), CFrame.Angles(math.rad(90), 0, 0))
							task.wait()

							FPos(BasePart, CFrame.new(0, 1.5, TRootPart.Velocity.Magnitude / 1.25), CFrame.Angles(math.rad(90), 0, 0))
							task.wait()

							FPos(BasePart, CFrame.new(0, -1.5, -TRootPart.Velocity.Magnitude / 1.25), CFrame.Angles(0, 0, 0))
							task.wait()

							FPos(BasePart, CFrame.new(0, 1.5, TRootPart.Velocity.Magnitude / 1.25), CFrame.Angles(math.rad(90), 0, 0))
							task.wait()

							FPos(BasePart, CFrame.new(0, -1.5, 0), CFrame.Angles(math.rad(90), 0, 0))
							task.wait()

							FPos(BasePart, CFrame.new(0, -1.5, 0), CFrame.Angles(0, 0, 0))
							task.wait()

							FPos(BasePart, CFrame.new(0, -1.5 ,0), CFrame.Angles(math.rad(-90), 0, 0))
							task.wait()

							FPos(BasePart, CFrame.new(0, -1.5, 0), CFrame.Angles(0, 0, 0))
							task.wait()
						end
					else
						break
					end
				until BasePart.Velocity.Magnitude > 500 or BasePart.Parent ~= TargetPlayer.Character or TargetPlayer.Parent ~= Players or not TargetPlayer.Character == TCharacter or THumanoid.Sit or Humanoid.Health <= 0 or tick() > Time + TimeToWait
			end

			workspace.FallenPartsDestroyHeight = 0/0

			local BV = Instance.new("BodyVelocity")
			BV.Name = "EpixVel"
			BV.Parent = RootPart
			BV.Velocity = Vector3.new(9e8, 9e8, 9e8)
			BV.MaxForce = Vector3.new(1/0, 1/0, 1/0)

			Humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, false)

			if TRootPart and THead then
				if (TRootPart.CFrame.p - THead.CFrame.p).Magnitude > 5 then
					SFBasePart(THead)
				else
					SFBasePart(TRootPart)
				end
			elseif TRootPart and not THead then
				SFBasePart(TRootPart)
			elseif not TRootPart and THead then
				SFBasePart(THead)
			elseif not TRootPart and not THead and Accessory and Handle then
				SFBasePart(Handle)
			else
			end

			BV:Destroy()
			Humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, true)
			workspace.CurrentCamera.CameraSubject = Humanoid

			repeat
				RootPart.CFrame = getgenv().OldPos * CFrame.new(0, .5, 0)
				Character:SetPrimaryPartCFrame(getgenv().OldPos * CFrame.new(0, .5, 0))
				Humanoid:ChangeState("GettingUp")
				table.foreach(Character:GetChildren(), function(_, x)
					if x:IsA("BasePart") then
						x.Velocity, x.RotVelocity = Vector3.new(), Vector3.new()
					end
				end)
				task.wait()
			until (RootPart.Position - getgenv().OldPos.p).Magnitude < 25
			workspace.FallenPartsDestroyHeight = getgenv().FPDH
		else
		end
	end
	local function getplayer()
		local textbox = script.Parent.Parent.Parent.PlayerNameTextBox
		local player = game.Players:FindFirstChild(textbox.Text)
		if player then
			return player
		end
	end
	script.Parent.MouseButton1Click:Connect(function()
		local PLRTOFLING = getplayer()
		if PLRTOFLING then
			SkidFling(PLRTOFLING)
		end
	end)
end;
task.spawn(C_20);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.target.Frame.ActionButtonTRASH.LocalScript
local function C_22()
	local script = G2L["22"];
	local function getplayer()
		local textbox = script.Parent.Parent.Parent.PlayerNameTextBox
		local player = game.Players:FindFirstChild(textbox.Text)
		if player then
			return player
		end
	end
	local remote
	do
		local reg = (getreg or debug.getregistry)()
		for i=1,#reg do
			local f = reg[i]
			if type(f)=="table" and rawget(f,"FireServer") and rawget(f,"BindEvents") then
				remote = f
			end
		end
	end
	function moveThing(bmd, location)
		remote:FireServer("UpdateProperty", bmd, "CFrame", location)
		wait()
		remote:FireServer("SquishDough", bmd)
	end
	local ruinherlife = false
	local playerToF0ckIt
	script.Parent.MouseButton1Click:Connect(function()
		local PlayerToManager = getplayer()
		if PlayerToManager then
			playerToF0ckIt = PlayerToManager
			ruinherlife = not ruinherlife
			if ruinherlife then
				script.Parent.Text ="un fill it with garbage"
				script.Parent.BackgroundColor3 = Color3.new(1, 0, 0)
			else
				script.Parent.Text = "fill it with garbage"
				script.Parent.BackgroundColor3 = Color3.new(0, 0.470588, 0.843137)
			end
			while ruinherlife do
				wait()
				local posicionActual = playerToF0ckIt.Character.HumanoidRootPart.Position
				if not posicionActual then return end
				spawn(function()
					for i,v in pairs(workspace.AllDough:GetChildren()) do
						local NEWPLRPOS = CFrame.new(
							posicionActual.X + math.random(-5, 5), -- Variación en el eje X (-10 a 10)
							posicionActual.Y, -- Mantenemos la misma altura
							posicionActual.Z + math.random(-5, 5) -- Variación en el eje Z (-10 a 10)
						)
						moveThing(v, NEWPLRPOS)
					end
				end)
				spawn(function()
					for i,v in pairs(workspace.AllMountainDew:GetChildren()) do
						local NEWPLRPOS = CFrame.new(
							posicionActual.X + math.random(-5, 5), -- Variación en el eje X (-10 a 10)
							posicionActual.Y, -- Mantenemos la misma altura
							posicionActual.Z + math.random(-5, 5) -- Variación en el eje Z (-10 a 10)
						)
						moveThing(v, NEWPLRPOS)
					end
				end)
				spawn(function()
					for i,v in pairs(workspace.BoxingRoom:GetChildren()) do
						local NEWPLRPOS = CFrame.new(
							posicionActual.X + math.random(-5, 5), -- Variación en el eje X (-10 a 10)
							posicionActual.Y, -- Mantenemos la misma altura
							posicionActual.Z + math.random(-5, 5) -- Variación en el eje Z (-10 a 10)
						)
						moveThing(v, NEWPLRPOS)
					end
				end)
				spawn(function()
					for i,v in pairs(workspace.AllBox:GetChildren()) do
						local NEWPLRPOS = CFrame.new(
							posicionActual.X + math.random(-5, 5), -- Variación en el eje X (-10 a 10)
							posicionActual.Y, -- Mantenemos la misma altura
							posicionActual.Z + math.random(-5, 5) -- Variación en el eje Z (-10 a 10)
						)
						moveThing(v, NEWPLRPOS)
					end
				end)
				spawn(function()
					for i,v in pairs(workspace.AllSupplyBoxes:GetChildren()) do
						local NEWPLRPOS = CFrame.new(
							posicionActual.X + math.random(-5, 5), -- Variación en el eje X (-10 a 10)
							posicionActual.Y, -- Mantenemos la misma altura
							posicionActual.Z + math.random(-5, 5) -- Variación en el eje Z (-10 a 10)
						)
						moveThing(v, NEWPLRPOS)
					end
				end)
			end
		end
	end)
end;
task.spawn(C_22);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.target.Frame.ActionButtonHOUSE.LocalScript
local function C_24()
	local script = G2L["24"];
	local button = script.Parent
	local originalText = button.Text -- Guarda el texto original
	local housesFolder = game:GetService("Workspace"):FindFirstChild("Houses")

	local function getplayer()
		local textbox = button.Parent.Parent.PlayerNameTextBox
		return game.Players:FindFirstChild(textbox.Text)
	end

	button.MouseButton1Click:Connect(function()
		local playerToHouse = getplayer()

		-- Verificar si la carpeta de casas existe
		if not housesFolder then
			button.Text = "Houses folder not found"
			task.wait(2)
			button.Text = originalText
			return
		end

		-- Verificar si se encontró el jugador
		if not playerToHouse then
			button.Text = "Incorrect player"
			task.wait(2)
			button.Text = originalText
			return
		end

		-- Buscar la casa del jugador
		local playerHouse
		for _, house in pairs(housesFolder:GetChildren()) do
			if house:FindFirstChild(playerToHouse.DisplayName) then
				playerHouse = house
				break
			end
		end

		if not playerHouse then
			button.Text = "House not found"
			task.wait(2)
			button.Text = originalText
			return
		end

		-- Mostrar dirección y esperar confirmación
		local address = playerHouse.Address.Value
		button.Text = address .. " (Press again to TP)"

		local confirmed = false
		local connection
		connection = button.MouseButton1Click:Connect(function()
			confirmed = true
			game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(playerHouse:FindFirstChild(playerToHouse.DisplayName).Head.CFrame)
			button.Text = originalText -- Restaurar el texto original
			connection:Disconnect() -- Desconectar la confirmación
		end)

		task.wait(3) -- Espera 3 segundos

		if not confirmed then
			button.Text = originalText -- Si no confirman, vuelve al texto original
			connection:Disconnect()
		end
	end)

end;
task.spawn(C_24);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.target.Frame.ActionButtonHOUSE.LocalScript
local function C_26()
	local script = G2L["26"];
	--teleport to a player
	local function getplayer()
		local textbox = script.Parent.Parent.Parent.PlayerNameTextBox
		local player = game.Players:FindFirstChild(textbox.Text)
		if player then
			return player
		end
	end
	local LP = game:GetService("Players").LocalPlayer
	script.Parent.MouseButton1Click:Connect(function()
		local player = getplayer()
		if player and player.Character then
			LP.Character:SetPrimaryPartCFrame(player.Character:GetPrimaryPartCFrame())
		end
	end)
end;
task.spawn(C_26);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.target.Frame.ActionButtonHOUSE.LocalScript
local function C_28()
	local script = G2L["28"];
	local function bring(PlayerToBring, position)
		local Players = game:GetService("Players")
		local player = Players.LocalPlayer
		local humanoid = player.Character.Humanoid
		local oldPos = player.Character.PrimaryPart.CFrame
		local man = PlayerToBring

		if man then
			--check if he's respawning or sitting
			local htxt
			if man.Character==nil or man.Character:FindFirstChild("HumanoidRootPart")==nil or man.Character:FindFirstChild("Humanoid")==nil then
				htxt = "Failed because player is respawning"
			end
			local target = man.Character.HumanoidRootPart
			if man.Character.Humanoid.Sit then
				htxt = "Failed because player is sitting"
			end
			if htxt then
				local h = Instance.new("Message",workspace)
				h.Text = htxt
				wait(3)
				h:Destroy()
				return
			end
			--get in a car
			if not workspace.Cars:IsAncestorOf(humanoid.SeatPart) then
				humanoid.Sit=false
				wait(0.1)
				for _,car in ipairs(workspace.Cars:GetChildren()) do
					if car:FindFirstChild("Driver") and car.Driver.Occupant==nil and car:FindFirstChild("Owner") and car.Owner.Value==nil then
						car.Driver:Sit(humanoid)
						wait(0.3)
						if humanoid.SeatPart then
							break
						end
					end
				end
			end
			local seat = humanoid.SeatPart
			local car = seat.Parent
			local returncf = position
			for _,v in ipairs(car:GetChildren()) do if v.Name=="Wheel" then v.CanCollide=false end end
			seat.CanCollide = false
			for j=1,4 do
				--attempt to sit manager
				seat.Anchored=false
				local e = 0
				while car.HoodSeat.Occupant==nil and target.Parent and e<5 do
					local newpos = target.Position+Vector3.new(0,-3,0)+target.CFrame.lookVector*4.5+target.Velocity*Vector3.new(.5,0,.5)
					local flatdir = (target.CFrame.lookVector*Vector3.new(1,0,1)).Unit --target's looking direction, flattened
					if not (flatdir.x < 2) then --inf
						flatdir = Vector3.new(1,0,0)
					end
					car:SetPrimaryPartCFrame(CFrame.new(newpos,newpos-flatdir))
					seat.Velocity=Vector3.new()
					local e2=0
					while car.HoodSeat.Occupant==nil and target.Parent and e2<0.7 do
						e2=e2+wait()
					end
					e=e+e2
				end
				--attempt to move manager
				car:SetPrimaryPartCFrame(returncf)
				wait(.1)
				seat.Anchored=true
				wait(0.5)
				repeat 
					wait()
				until PlayerToBring.Character.Humanoid.Sit == false
				break
			end
			wait(1)
			--reset car
			seat.Anchored=false
			wait()
			car:SetPrimaryPartCFrame(CFrame.new(120,10,-75))
			wait()
		end

		--become manager
		humanoid.Sit=false
		wait(0.1)
		player.Character.HumanoidRootPart.CFrame = oldPos
	end

	local function getplayer()
		local textbox = script.Parent.Parent.Parent.PlayerNameTextBox
		local player = game.Players:FindFirstChild(textbox.Text)
		if player then
			return player
		end
	end

	script.Parent.MouseButton1Click:Connect(function()
		--bring getplayer() to the void
		if getplayer() then
			bring(getplayer(), CFrame.new(111111110, 11111110, 11111110))
		end
	end)
end;
task.spawn(C_28);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.target.Frame.ActionButtonHOUSE.LocalScript
local function C_2a()
	local script = G2L["2a"];
	local function getplayer()
		local textbox = script.Parent.Parent.Parent.PlayerNameTextBox
		local player = game.Players:FindFirstChild(textbox.Text)
		if player then
			return player
		end
	end

	script.Parent.MouseButton1Click:Connect(function()
		if getplayer() then
			local playerToHouse = getplayer()
			for _, house in pairs(game:GetService("Workspace").Houses:GetChildren()) do
				if house:FindFirstChild(getplayer().DisplayName) then
					game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(house:FindFirstChild(getplayer().DisplayName).Head.CFrame)
				end
			end
		end
	end)
end;
task.spawn(C_2a);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.target.Frame.ActionButtonHOUSE.LocalScript
local function C_2c()
	local script = G2L["2c"];
	local teleportPlaces = {
		["Pizza Boxer"] = CFrame.new(59.600006, -2.600000, 12.499998),
		["Supplier"] = CFrame.new(6.900038, 6.600001, -1022.099548),
		["Cook"] = CFrame.new(37.000175, -2.599858, 53.699764),
		["Delivery"] = CFrame.new(65.131523, -2.600000, -15.769568),
		["Cashier"] = CFrame.new(48.896114, -2.599873, 83.585342),
	}

	local function getplayer()
		local textbox = script.Parent.Parent.Parent.PlayerNameTextBox
		local player = game.Players:FindFirstChild(textbox.Text)
		if player then
			return player
		end
	end

	local function bring(PlayerToBring, position)
		local Players = game:GetService("Players")
		local player = Players.LocalPlayer
		local humanoid = player.Character.Humanoid
		local oldPos = player.Character.PrimaryPart.CFrame
		local man = PlayerToBring

		if man then
			--check if he's respawning or sitting
			local htxt
			if man.Character==nil or man.Character:FindFirstChild("HumanoidRootPart")==nil or man.Character:FindFirstChild("Humanoid")==nil then
				htxt = "Failed because player is respawning"
			end
			local target = man.Character.HumanoidRootPart
			if man.Character.Humanoid.Sit then
				htxt = "Failed because player is sitting"
			end
			if htxt then
				local h = Instance.new("Message",workspace)
				h.Text = htxt
				wait(3)
				h:Destroy()
				return
			end
			--get in a car
			if not workspace.Cars:IsAncestorOf(humanoid.SeatPart) then
				humanoid.Sit=false
				wait(0.1)
				for _,car in ipairs(workspace.Cars:GetChildren()) do
					if car:FindFirstChild("Driver") and car.Driver.Occupant==nil and car:FindFirstChild("Owner") and car.Owner.Value==nil then
						car.Driver:Sit(humanoid)
						wait(0.3)
						if humanoid.SeatPart then
							break
						end
					end
				end
			end
			local seat = humanoid.SeatPart
			local car = seat.Parent
			local returncf = position
			for _,v in ipairs(car:GetChildren()) do if v.Name=="Wheel" then v.CanCollide=false end end
			seat.CanCollide = false
			for j=1,4 do
				--attempt to sit manager
				seat.Anchored=false
				local e = 0
				while car.HoodSeat.Occupant==nil and target.Parent and e<5 do
					local newpos = target.Position+Vector3.new(0,-3,0)+target.CFrame.lookVector*4.5+target.Velocity*Vector3.new(.5,0,.5)
					local flatdir = (target.CFrame.lookVector*Vector3.new(1,0,1)).Unit --target's looking direction, flattened
					if not (flatdir.x < 2) then --inf
						flatdir = Vector3.new(1,0,0)
					end
					car:SetPrimaryPartCFrame(CFrame.new(newpos,newpos-flatdir))
					seat.Velocity=Vector3.new()
					local e2=0
					while car.HoodSeat.Occupant==nil and target.Parent and e2<0.7 do
						e2=e2+wait()
					end
					e=e+e2
				end
				--attempt to move manager
				car:SetPrimaryPartCFrame(returncf)
				wait(.1)
				seat.Anchored=true
				wait(0.5)
				repeat 
					wait()
				until PlayerToBring.Character.Humanoid.Sit == false
				break
			end
			wait(0.5)
			--reset car
			seat.Anchored=false
			wait()
			car:SetPrimaryPartCFrame(CFrame.new(120,10,-75))
			wait()
		end

		--become manager
		humanoid.Sit=false
		wait(0.1)
		player.Character.HumanoidRootPart.CFrame = oldPos
	end



	local scrollingFrame = script.Parent:FindFirstChild("ScrollingFrame") -- ScrollingFrame dentro del botón
	local templateButton = scrollingFrame:FindFirstChild("Template_Button"):Clone() -- El template del botón

	local function createButton(name, position)
		-- Clonamos el Template_Button
		local newButton = templateButton:Clone()
		newButton.Name = name
		newButton.Text = name
		newButton.Parent = scrollingFrame

		-- Ajustamos el tamaño y la posición de los botones
		newButton.Position = UDim2.new(0, 0, 0, (newButton.Size.Y.Offset + 10) * #scrollingFrame:GetChildren()) -- Espaciado entre botones

		-- Definimos la acción cuando el jugador hace clic en el botón
		newButton.MouseButton1Click:Connect(function()
			bring(getplayer(), position)
		end)
	end


	-- Borramos los botones existentes antes de crear nuevos
	for _, child in pairs(scrollingFrame:GetChildren()) do
		if child:IsA("TextButton") and child.Name ~= "Template_Button" then
			child:Destroy()
		end
	end

	-- Crear los botones de teleportación en el ScrollingFrame
	for name, position in pairs(teleportPlaces) do
		createButton(name, position)
	end

	script.Parent.MouseButton1Click:Connect(function()
		for _, v in pairs(scrollingFrame:GetDescendants()) do
			if v:IsA("TextButton") and v.Text:lower() == "button" then v:Remove() end
		end
		-- Si el ScrollingFrame no está visible, lo mostramos
		scrollingFrame.Visible = not scrollingFrame.Visible
	end)

end;
task.spawn(C_2c);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.target.Frame.ActionButtonHOUSE.LocalScript
local function C_31()
	local script = G2L["31"];
	local button = script.Parent
	local originalText = button.Text
	local Players = game:GetService("Players")
	local LocalPlayer = Players.LocalPlayer
	local AnnoyActive = false
	local function annoy(PlayerToAnnoy: Player)
		if not PlayerToAnnoy then
			return
		end
		AnnoyActive = true
		task.spawn(function()
			while AnnoyActive do
				local player = LocalPlayer
				local humanoid = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
				if not humanoid or not humanoid.RootPart then
					task.wait(1)
					continue
				end
				local oldPos = player.Character.PrimaryPart.CFrame
				local man = PlayerToAnnoy

				if man and man.Character and man.Character:FindFirstChild("HumanoidRootPart") and man.Character:FindFirstChild("Humanoid") then
					if not man.Character.Humanoid.Sit then
						-- conseguir un coche
						if not (humanoid.SeatPart and workspace.Cars:IsAncestorOf(humanoid.SeatPart)) then
							humanoid.Sit = false
							task.wait(0.1)
							for _, car in ipairs(workspace.Cars:GetChildren()) do
								if car:FindFirstChild("Driver") and car.Driver.Occupant == nil and car:FindFirstChild("Owner") and car.Owner.Value == nil then
									car.Driver:Sit(humanoid)
									task.wait(0.3)
									if humanoid.SeatPart then break end
								end
							end
						end

						local seat = humanoid.SeatPart
						if not seat then
							task.wait(1)
							continue
						end

						local car = seat.Parent
						local target = man.Character.HumanoidRootPart

						-- Desactivar colisiones de ruedas
						for _, v in ipairs(car:GetChildren()) do
							if v.Name == "Wheel" then v.CanCollide = false end
						end
						seat.CanCollide = false

						-- Intentar atrapar al jugador
						seat.Anchored = false
						local e = 0
						while car.HoodSeat and car.HoodSeat.Occupant == nil and target.Parent and e < 5 do
							local newpos = target.Position + Vector3.new(0,-3,0) + target.CFrame.LookVector*4.5 + target.Velocity*Vector3.new(.5,0,.5)
							local flatdir = (target.CFrame.LookVector*Vector3.new(1,0,1)).Unit
							if not (flatdir.x < 2) then
								flatdir = Vector3.new(1,0,0)
							end
							car:SetPrimaryPartCFrame(CFrame.new(newpos, newpos - flatdir))
							seat.Velocity = Vector3.new()
							local e2 = 0
							while car.HoodSeat.Occupant == nil and target.Parent and e2 < 0.7 do
								e2 += task.wait()
							end
							e += e2
						end

						-- Llevar a posición aleatoria
						local randomPos = CFrame.new(
							math.random(-200,200),
							10,
							math.random(-200,200)
						)
						car:SetPrimaryPartCFrame(randomPos)
						task.wait(0.5)
						seat.Anchored = true

						-- Esperar hasta que el jugador se baje
						repeat
							task.wait()
						until not (man.Character and man.Character:FindFirstChild("Humanoid") and man.Character.Humanoid.Sit)

						-- Reset car
						task.wait(0.5)
						seat.Anchored = false
						car:SetPrimaryPartCFrame(CFrame.new(120,10,-75))

						-- Devolver al jugador local
						humanoid.Sit = false
						task.wait(0.1)
						if player.Character and player.Character.PrimaryPart then
							player.Character:SetPrimaryPartCFrame(oldPos)
						end

						-- Esperar 5 segundos antes de repetir
						task.wait(5)
					end
				end

				task.wait(1)
			end
		end)
	end

	-- Uso:
	-- annoy(Players:FindFirstChild("NombreDelJugador"))

	local function unannoy()
		AnnoyActive = false
	end



	local function getPlayer()
		local textbox = button.Parent.Parent:FindFirstChild("PlayerNameTextBox")
		if textbox then
			return game.Players:FindFirstChild(textbox.Text)
		end
		return nil
	end

	button.MouseButton1Click:Connect(function()
		if AnnoyActive then
			button.Text = originalText
			unannoy()
		else
			button.Text = "STOP"
			if getPlayer() then
				annoy(getPlayer())
			end
		end
	end)

end;
task.spawn(C_31);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.target.Frame.ActionButtonHOUSE.LocalScript
local function C_33()
	local script = G2L["33"];
	local function getplayer()
		local textbox = script.Parent.Parent.Parent.PlayerNameTextBox
		local player = game.Players:FindFirstChild(textbox.Text)
		if player then
			return player
		end
	end

	local function bring(PlayerToBring, position)
		local Players = game:GetService("Players")
		local player = Players.LocalPlayer
		local humanoid = player.Character.Humanoid
		local oldPos = player.Character.PrimaryPart.CFrame
		local man = PlayerToBring

		if man then
			--check if he's respawning or sitting
			local htxt
			if man.Character==nil or man.Character:FindFirstChild("HumanoidRootPart")==nil or man.Character:FindFirstChild("Humanoid")==nil then
				htxt = "Failed because player is respawning"
			end
			local target = man.Character.HumanoidRootPart
			if man.Character.Humanoid.Sit then
				htxt = "Failed because player is sitting"
			end
			if htxt then
				local h = Instance.new("Message",workspace)
				h.Text = htxt
				wait(3)
				h:Destroy()
				return
			end
			--get in a car
			if not workspace.Cars:IsAncestorOf(humanoid.SeatPart) then
				humanoid.Sit=false
				wait(0.1)
				for _,car in ipairs(workspace.Cars:GetChildren()) do
					if car:FindFirstChild("Driver") and car.Driver.Occupant==nil and car:FindFirstChild("Owner") and car.Owner.Value==nil then
						car.Driver:Sit(humanoid)
						wait(0.3)
						if humanoid.SeatPart then
							break
						end
					end
				end
			end
			local seat = humanoid.SeatPart
			local car = seat.Parent
			local returncf = position
			for _,v in ipairs(car:GetChildren()) do if v.Name=="Wheel" then v.CanCollide=false end end
			seat.CanCollide = false
			for j=1,4 do
				--attempt to sit manager
				seat.Anchored=false
				local e = 0
				while car.HoodSeat.Occupant==nil and target.Parent and e<5 do
					local newpos = target.Position+Vector3.new(0,-3,0)+target.CFrame.lookVector*4.5+target.Velocity*Vector3.new(.5,0,.5)
					local flatdir = (target.CFrame.lookVector*Vector3.new(1,0,1)).Unit --target's looking direction, flattened
					if not (flatdir.x < 2) then --inf
						flatdir = Vector3.new(1,0,0)
					end
					car:SetPrimaryPartCFrame(CFrame.new(newpos,newpos-flatdir))
					seat.Velocity=Vector3.new()
					local e2=0
					while car.HoodSeat.Occupant==nil and target.Parent and e2<0.7 do
						e2=e2+wait()
					end
					e=e+e2
				end
				--attempt to move manager
				car:SetPrimaryPartCFrame(returncf)
				wait(.1)
				seat.Anchored=true
				wait(0.5)
				repeat 
					wait()
				until PlayerToBring.Character.Humanoid.Sit == false
				break
			end
			wait(0.5)
			--reset car
			seat.Anchored=false
			wait()
			car:SetPrimaryPartCFrame(CFrame.new(120,10,-75))
			wait()
		end

		--become manager
		humanoid.Sit=false
		wait(0.1)
		player.Character.HumanoidRootPart.CFrame = oldPos
	end

	local teleportPlaces = {
		["Starting Zone"] = CFrame.new(49, 3, 198),
		["Cashier Area"] = CFrame.new(48, 4, 80),
		["Cook Area"] = CFrame.new(42, 4, 61),
		["Boxer Area"] = CFrame.new(55, 4, 31),
		["Supplier Area"] = CFrame.new(8, 13, -1020),
		["Manager Room"] = CFrame.new(23, 5, 6.5)
	}

	local scrollingFrame = script.Parent:FindFirstChild("ScrollingFrame") -- ScrollingFrame dentro del botón
	local templateButton = scrollingFrame:FindFirstChild("Template_Button"):Clone() -- El template del botón

	local function createButton(name, position)
		-- Clonamos el Template_Button
		local newButton = templateButton:Clone()
		newButton.Name = name
		newButton.Text = name
		newButton.Parent = scrollingFrame

		-- Ajustamos el tamaño y la posición de los botones
		newButton.Position = UDim2.new(0, 0, 0, (newButton.Size.Y.Offset + 10) * #scrollingFrame:GetChildren()) -- Espaciado entre botones

		-- Definimos la acción cuando el jugador hace clic en el botón
		newButton.MouseButton1Click:Connect(function()
			-- Teletransportar al jugador al lugar correspondiente
			bring(getplayer(), position)
		end)
	end


	-- Borramos los botones existentes antes de crear nuevos
	for _, child in pairs(scrollingFrame:GetChildren()) do
		if child:IsA("TextButton") and child.Name ~= "Template_Button" then
			child:Destroy()
		end
	end

	-- Crear los botones de teleportación en el ScrollingFrame
	for name, position in pairs(teleportPlaces) do
		createButton(name, position)
	end

	script.Parent.MouseButton1Click:Connect(function()
		for _, v in pairs(scrollingFrame:GetDescendants()) do
			if v:IsA("TextButton") and v.Text:lower() == "button" then v:Remove() end
		end
		-- Si el ScrollingFrame no está visible, lo mostramos
		scrollingFrame.Visible = not scrollingFrame.Visible
	end)
end;
task.spawn(C_33);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.target.Frame.ActionButtonHOUSE.LocalScript
local function C_38()
	local script = G2L["38"];

	-- Función recursiva para encontrar la parte "Floor"
	local function FindFloorRecursive(model: Instance): BasePart?
		for _, child in ipairs(model:GetChildren()) do
			if child:IsA("BasePart") and child.Name == "Floor" then
				return child
			elseif child:IsA("Model") then
				local found = FindFloorRecursive(child)
				if found then
					return found
				end
			end
		end
		return nil
	end

	local function GetHousePosition(house: Model)

		local biggestHouseModel = house:FindFirstChild("CurrentUpgrade").Value

		if not biggestHouseModel then
			warn("❌ No se encontró CurrentUpgrade")
			return nil
		end

		-- Buscar recursivamente la parte "Floor"
		local floor = FindFloorRecursive(biggestHouseModel)
		if not floor then
			warn("❌ No se encontró ninguna parte llamada 'Floor' en "..biggestHouseModel.Name)
			return nil
		end

		-- Devolver posición ajustada en Y como CFrame
		return CFrame.new(floor.Position + Vector3.new(0, 5, 0))
	end
	local player = game.Players.LocalPlayer

	local function BringAllCarsOneByOne(position: CFrame)
		local character = player.Character
		if not character or not character:FindFirstChild("Humanoid") or not character.PrimaryPart then return end
		local humanoid = character.Humanoid
		local oldPos = character.PrimaryPart.CFrame

		for _, car in ipairs(workspace.Cars:GetChildren()) do
			if car:IsA("Model") and car:FindFirstChild("Driver") then
				local seat = car.Driver

				-- Asegurarse de que el asiento esté libre
				if seat.Occupant == nil then
					-- Sentar al jugador
					humanoid.Sit = false
					task.wait(0.2)
					seat:Sit(humanoid)
					task.wait(0.5)

					-- Si está sentado, teletransportar el auto
					if humanoid.SeatPart == seat then
						if not car.PrimaryPart then
							-- Si no tiene PrimaryPart, elegir uno (ej: el asiento)
							car.PrimaryPart = seat
						end

						-- Teletransportar el auto entero
						car:SetPrimaryPartCFrame(position)
						task.wait(0.5)

						-- Bajar al jugador antes de pasar al siguiente auto
						humanoid.Sit = false
						task.wait(0.5)
					end
				end
			end
		end

		-- Al final, regresar al jugador a su posición original
		task.wait(0.5)
		if character.PrimaryPart then
			character:SetPrimaryPartCFrame(oldPos)
		end
	end



	local button = script.Parent
	local originalText = button.Text -- Guarda el texto original
	local housesFolder = game:GetService("Workspace"):FindFirstChild("Houses")

	local function getplayer()
		local textbox = button.Parent.Parent.PlayerNameTextBox
		return game.Players:FindFirstChild(textbox.Text)
	end



	script.Parent.MouseButton1Click:Connect(function()
		local playerToHouse = getplayer()

		-- Verificar si la carpeta de casas existe
		if not housesFolder then
			button.Text = "Houses folder not found"
			task.wait(2)
			button.Text = originalText
			return
		end

		-- Verificar si se encontró el jugador
		if not playerToHouse then
			button.Text = "Incorrect player"
			task.wait(2)
			button.Text = originalText
			return
		end

		-- Buscar la casa del jugador
		local playerHouse
		for _, house in pairs(housesFolder:GetChildren()) do
			if house:FindFirstChild(playerToHouse.DisplayName) then
				playerHouse = house
				break
			end
		end

		if not playerHouse then
			button.Text = "House not found"
			task.wait(2)
			button.Text = originalText
			return
		end

		BringAllCarsOneByOne(GetHousePosition(playerHouse))
	end)
end;
task.spawn(C_38);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.target.Frame.ActionButtonHOUSE.LocalScript
local function C_3a()
	local script = G2L["3a"];
	local player = game.Players.LocalPlayer
	local function DropCarsOnPlayerOneByOne(targetPlayer: Player)
		local localPlayer = game.Players.LocalPlayer
		if not localPlayer.Character then return end

		local character = localPlayer.Character
		local humanoid = character:FindFirstChild("Humanoid")
		local hrp = character:FindFirstChild("HumanoidRootPart")
		if not humanoid or not hrp then return end

		if not targetPlayer or not targetPlayer.Character then return end
		local targetChar = targetPlayer.Character
		local targetHRP = targetChar:FindFirstChild("HumanoidRootPart")
		if not targetHRP then return end

		-- Guardar posición original del player
		local oldPos = hrp.CFrame

		for _, car in ipairs(workspace.Cars:GetChildren()) do
			if car:IsA("Model") and car:FindFirstChild("Driver") then
				local seat = car.Driver

				-- Revisar si el asiento está libre
				if seat.Occupant == nil then
					-- Forzar al player a sentarse en el auto
					humanoid.Sit = false
					task.wait(0.2)
					seat:Sit(humanoid)
					task.wait(0.5)

					if humanoid.SeatPart == seat then
						-- Definir PrimaryPart si no tiene
						if not car.PrimaryPart then
							car.PrimaryPart = seat
						end

						-- 🔥 Recalcular la posición predicha en cada iteración
						local velocity = targetHRP.Velocity
						local look = targetHRP.CFrame.LookVector
						local speed = velocity.Magnitude

						local predictTime = 1
						local predictedPos = targetHRP.Position
							+ (look * speed * predictTime * 0.1)
							+ (velocity * predictTime * 0.9)

						local dropPos = CFrame.new(predictedPos + Vector3.new(0, 60, 0))

						-- Teletransportar el auto sobre el jugador
						car:SetPrimaryPartCFrame(dropPos)
						task.wait(0.5)

						-- Bajar al jugador del auto
						humanoid.Sit = false
						task.wait(1)
					end
				end
			end
		end

		-- Regresar al player a su posición original
		task.wait(0.5)
		if character.PrimaryPart then
			character:SetPrimaryPartCFrame(oldPos)
		end
	end
	local button = script.Parent
	local originalText = button.Text -- Guarda el texto original

	local function getplayer()
		local textbox = button.Parent.Parent.PlayerNameTextBox
		return game.Players:FindFirstChild(textbox.Text)
	end



	script.Parent.MouseButton1Click:Connect(function()
		local playerToHouse = getplayer()
		if getplayer() then
			DropCarsOnPlayerOneByOne(playerToHouse)
		else
			warn("Player not found")
		end
	end)
end;
task.spawn(C_3a);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.trolling.Frame.ActionButtonMANAGER.LocalScript
local function C_3e()
	local script = G2L["3e"];
	script.Parent.MouseButton1Click:Connect(function()
		--by sirelKilla (v3rm)

		--vars
		local Players = game:GetService("Players")
		local player = Players.LocalPlayer
		local humanoid = player.Character.Humanoid
		local character = player.Character
		local OldPos = character.PrimaryPart.CFrame
		local mteam = game:GetService("Teams").Manager
		local man = mteam:GetPlayers()[1]
		--deletes a glitchy chair in the office (optional)
		if workspace:FindFirstChild("ExtraChair") and workspace.ExtraChair:FindFirstChild("Seat") then
			workspace.ExtraChair.Seat:Destroy()
		end

		if man then
			--check if he's respawning or sitting
			local htxt
			if man.Character==nil or man.Character:FindFirstChild("HumanoidRootPart")==nil or man.Character:FindFirstChild("Humanoid")==nil then
				htxt = "Failed because manager is respawning"
			end
			local target = man.Character.HumanoidRootPart
			if man.Character.Humanoid.Sit then
				htxt = "Failed because manager is sitting"
			end
			if htxt then
				local h = Instance.new("Message",workspace)
				h.Text = htxt
				wait(3)
				h:Destroy()
				return
			end
			--get in a car
			if not workspace.Cars:IsAncestorOf(humanoid.SeatPart) then
				humanoid.Sit=false
				wait(0.1)
				for _,car in ipairs(workspace.Cars:GetChildren()) do
					if car:FindFirstChild("Driver") and car.Driver.Occupant==nil and car:FindFirstChild("Owner") and car.Owner.Value==nil then
						car.Driver:Sit(humanoid)
						wait(0.3)
						if humanoid.SeatPart then
							break
						end
					end
				end
			end
			local seat = humanoid.SeatPart
			local car = seat.Parent
			local returncf = CFrame.new(17,-3.5,21)*CFrame.Angles(0,math.pi/2,0)
			for _,v in ipairs(car:GetChildren()) do if v.Name=="Wheel" then v.CanCollide=false end end
			seat.CanCollide = false
			for j=1,4 do
				--attempt to sit manager
				seat.Anchored=false
				local e = 0
				while car.HoodSeat.Occupant==nil and mteam:GetPlayers()[1] and target.Parent and e<5 do
					local newpos = target.Position+Vector3.new(0,-3,0)+target.CFrame.lookVector*4.5+target.Velocity*Vector3.new(.5,0,.5)
					local flatdir = (target.CFrame.lookVector*Vector3.new(1,0,1)).Unit --target's looking direction, flattened
					if not (flatdir.x < 2) then --inf
						flatdir = Vector3.new(1,0,0)
					end
					car:SetPrimaryPartCFrame(CFrame.new(newpos,newpos-flatdir))
					seat.Velocity=Vector3.new()
					local e2=0
					while car.HoodSeat.Occupant==nil and mteam:GetPlayers()[1] and target.Parent and e2<0.7 do
						e2=e2+wait()
					end
					e=e+e2
				end
				--attempt to move manager
				car:SetPrimaryPartCFrame(returncf)
				wait(.1)
				car:SetPrimaryPartCFrame(returncf)
				seat.Anchored=true
				e = 0
				while mteam:GetPlayers()[1] and target.Parent and e<1 do
					e=e+wait()
				end
				car.HoodSeat:ClearAllChildren() --unsits anyone
				e = 0
				while mteam:GetPlayers()[1] and target.Parent and e<0.5 do
					e=e+wait()
				end
				if mteam:GetPlayers()[1]==nil or target.Parent==nil then
					break
				end
			end
			--reset car
			seat.Anchored=false
			wait()
			car:SetPrimaryPartCFrame(CFrame.new(120,10,-75))
			wait()
		end

		--become manager
		humanoid.Sit = false
		wait(0.1)
		repeat
			character:SetPrimaryPartCFrame(workspace.ManagerChair.Seat.CFrame)
			wait()
		until humanoid.sit == true
		wait(0.5)
		repeat 
			humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
			wait()
		until humanoid.sit == false
		wait(0.1)
		for i = 1, 5 do
			player.Character.PrimaryPart.CFrame = OldPos
			wait()
		end
	end)
end;
task.spawn(C_3e);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.trolling.Frame.ActionButtonLOV.LocalScript
local function C_40()
	local script = G2L["40"];
	local ActiveLoop = false
	script.Parent.MouseButton1Click:Connect(function()
		ActiveLoop = not ActiveLoop
		if ActiveLoop then
			script.Parent.BackgroundColor3 = Color3.new(1, 0, 0)
			script.Parent.Text = "unloop open ovens"
		else
			script.Parent.BackgroundColor3 = Color3.new(0, 0.470588, 0.843137)
			script.Parent.Text = "loop open ovens"
		end
		while ActiveLoop and wait() do
			for i,v in pairs(game:GetService("Workspace").Ovens:GetChildren()) do
				v.Door.ClickDetector.Detector:FireServer()
				wait(.15)
			end
		end
	end)
end;
task.spawn(C_40);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.trolling.Frame.ActionButtonBCR.LocalScript
local function C_42()
	local script = G2L["42"];
	local ActiveLoop = false
	script.Parent.MouseButton1Click:Connect(function()
		ActiveLoop = not ActiveLoop
		if ActiveLoop then
			script.Parent.BackgroundColor3 = Color3.new(1, 0, 0)
			script.Parent.Text = "end the disaster"
		else
			script.Parent.BackgroundColor3 = Color3.new(0, 0.470588, 0.843137)
			script.Parent.Text = "Break cook room"
		end 
		local lp = game:GetService("Players").LocalPlayer
		local remote
		local ffc = game.FindFirstChild

		do
			local reg = (getreg or debug.getregistry)()
			for i=1,#reg do
				local f = reg[i]
				if type(f)=="table" and rawget(f,"FireServer") and rawget(f,"BindEvents") then
					remote = f
				end
			end
		end

		local	function moveThing(bmd, location)
			remote:FireServer("UpdateProperty", bmd, "CFrame", location)
			wait()
			remote:FireServer("SquishDough", bmd)
		end

		-- cook room fucker main script

		for i,v in pairs(workspace.AllSupplyBoxes:GetChildren()) do
			v.CFrame = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame
		end
		wait()


		spawn(function()
			while ActiveLoop and wait() do
				spawn(function()
					for i,v in pairs(workspace.AllDough:GetChildren()) do
						moveThing(v, CFrame.new(Random.new():NextNumber(22.6,51.6),Random.new():NextNumber(3.6,14.6),Random.new():NextNumber(55.5,70.5)))
					end
				end)
				spawn(function()
					for i,v in pairs(workspace.AllMountainDew:GetChildren()) do
						moveThing(v, CFrame.new(Random.new():NextNumber(22.6,51.6),Random.new():NextNumber(3.6,14.6),Random.new():NextNumber(55.5,70.5)))
					end
				end)
				spawn(function()
					for i,v in pairs(workspace.BoxingRoom:GetChildren()) do
						moveThing(v, CFrame.new(Random.new():NextNumber(22.6,51.6),Random.new():NextNumber(3.6,14.6),Random.new():NextNumber(55.5,70.5)))
					end
				end)
				spawn(function()
					for i,v in pairs(workspace.AllBox:GetChildren()) do
						moveThing(v, CFrame.new(Random.new():NextNumber(22.6,51.6),Random.new():NextNumber(3.6,14.6),Random.new():NextNumber(55.5,70.5)))
					end
				end)
				spawn(function()
					for i,v in pairs(workspace.AllSupplyBoxes:GetChildren()) do
						moveThing(v, CFrame.new(Random.new():NextNumber(22.6,51.6),Random.new():NextNumber(3.6,14.6),Random.new():NextNumber(55.5,70.5)))
					end
				end)
			end
		end)
	end)
end;
task.spawn(C_42);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.trolling.Frame.ActionButtonCAR.LocalScript
local function C_44()
	local script = G2L["44"];
	script.Parent.MouseButton1Click:Connect(function()
		local LP = game:GetService("Players").LocalPlayer
		local character = LP.Character or LP.CharacterAdded:Wait()
		local oldPos = character.PrimaryPart.CFrame
		wait()
		local humanoid = character:FindFirstChildWhichIsA("Humanoid")
		if not workspace.Cars:IsAncestorOf(humanoid.SeatPart) then
			humanoid.Sit=false
			wait(0.1)
			for _,car in ipairs(workspace.Cars:GetChildren()) do
				if car:FindFirstChild("Driver") and car.Driver.Occupant==nil and car:FindFirstChild("Owner") and car.Owner.Value==nil then
					car.Driver:Sit(humanoid)
					wait(0.3)
					if humanoid.SeatPart then
						break
					end
				end
			end
		end
		local seat = humanoid.SeatPart
		local car = seat.Parent
		car:SetPrimaryPartCFrame(oldPos)
	end)
end;
task.spawn(C_44);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.trolling.Frame.ActionButtonGUO.LocalScript
local function C_46()
	local script = G2L["46"];
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local hrp = character:WaitForChild("HumanoidRootPart")

	local Trees = workspace:WaitForChild("Trees")
	local UprootRemote = workspace.Main:WaitForChild("UprootTree")

	-- Distancia máxima en studs
	local MAX_DISTANCE = 15

	-- Toggle variable
	local farming = false

	-- Loop
	task.spawn(function()
		while true do
			if farming then
				for _, tree in pairs(Trees:GetChildren()) do
					local part = tree:FindFirstChildOfClass("Part")
					if part then
						local distance = (part.Position - hrp.Position).Magnitude
						if distance <= MAX_DISTANCE then
							UprootRemote:FireServer(tree)
							task.wait(0.2) -- pequeño delay entre árboles
						end
					end
				end
			end
			task.wait(1) -- Espera general del loop
		end
	end)

	-- Botón toggle
	script.Parent.MouseButton1Click:Connect(function()
		farming = not farming
		if farming then
			script.Parent.Text = "Auto-Uproot Near Trees: ON"
		else
			script.Parent.Text = "Auto-Uproot Near Trees: OFF"
		end
	end)

end;
task.spawn(C_46);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.trolling.Frame.ActionButtonAVK.LocalScript
local function C_48()
	local script = G2L["48"];

	local Players = game:GetService("Players")
	local function bring(PlayerToBring, position)
		local Players = game:GetService("Players")
		local player = Players.LocalPlayer
		local humanoid = player.Character.Humanoid
		local oldPos = player.Character.PrimaryPart.CFrame
		local man = PlayerToBring

		if man then
			--check if he's respawning or sitting
			local htxt
			if man.Character==nil or man.Character:FindFirstChild("HumanoidRootPart")==nil or man.Character:FindFirstChild("Humanoid")==nil then
				htxt = "Failed because player is respawning"
			end
			local target = man.Character.HumanoidRootPart
			if man.Character.Humanoid.Sit then
				htxt = "Failed because player is sitting"
			end
			if htxt then
				local h = Instance.new("Message",workspace)
				h.Text = htxt
				wait(3)
				h:Destroy()
				return
			end
			--get in a car
			if not workspace.Cars:IsAncestorOf(humanoid.SeatPart) then
				humanoid.Sit=false
				wait(0.1)
				for _,car in ipairs(workspace.Cars:GetChildren()) do
					if car:FindFirstChild("Driver") and car.Driver.Occupant==nil and car:FindFirstChild("Owner") and car.Owner.Value==nil then
						car.Driver:Sit(humanoid)
						wait(0.3)
						if humanoid.SeatPart then
							break
						end
					end
				end
			end
			local seat = humanoid.SeatPart
			local car = seat.Parent
			local returncf = position
			for _,v in ipairs(car:GetChildren()) do if v.Name=="Wheel" then v.CanCollide=false end end
			seat.CanCollide = false
			for j=1,4 do
				--attempt to sit manager
				seat.Anchored=false
				local e = 0
				while car.HoodSeat.Occupant==nil and target.Parent and e<5 do
					local newpos = target.Position+Vector3.new(0,-3,0)+target.CFrame.lookVector*4.5+target.Velocity*Vector3.new(.5,0,.5)
					local flatdir = (target.CFrame.lookVector*Vector3.new(1,0,1)).Unit --target's looking direction, flattened
					if not (flatdir.x < 2) then --inf
						flatdir = Vector3.new(1,0,0)
					end
					car:SetPrimaryPartCFrame(CFrame.new(newpos,newpos-flatdir))
					seat.Velocity=Vector3.new()
					local e2=0
					while car.HoodSeat.Occupant==nil and target.Parent and e2<0.7 do
						e2=e2+wait()
					end
					e=e+e2
				end
				--attempt to move manager
				car:SetPrimaryPartCFrame(returncf)
				wait(.1)
				seat.Anchored=true
				wait(0.5)
				repeat 
					wait()
				until PlayerToBring.Character.Humanoid.Sit == false
				break
			end
			wait(1)
			--reset car
			seat.Anchored=false
			wait()
			car:SetPrimaryPartCFrame(CFrame.new(120,10,-75))
			wait()
		end

		--become manager
		humanoid.Sit=false
		wait(0.1)
		player.Character.HumanoidRootPart.CFrame = oldPos
	end


	function BringEveryOneByOne(position)
		for _,plr in ipairs(Players:GetPlayers()) do
			if plr ~= Players.LocalPlayer then
				bring(plr, position)
				wait(2) -- tiempo entre cada jugador
			end
		end
	end

	script.Parent.MouseButton1Click:Connect(function()
		BringEveryOneByOne(CFrame.new(111111110, 11111110, 11111110))
	end)
end;
task.spawn(C_48);
-- StarterGui.ScreenGui.WindowFrame.ContentArea.trolling.Frame.ActionButtonTRUCK.LocalScript
local function C_4a()
	local script = G2L["4a"];
	script.Parent.MouseButton1Click:Connect(function()
		local LP = game:GetService("Players").LocalPlayer
		local character = LP.Character or LP.CharacterAdded:Wait()
		local oldPos = character.PrimaryPart.CFrame
		wait()
		local humanoid = character:FindFirstChildWhichIsA("Humanoid")
		if not workspace.Trucks:IsAncestorOf(humanoid.SeatPart) then
			humanoid.Sit=false
			wait(0.1)
			for _,truck in ipairs(workspace.Trucks:GetChildren()) do
				if truck:FindFirstChild("Driver") and truck.Driver.Occupant == nil then
					truck.Driver:Sit(humanoid)
					wait(0.3)
					if humanoid.SeatPart then
						break
					end
				end
			end
		end
		local seat = humanoid.SeatPart
		local truck = seat.Parent
		truck:SetPrimaryPartCFrame(oldPos)
	end)
end;
