			loadstring(game:HttpGet("https://raw.githubusercontent.com/incognitototo/sda/main/dwa.lua", true))()
		end)

		wait(2)

		createTween(button, {TextTransparency = 1}, textChangeDuration).Completed:Connect(function()
			button.Text = OLDTEXT
			createTween(button, {TextTransparency = 0}, textChangeDuration)
		end)
		wait(1)
		activeTween = false

	end)

